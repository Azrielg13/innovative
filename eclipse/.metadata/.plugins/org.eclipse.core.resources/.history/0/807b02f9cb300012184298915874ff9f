package com.sce.esp.object.model;
import java.sql.SQLException;

import com.sce.esp.object.dao.TrendDAO;
import com.sce.esp.object.jpa.AutoInsertable;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@AutoInsertable
@Table(schema="MDI",name="MDIS53_TREND")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM Trend o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM Trend o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM Trend o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS53_TREND o WHERE o.ID=?"),//AUTO-GENERATED
})
public class Trend extends TrendDAO{
	public Trend(){
	}
	public Trend(int id){
		super(id);
	}
	public Trend(Trend orig){
		super(orig);
	}
	public String getLookUpName(){
		try {
			return this.getLookUp().getName();
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		return "null";
	}
}
