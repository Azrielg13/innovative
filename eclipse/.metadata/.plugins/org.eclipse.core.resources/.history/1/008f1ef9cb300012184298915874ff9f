package com.sce.esp.object.model;
import com.sce.esp.object.dao.ScopeTypeDescriptiveDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDIU60_SCOPE_TYPE_DESCRIPTIVE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM ScopeTypeDescriptive o WHERE o.SCOPE_TYPE_ID=?1 AND o.DESC_TYPE_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM ScopeTypeDescriptive o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM ScopeTypeDescriptive o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByScopeType", query="SELECT o FROM ScopeTypeDescriptive o WHERE o.SCOPE_TYPE_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIU60_SCOPE_TYPE_DESCRIPTIVE o WHERE o.SCOPE_TYPE_ID=? AND o.DESC_TYPE_ID=?"),//AUTO-GENERATED
})
public class ScopeTypeDescriptive extends ScopeTypeDescriptiveDAO{
	public ScopeTypeDescriptive(){
	}
	public ScopeTypeDescriptive(int scopeTypeId, int descTypeId){
		super(scopeTypeId, descTypeId);
	}
	public ScopeTypeDescriptive(ScopeTypeDescriptive orig){
		super(orig);
	}
}
