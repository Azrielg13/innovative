package com.sce.esp.object.model;
import com.sce.esp.object.dao.SysGrowthDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDI213_SYS_GROWTH")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM SysGrowth o WHERE o.PLANYEAR=?1 AND o.SIM_ID=?2 AND o.SYS_ID=?3 AND o.YEAR=?4 AND o.GROWTH_TYPE_ID=?5"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM SysGrowth o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM SysGrowth o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI213_SYS_GROWTH o WHERE o.PLANYEAR=? AND o.SIM_ID=? AND o.SYS_ID=? AND o.YEAR=? AND o.GROWTH_TYPE_ID=?"),//AUTO-GENERATED
})
public class SysGrowth extends SysGrowthDAO{
	public SysGrowth(){
	}
	public SysGrowth(int planyear, int simId, int sysId, int year, int growthTypeId){
		super(planyear, simId, sysId, year, growthTypeId);
	}
	public SysGrowth(SysGrowth orig){
		super(orig);
	}
}
