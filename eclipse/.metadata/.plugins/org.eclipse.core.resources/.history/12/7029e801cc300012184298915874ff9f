package com.sce.esp.object.dao;
/**Copy Right Frank todo */
/**Description of class, (we need to get this from somewhere, database? xml?)*/
import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.object.jpa.EntityManagerHelper;
import com.sce.esp.object.jpa.PrimaryKey;
import com.sce.esp.object.model.Department;
import com.sce.esp.object.model.MDIObject;
import com.sce.esp.object.model.PifImpactType;
import java.util.Calendar;
import java.util.Collection;
import java.util.Vector;
import javax.persistence.Cache;
import javax.persistence.Column;
import javax.persistence.EntityManager;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.TypedQuery;
public abstract class PifImpactTypeDAO extends MDIObject{
	public static enum KEY_PROPERTY{PLANYEAR,PIF_IMPACT_TYPE_ID};
	public static enum PROPERTY{PLANYEAR,PIF_IMPACT_TYPE_ID,IMPACT_TYPE_NAME,DESCRIPTION,ALT_DOUBLE1,ALT_VARCHAR1,ALT_DATE1,INSERT_TS,INSERT_USER_ID,MODIFIED_TS,MODIFIED_USER_ID,DELETED_TS,DELETED_USER_ID};
	public final static int PLANYEAR_LIMIT = 4;
	public final static int PIFIMPACTTYPEID_LIMIT = 9;
	public final static int IMPACTTYPENAME_LIMIT = 64;
	public final static int DESCRIPTION_LIMIT = 256;
	public final static int ALTDOUBLE1_LIMIT = 24;
	public final static int ALTVARCHAR1_LIMIT = 256;
	public final static int ALTDATE1_LIMIT = 7;
	private int planyear;
	private int pifImpactTypeId;
	private String impactTypeName;
	private String description;
	private double altDouble1;
	private String altVarchar1;
	private Calendar altDate1;
	private Department department;
	public static PifImpactType getInstance(int planyear, int pifImpactTypeId)throws java.sql.SQLException{
		return getInstance(planyear, pifImpactTypeId, true);
	}
	public static PifImpactType getInstance(int planyear, int pifImpactTypeId, boolean fetch)throws java.sql.SQLException{
		if(isNull(planyear, pifImpactTypeId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(planyear, pifImpactTypeId);
		Cache cache = em.getEntityManagerFactory().getCache();
		PifImpactType o = null;
		if(cache != null && cache.contains(PifImpactType.class, pk))
			o = em.find(PifImpactType.class, pk);
		if(o==null && getAICache().contains(PifImpactType.class, pk))
			o = getAICache().find(PifImpactType.class, pk);
		if(o==null && fetch)
			o = em.find(PifImpactType.class, pk);
		if(o==null && fetch && PifImpactType.class.getAnnotation(AutoInsertable.class) != null){
			o = new PifImpactType(planyear, pifImpactTypeId);
			getAICache().cache(PifImpactType.class, o);
		}
		return o;
	}
	public static Collection<PifImpactType> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<PifImpactType> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<PifImpactType> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM PifImpactType o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<PifImpactType> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<PifImpactType> tq = em.createQuery(jpql,PifImpactType.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<PifImpactType> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<PifImpactType> tq = em.createNamedQuery(name,PifImpactType.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public PifImpactTypeDAO(){}
	public PifImpactTypeDAO(int planyear, int pifImpactTypeId){
		this.planyear=planyear;
		this.pifImpactTypeId=pifImpactTypeId;
	}
	public PifImpactTypeDAO(PifImpactTypeDAO orig){
		super(orig);
		this.planyear=orig.getPlanyear();
		copyFrom(orig);
	}
	public void copyFrom(PifImpactTypeDAO orig){
		this.impactTypeName=orig.getImpactTypeName();
		this.description=orig.getDescription();
		this.altDouble1=orig.getAltDouble1();
		this.altVarchar1=orig.getAltVarchar1();
		this.altDate1=orig.getAltDate1();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{planyear, pifImpactTypeId};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	@Id
	@Column(name="PLANYEAR",nullable=false,length=4)
	public int getPlanyear(){
		return planyear;
	}
	public void setPlanyear(int planyear) throws java.sql.SQLException{
		if(isSame(planyear, getPlanyear()))return;
		int oldValue = getPlanyear();
		this.planyear=planyear;
		setProperty("PLANYEAR", planyear, oldValue);
		department=null;
	}
	@Id
	@SequenceGenerator(name="MDI677_SEQ",sequenceName="MDI677_SEQ")
	@Column(name="PIF_IMPACT_TYPE_ID",nullable=false,length=9)
	public int getPifImpactTypeId(){
		return pifImpactTypeId;
	}
	public void setPifImpactTypeId(int pifImpactTypeId) throws java.sql.SQLException{
		if(isSame(pifImpactTypeId, getPifImpactTypeId()))return;
		int oldValue = getPifImpactTypeId();
		this.pifImpactTypeId=pifImpactTypeId;
		setProperty("PIF_IMPACT_TYPE_ID", pifImpactTypeId, oldValue);
	}
	@Column(name="IMPACT_TYPE_NAME",nullable=false,length=64)
	public String getImpactTypeName(){
		return impactTypeName;
	}
	public void setImpactTypeName(String impactTypeName) throws java.sql.SQLException{
		if(isSame(impactTypeName, getImpactTypeName()))return;
		String oldValue = getImpactTypeName();
		this.impactTypeName=impactTypeName;
		setProperty("IMPACT_TYPE_NAME", impactTypeName, oldValue);
	}
	@Column(name="DESCRIPTION",nullable=true,length=256)
	public String getDescription(){
		return description;
	}
	public void setDescription(String description) throws java.sql.SQLException{
		if(isSame(description, getDescription()))return;
		String oldValue = getDescription();
		this.description=description;
		setProperty("DESCRIPTION", description, oldValue);
	}
	@Column(name="ALT_DOUBLE1",nullable=true,length=24)
	public double getAltDouble1(){
		return altDouble1;
	}
	public void setAltDouble1(double altDouble1) throws java.sql.SQLException{
		if(isSame(altDouble1, getAltDouble1()))return;
		double oldValue = getAltDouble1();
		this.altDouble1=altDouble1;
		setProperty("ALT_DOUBLE1", altDouble1, oldValue);
	}
	@Column(name="ALT_VARCHAR1",nullable=true,length=256)
	public String getAltVarchar1(){
		return altVarchar1;
	}
	public void setAltVarchar1(String altVarchar1) throws java.sql.SQLException{
		if(isSame(altVarchar1, getAltVarchar1()))return;
		String oldValue = getAltVarchar1();
		this.altVarchar1=altVarchar1;
		setProperty("ALT_VARCHAR1", altVarchar1, oldValue);
	}
	@Column(name="ALT_DATE1",nullable=true,length=7)
	public Calendar getAltDate1(){
		return altDate1;
	}
	public void setAltDate1(Calendar altDate1) throws java.sql.SQLException{
		if(isSame(altDate1, getAltDate1()))return;
		Calendar oldValue = getAltDate1();
		this.altDate1=altDate1;
		setProperty("ALT_DATE1", altDate1, oldValue);
	}
	public Department getDepartment()throws java.sql.SQLException{
		if(department==null)
			department=Department.getInstance(getPlanyear());
		return department;
	}
	public void setDepartment(Department department)throws java.sql.SQLException{
		setPlanyear(department==null?0:department.getPlanyear());
		this.department=department;
	}
	public PifImpactType copy()throws java.sql.SQLException{
		PifImpactType cp = new PifImpactType((PifImpactType)this);
		copyChildrenTo(cp);
		return cp;
	}
	public void copyChildrenTo(PifImpactTypeDAO cp)throws java.sql.SQLException{
		super.copyChildrenTo(cp);
	}
	public Vector<String> getDifference(PifImpactTypeDAO o){
		Vector<String> diffs = super.getDifference(o);
		if(!isSame(getPlanyear(),o.getPlanyear())) diffs.add("PLANYEAR");
		if(!isSame(getPifImpactTypeId(),o.getPifImpactTypeId())) diffs.add("PIF_IMPACT_TYPE_ID");
		if(!isSame(getImpactTypeName(),o.getImpactTypeName())) diffs.add("IMPACT_TYPE_NAME");
		if(!isSame(getDescription(),o.getDescription())) diffs.add("DESCRIPTION");
		if(!isSame(getAltDouble1(),o.getAltDouble1())) diffs.add("ALT_DOUBLE1");
		if(!isSame(getAltVarchar1(),o.getAltVarchar1())) diffs.add("ALT_VARCHAR1");
		if(!isSame(getAltDate1(),o.getAltDate1())) diffs.add("ALT_DATE1");
		return diffs;
	}
	public void insertChildren()throws java.sql.SQLException{
	}
}
