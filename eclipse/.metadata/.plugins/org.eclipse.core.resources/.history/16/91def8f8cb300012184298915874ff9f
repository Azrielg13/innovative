/**
 * 
 */
package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.ProjFileDAO;
@Entity
@Table(schema="MDI",name="MDIS45_PROJ_FILE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM ProjFile o WHERE o.FILE_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM ProjFile o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM ProjFile o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS45_PROJ_FILE o WHERE o.FILE_ID=?"),//AUTO-GENERATED
})
public class ProjFile extends ProjFileDAO implements Serializable{

	private static final long serialVersionUID = -3336864448394897747L;

	/**
	 * @param fileId
	 */
	public ProjFile() {
		super(0);
	}
	
	public ProjFile(int fileId) {
		super(fileId);
	}

	public ProjFile(ProjFile projFile) {
		super(projFile);
	}

	public boolean isEditable() {
		return true;
	}
}
