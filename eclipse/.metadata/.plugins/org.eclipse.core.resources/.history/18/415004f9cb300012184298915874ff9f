package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.QbrDAO;
@Entity
@Table(schema="MDI",name="MDIS97_QBR")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM Qbr o WHERE o.QBR_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM Qbr o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM Qbr o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS97_QBR o WHERE o.QBR_ID=?"),//AUTO-GENERATED
})
public class Qbr extends QbrDAO implements Serializable{

	private static final long serialVersionUID = 1L;

	public Qbr(){
	}
	public Qbr(int qbrId) {
		super(qbrId);
	}
	
	public Qbr(Qbr qbr) {
		super(qbr);
	}

	public String toString() {
		return getName();
	}
}
