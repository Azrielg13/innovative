package com.sce.esp.object.model;

import java.io.Serializable;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.SysMonthlyDAO;
import com.sce.esp.object.jpa.AutoInsertable;
@Entity
@AutoInsertable
@Table(schema="MDI",name="MDI065_SYS_MONTHLY")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM SysMonthly o WHERE o.PLANYEAR=?1 AND o.SIM_ID=?2 AND o.SYS_ID=?3 AND o.YEAR=?4 AND o.MONTH=?5"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM SysMonthly o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM SysMonthly o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI065_SYS_MONTHLY o WHERE o.PLANYEAR=? AND o.SIM_ID=? AND o.SYS_ID=? AND o.YEAR=? AND o.MONTH=?"),//AUTO-GENERATED
})
public class SysMonthly extends SysMonthlyDAO implements AbstractMonthlyPeak, Serializable{
	
	private static final long serialVersionUID = 7545544547492253516L;

	/**
	 * The Constructor.
	 * 
	 * @param sysId the sys id
	 * @param month the month
	 * @param year the year
	 * @param planyear the planyear
	 */
	public SysMonthly(){
	}
	public SysMonthly(int planyear, int simId, int sysId, int year, int month){
		super(planyear,simId,sysId,year,month);
	}
	
	public SysMonthly(SysMonthly sysMonthly) {
		super(sysMonthly);
	}

	/**
	 * 
	 * @param simId
	 * @return
	 * @throws SQLException
	 */
	public double getPeakValue() throws SQLException {		
		return getMonthlyPeak();
	}
}

