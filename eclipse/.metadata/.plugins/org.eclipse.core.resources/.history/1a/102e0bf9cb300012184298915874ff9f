package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PqTroubleLogFileDAO;
@Entity
@Table(schema="MDI",name="MDI990_PQ_TROUBLE_LOG_FILE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PqTroubleLogFile o WHERE o.FILE_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PqTroubleLogFile o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PqTroubleLogFile o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByPqTroubleLog", query="SELECT o FROM PqTroubleLogFile o WHERE o.PQ_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI990_PQ_TROUBLE_LOG_FILE o WHERE o.FILE_ID=?"),//AUTO-GENERATED
})
public class PqTroubleLogFile extends PqTroubleLogFileDAO implements Serializable{

	private static final long serialVersionUID = -5326382162000059677L;

	/**
	 * The Constructor.
	 *
	 * @param fileId the file id
	 */
	public PqTroubleLogFile(){
	}
	public PqTroubleLogFile(int fileId){
		super(fileId);
	}
	public PqTroubleLogFile(PqTroubleLogFile pqTroubleLogFile) {
		super(pqTroubleLogFile);
	}
	public int getPlanyear(){
    	return Department.getActivePlanYear();
    }
	
	public boolean isEditable(){
		return getPlanyear() == Department.getActivePlanYear();
	}
}
