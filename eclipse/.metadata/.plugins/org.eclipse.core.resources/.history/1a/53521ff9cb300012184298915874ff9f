package com.sce.esp.object.model;

import java.sql.SQLException;

import com.sce.esp.object.dao.StRelationDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDIT24_ST_RELATION")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM StRelation o WHERE o.ST_ID=?1 AND o.PRED_TASK_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM StRelation o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM StRelation o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findBySuccTask", query="SELECT o FROM StRelation o WHERE o.ST_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByPredTask", query="SELECT o FROM StRelation o WHERE o.PRED_TASK_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIT24_ST_RELATION o WHERE o.ST_ID=? AND o.PRED_TASK_ID=?"),//AUTO-GENERATED
})
public class StRelation extends StRelationDAO{
	public StRelation(){
	}
	public StRelation(int stId, int predTaskId){
		super(stId, predTaskId);
	}
	public StRelation(StRelation orig){
		super(orig);
	}
	public StRelation(int stId, int predTaskId, int relation) throws SQLException {
		super(stId,predTaskId);
		setRelationType(relation);
	}
}
