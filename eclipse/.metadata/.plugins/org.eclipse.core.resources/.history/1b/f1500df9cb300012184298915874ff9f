package com.sce.esp.object.model;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.ScopeTypeCprDAO;
@Entity
@Table(schema="MDI",name="MDISX8_SCOPE_TYPE_CPR")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM ScopeTypeCpr o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM ScopeTypeCpr o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM ScopeTypeCpr o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByScopeType", query="SELECT o FROM ScopeTypeCpr o WHERE o.SCOPE_TYPE_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDISX8_SCOPE_TYPE_CPR o WHERE o.ID=?"),//AUTO-GENERATED
})
public class ScopeTypeCpr extends ScopeTypeCprDAO{
	public ScopeTypeCpr(){
	}
	public ScopeTypeCpr(int id){
		super(id);
	}
	public ScopeTypeCpr(ScopeTypeCpr orig){
		super(orig);
	}
}
