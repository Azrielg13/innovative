package com.sce.esp.object.model;

import java.io.Serializable;

public class SysGrowthPersister implements Comparable<Object>, Serializable{
	private static final long serialVersionUID = 3775339227246879806L;
	private byte growthTypeId;
	private float growthHC;
	private float growthBC;
	private float growthLC;
	public SysGrowthPersister(byte growthTypeId){
		setGrowthTypeId(growthTypeId);
	}
	public int getGrowthTypeId(){
		return growthTypeId;
	}
	public void setGrowthTypeId(byte growthTypeId){
		this.growthTypeId = growthTypeId;
	}
	public float getGrowthHC(){
		return growthHC;
	}
	public void setGrowthHC(float growthHC){
		this.growthHC = growthHC;
	}
	public float getGrowthBC(){
		return growthBC;
	}
	public void setGrowthBC(float growthBC){
		this.growthBC = growthBC;
	}
	public float getGrowthLC(){
		return growthLC;
	}
	public void setGrowthLC(float growthLC){
		this.growthLC = growthLC;
	}
	public int compareTo(Object o) {
		if(o instanceof SysGrowthPersister){
			SysGrowthPersister sg = (SysGrowthPersister)o;
			if(getGrowthTypeId() < sg.getGrowthTypeId())
				return -1;
			else if(getGrowthTypeId() > sg.getGrowthTypeId())
				return 1;
		}
		return 0;
	}
}