package com.sce.esp.object.model;
import java.sql.SQLException;

import com.sce.esp.object.dao.DefaultPartDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDIU10_DEFAULT_PART")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM DefaultPart o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM DefaultPart o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM DefaultPart o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByParentScopeType", query="SELECT o FROM DefaultPart o WHERE o.PARENT_ST_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIU10_DEFAULT_PART o WHERE o.ID=?"),//AUTO-GENERATED
})
public class DefaultPart extends DefaultPartDAO{
	public DefaultPart(){
	}
	public DefaultPart(int id){
		super(id);
	}
	public DefaultPart(DefaultPart orig){
		super(orig);
	}
	public DefaultPart(ScopeType parent, ScopeType child,LookUp action) throws SQLException{
		this.setParentScopeType(parent);
		this.setScopeType(child);
		this.setAction(action);
	}
}
