package com.sce.esp.object.model;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.CostSpreadDAO;
@Entity
@Table(schema="MDI",name="MDIT18_COST_SPREAD")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM CostSpread o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM CostSpread o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM CostSpread o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findBySchedule", query="SELECT o FROM CostSpread o WHERE o.SCHEDULE_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIT18_COST_SPREAD o WHERE o.ID=?"),//AUTO-GENERATED
})
public class CostSpread extends CostSpreadDAO{
	public CostSpread(){
	}
	public CostSpread(int id){
		super(id);
	}
	public CostSpread(CostSpread orig){
		super(orig);
	}
}
