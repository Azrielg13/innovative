package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.ProgramYearDAO;
@Entity
@Table(schema="MDI",name="MDIS23_PROGRAM_YEAR")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM ProgramYear o WHERE o.PROGRAM_ID=?1 AND o.YEAR=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM ProgramYear o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM ProgramYear o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS23_PROGRAM_YEAR o WHERE o.PROGRAM_ID=? AND o.YEAR=?"),//AUTO-GENERATED
})
public class ProgramYear extends ProgramYearDAO implements Serializable{

	private static final long serialVersionUID = -5533851380067123376L;

	public ProgramYear(){
		super(0,0);
	}
	
	public ProgramYear(int programId, int year) {
		super(programId, year);
	}

	public ProgramYear(ProgramYear programYear) {
		super(programYear);
	}

}
