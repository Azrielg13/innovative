package com.sce.esp.object.model;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.CprSubAcctDAO;
@Entity
@Table(schema="MDI",name="MDISC6_CPR_SUB_ACCT")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM CprSubAcct o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM CprSubAcct o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM CprSubAcct o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByCprPrimeAcct", query="SELECT o FROM CprSubAcct o WHERE o.CPR_PRIME_ACCT_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByID_FETCH", query="SELECT o FROM CprSubAcct o WHERE o.ID=?1 OR o.ID IS NOT NULL")
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDISC6_CPR_SUB_ACCT o WHERE o.ID=?"),//AUTO-GENERATED
})
public class CprSubAcct extends CprSubAcctDAO{
	public CprSubAcct(){
	}
	public CprSubAcct(int id){
		super(id);
	}
	public CprSubAcct(CprSubAcct orig){
		super(orig);
	}
	@Override
	public int compareTo(Object o){
		int c2 = super.compareTo(o);
		if(c2==0 && o instanceof CprSubAcct){
			try {
				c2 = getCprPrimeAcct().compareTo(((CprSubAcct)o).getCprPrimeAcct());
			} catch (SQLException e) {
				e.printStackTrace();
			}
		}
		return c2;
	}
	@Override
	public String toString(){
		return getName();
	}
}
