package com.sce.esp.object.model;
import com.sce.esp.object.dao.GiCommentDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDIZ21_GI_COMMENT")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM GiComment o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM GiComment o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM GiComment o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIZ21_GI_COMMENT o WHERE o.ID=?"),//AUTO-GENERATED
})
public class GiComment extends GiCommentDAO{
	public GiComment(){
	}
	public GiComment(int id){
		super(id);
	}
	public GiComment(GiComment orig){
		super(orig);
	}
}
