package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.ElemDAO;
@Entity
@Table(schema="MDI",name="MDI535_ELEM")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM Elem o WHERE o.PLANYEAR=?1 AND o.ELEM_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM Elem o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM Elem o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByDepartment", query="SELECT o FROM Elem o WHERE o.PLANYEAR=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI535_ELEM o WHERE o.PLANYEAR=? AND o.ELEM_ID=?"),//AUTO-GENERATED
})
public class Elem extends ElemDAO implements Serializable{
	private static final long serialVersionUID = 8100668757119971520L;
	public Elem(){
	}
	public Elem(int planyear, int elemId){
		super(planyear,elemId);
	}
	public Elem(int planyear) {
		super(planyear,0);
	}
	public Elem(Elem elem) {
		super(elem);
	}
	public String toString() {
		return getTitle();
	}
}
