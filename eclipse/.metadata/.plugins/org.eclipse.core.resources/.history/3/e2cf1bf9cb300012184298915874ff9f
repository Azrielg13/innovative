package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PqProblemTypeDAO;
@Entity
@Table(schema="MDI",name="MDI960_PQ_PROBLEM_TYPE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PqProblemType o WHERE o.PROB_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PqProblemType o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PqProblemType o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI960_PQ_PROBLEM_TYPE o WHERE o.PROB_ID=?"),//AUTO-GENERATED
})
public class PqProblemType extends PqProblemTypeDAO implements Serializable{
	
	private static final long serialVersionUID = -3451920757379291002L;
	
	/**
	 * The Constructor.
	 */
	public PqProblemType() {
		super(0);
	}
	
	/**
	 * The Constructor.
	 * 
	 * @param id the id
	 */
	public PqProblemType(int probId){
		super(probId);
	}
	
	public PqProblemType(PqProblemType pqProblemType) {
		super(pqProblemType);
	}
    
	/**
	 * String representation for combo boxes.
	 * 
	 * @return the string
	 */
	public String toString(){
		return getProblemType();
	}
	public int getPlanyear(){
    	return Department.getActivePlanYear();
    }
}

