package com.sce.esp.object.model;
import com.sce.esp.object.dao.RelationDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDIS36_RELATION")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM Relation o WHERE o.RELATION_ID=?1 AND o.RELATED_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM Relation o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM Relation o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS36_RELATION o WHERE o.RELATION_ID=? AND o.RELATED_ID=?"),//AUTO-GENERATED
})
public class Relation extends RelationDAO{
	public Relation(){
	}
	public Relation(int relationId, int relatedId){
		super(relationId, relatedId);
	}
	public Relation(Relation orig){
		super(orig);
	}
}
