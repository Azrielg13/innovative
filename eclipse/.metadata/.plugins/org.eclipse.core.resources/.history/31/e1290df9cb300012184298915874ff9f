package com.sce.esp.object.model;
import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.ProjRelationDAO;
@Entity
@Table(schema="MDI",name="MDISP02_PROJ_RELATION")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM ProjRelation o WHERE o.PROJ_RELATED_ID=?1 AND o.REL_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM ProjRelation o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM ProjRelation o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDISP02_PROJ_RELATION o WHERE o.PROJ_RELATED_ID=? AND o.REL_ID=?"),//AUTO-GENERATED
})
public class ProjRelation extends ProjRelationDAO implements Serializable{
	private static final long serialVersionUID = 1L;
	public ProjRelation(){
	}
	public ProjRelation(int relId, int projRelatedId){
		super(relId, projRelatedId);
	}
	public ProjRelation(ProjRelation orig){
		super(orig);
	}
}
