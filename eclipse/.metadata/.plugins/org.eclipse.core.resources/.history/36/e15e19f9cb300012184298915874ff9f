package com.sce.esp.object.model;

import java.io.Serializable;

import com.sce.esp.object.dao.LgProjDriverDAO;

import javax.persistence.Entity;
import javax.persistence.Table;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
@Entity
@Table(schema="MDI",name="MDIT60_LG_PROJ_DRIVER")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM LgProjDriver o WHERE o.LG_PROJ_DRIVER_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM LgProjDriver o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM LgProjDriver o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIT60_LG_PROJ_DRIVER o WHERE o.LG_PROJ_DRIVER_ID=?"),//AUTO-GENERATED
})
public class LgProjDriver extends LgProjDriverDAO implements Serializable{

	private static final long serialVersionUID = 5171638625083329158L;

	public LgProjDriver(){
	}
	public LgProjDriver(LgProjDriver orig) {
		super(orig);
	}

	public LgProjDriver(int lgProjDriverId) {
		super(lgProjDriverId);
	}
}
