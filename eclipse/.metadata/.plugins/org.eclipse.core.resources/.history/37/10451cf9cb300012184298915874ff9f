 * 
 */
package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.ProgramSponsorDAO;
@Entity
@Table(schema="MDI",name="MDIS26_PROGRAM_SPONSOR")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM ProgramSponsor o WHERE o.PROGRAM_ID=?1 AND o.ORG_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM ProgramSponsor o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM ProgramSponsor o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS26_PROGRAM_SPONSOR o WHERE o.PROGRAM_ID=? AND o.ORG_ID=?"),//AUTO-GENERATED
})
public class ProgramSponsor extends ProgramSponsorDAO implements Serializable{

	private static final long serialVersionUID = 680244590937249426L;

	/**
	 * @param programId
	 * @param sponsorOrgId
	 */
	public ProgramSponsor(){
		super(0,0);
	}
	
	public ProgramSponsor(int programId, int sponsorOrgId) {
		super(programId, sponsorOrgId);
	}

	public ProgramSponsor(ProgramSponsor programSponsor) {
		super(programSponsor);
	}

}
