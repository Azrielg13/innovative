package com.sce.esp.object.dao;
/**Copy Right Frank todo */
/**Description of class, (we need to get this from somewhere, database? xml?)*/
import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.object.jpa.EntityManagerHelper;
import com.sce.esp.object.jpa.PrimaryKey;
import com.sce.esp.object.model.LookUp;
import com.sce.esp.object.model.TspocRefList;
import java.util.Collection;
import java.util.Vector;
import javax.persistence.Cache;
import javax.persistence.Column;
import javax.persistence.EntityManager;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.TypedQuery;
public abstract class TspocRefListDAO extends DataAccessObject{
	public static enum KEY_PROPERTY{ID};
	public static enum PROPERTY{ID,NAME,LIST_ID};
	public final static int ID_LIMIT = 9;
	public final static int NAME_LIMIT = 50;
	public final static int LISTID_LIMIT = 9;
	private int id;
	private String name;
	private int listId;
	private LookUp lookUp;
	public static TspocRefList getInstance(int id)throws java.sql.SQLException{
		return getInstance(id, true);
	}
	public static TspocRefList getInstance(int id, boolean fetch)throws java.sql.SQLException{
		if(isNull(id))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(id);
		Cache cache = em.getEntityManagerFactory().getCache();
		TspocRefList o = null;
		if(cache != null && cache.contains(TspocRefList.class, pk))
			o = em.find(TspocRefList.class, pk);
		if(o==null && getAICache().contains(TspocRefList.class, pk))
			o = getAICache().find(TspocRefList.class, pk);
		if(o==null && fetch)
			o = em.find(TspocRefList.class, pk);
		if(o==null && fetch && TspocRefList.class.getAnnotation(AutoInsertable.class) != null){
			o = new TspocRefList(id);
			getAICache().cache(TspocRefList.class, o);
		}
		return o;
	}
	public static Collection<TspocRefList> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<TspocRefList> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<TspocRefList> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM TspocRefList o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<TspocRefList> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<TspocRefList> tq = em.createQuery(jpql,TspocRefList.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<TspocRefList> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<TspocRefList> tq = em.createNamedQuery(name,TspocRefList.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public TspocRefListDAO(){}
	public TspocRefListDAO(int id){
		this.id=id;
	}
	public TspocRefListDAO(TspocRefListDAO orig){
		super(orig);
		copyFrom(orig);
	}
	public void copyFrom(TspocRefListDAO orig){
		this.name=orig.getName();
		this.listId=orig.getListId();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{id};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	@Id
	@SequenceGenerator(name="MDIS52_SEQ",sequenceName="MDIS52_SEQ")
	@Column(name="ID",nullable=false,length=9)
	public int getId(){
		return id;
	}
	public void setId(int id) throws java.sql.SQLException{
		if(isSame(id, getId()))return;
		int oldValue = getId();
		this.id=id;
		setProperty("ID", id, oldValue);
	}
	@Column(name="NAME",nullable=true,length=50)
	public String getName(){
		return name;
	}
	public void setName(String name) throws java.sql.SQLException{
		if(isSame(name, getName()))return;
		String oldValue = getName();
		this.name=name;
		setProperty("NAME", name, oldValue);
	}
	@Column(name="LIST_ID",nullable=true,length=9)
	public int getListId(){
		return listId;
	}
	public void setListId(int listId) throws java.sql.SQLException{
		if(isSame(listId, getListId()))return;
		int oldValue = getListId();
		this.listId=listId;
		setProperty("LIST_ID", listId, oldValue);
		lookUp=null;
	}
	public LookUp getLookUp()throws java.sql.SQLException{
		if(lookUp==null)
			lookUp=LookUp.getInstance(getListId());
		return lookUp;
	}
	public void setLookUp(LookUp lookUp)throws java.sql.SQLException{
		setListId(lookUp==null?0:lookUp.getId());
		this.lookUp=lookUp;
	}
	public TspocRefList copy()throws java.sql.SQLException{
		TspocRefList cp = new TspocRefList((TspocRefList)this);
		copyChildrenTo(cp);
		return cp;
	}
	public void copyChildrenTo(TspocRefListDAO cp)throws java.sql.SQLException{
		super.copyChildrenTo(cp);
	}
	public Vector<String> getDifference(TspocRefListDAO o){
		Vector<String> diffs = super.getDifference(o);
		if(!isSame(getId(),o.getId())) diffs.add("ID");
		if(!isSame(getName(),o.getName())) diffs.add("NAME");
		if(!isSame(getListId(),o.getListId())) diffs.add("LIST_ID");
		return diffs;
	}
	public void insertChildren()throws java.sql.SQLException{
	}
}
