package com.sce.esp.object.model;
import com.sce.esp.object.dao.IssueFileDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDI011_ISSUE_FILE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM IssueFile o WHERE o.FILE_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM IssueFile o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM IssueFile o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByIssue", query="SELECT o FROM IssueFile o WHERE o.ISSUE_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI011_ISSUE_FILE o WHERE o.FILE_ID=?"),//AUTO-GENERATED
})
public class IssueFile extends IssueFileDAO{
	public IssueFile(){
	}
	public IssueFile(int fileId){
		super(fileId);
	}
	public IssueFile(IssueFile orig){
		super(orig);
	}
	@Override
	public boolean isEditable() {
		// TODO Auto-generated method stub
		return false;
	}
}
