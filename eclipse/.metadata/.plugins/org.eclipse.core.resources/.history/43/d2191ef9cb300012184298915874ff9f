package com.sce.esp.object.model;
import com.sce.esp.object.dao.ScopeItemLaborDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDIU45_SCOPE_ITEM_LABOR")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM ScopeItemLabor o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM ScopeItemLabor o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM ScopeItemLabor o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByScopeItem", query="SELECT o FROM ScopeItemLabor o WHERE o.SCOPE_ITEM_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByCostType", query="SELECT o FROM ScopeItemLabor o WHERE o.COST_TYPE_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIU45_SCOPE_ITEM_LABOR o WHERE o.ID=?"),//AUTO-GENERATED
})
public class ScopeItemLabor extends ScopeItemLaborDAO{
	public ScopeItemLabor(){
	}
	public ScopeItemLabor(int id){
		super(id);
	}
	public ScopeItemLabor(ScopeItemLabor orig){
		super(orig);
	}
}
