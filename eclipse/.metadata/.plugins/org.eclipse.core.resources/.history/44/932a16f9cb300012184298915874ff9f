package com.sce.esp.object.model;

import java.io.Serializable;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.CktContShedDAO;
@Entity
@Table(schema="MDI",name="MDI280_CKT_CONT_SHED")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM CktContShed o WHERE o.PLANYEAR=?1 AND o.SHED_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM CktContShed o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM CktContShed o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByCktCont", query="SELECT o FROM CktContShed o WHERE o.PLANYEAR=?1 AND o.CKT_ID=?2 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI280_CKT_CONT_SHED o WHERE o.PLANYEAR=? AND o.SHED_ID=?"),//AUTO-GENERATED
})
public class CktContShed extends CktContShedDAO  implements Comparable<Object>, Serializable{
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1031471711804211504L;
	/**
	 * The Constructor.
	 * 
	 * @param planyear the planyear
	 */
	public CktContShed(){
	}
	public CktContShed(int planyear) {
		super(planyear,0);
	}
	
	/**
	 * The Constructor.
	 * 
	 * @param planyear the planyear
	 * @param shedId the shed id
	 */
	public CktContShed(int planyear, int shedId){
		super(planyear,shedId);
	}
	
	public CktContShed(CktContShed cktContShed) {
		super(cktContShed);
	}

	public Ckt getCkt() throws SQLException{
		return Ckt.getInstance(getPlanyear(), getCktId());
	}
	/* (non-Javadoc)
	 * @see java.lang.Comparable#compareTo(java.lang.Object)
	 */
	public int compareTo(Object o) {
		CktContShed usc = (CktContShed)o;
		if(getStep()<usc.getStep())
			return -1;
		if(getStep()>usc.getStep())
			return 1;
		return super.compareTo(o);
	}
}
