package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.SubContDAO;
@Entity
@Table(schema="MDI",name="MDI320_SUB_CONT")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM SubCont o WHERE o.PLANYEAR=?1 AND o.SUB_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM SubCont o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM SubCont o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findBySub", query="SELECT o FROM SubCont o WHERE o.PLANYEAR=?1 AND o.SUB_ID=?2 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI320_SUB_CONT o WHERE o.PLANYEAR=? AND o.SUB_ID=?"),//AUTO-GENERATED
})
public class SubCont extends SubContDAO implements Serializable{
	
	private static final long serialVersionUID = 4868260401635792969L;

	/**
	 * The Constructor.
	 * 
	 * @param subId the sub id
	 * @param planyear the planyear
	 */
	public SubCont(){
	}
	public SubCont(int planyear, int subId){
		super(planyear,subId);
	}

	public SubCont(SubCont subCont) {
		super(subCont);
	}
}
