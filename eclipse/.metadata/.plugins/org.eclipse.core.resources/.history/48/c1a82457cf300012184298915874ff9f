package com.sce.esp.object.model;

import java.sql.SQLException;
import java.util.Collection;

import com.sce.esp.object.ESPTestCase;


public class CursorTest extends ESPTestCase{
	public void testInsert() throws SQLException{
		for(int x=0; x<2000; x++){
			Bbank bbank = new Bbank();
			bbank.setSubName("Alder is great, This name is too long however "+x);
			bbank.insert();
		}
	}
	public void testSelect() throws SQLException{
		for(int x=0; x<2000; x++)
			Bbank.getCollection(new String[]{"Eddie"}, "Mack");
	}
	public void testUpdate() throws SQLException{
		Bbank bbank = Bbank.getInstance(2012, 800);
		for(int x=0; x<2000; x++)
			bbank.setSubName("Alder is great, This name is too long however "+x);
	}
}
