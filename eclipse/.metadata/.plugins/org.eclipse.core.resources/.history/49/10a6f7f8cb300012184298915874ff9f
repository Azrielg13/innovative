package com.sce.esp.object.model;

import java.io.Serializable;

import com.sce.esp.object.dao.SceProjAssumptionDAO;

import javax.persistence.Entity;
import javax.persistence.Table;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
@Entity
@Table(schema="MDI",name="MDIT80_SCE_PROJ_ASSUMPTION")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM SceProjAssumption o WHERE o.SCE_PROJ_ASSUMPTION_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM SceProjAssumption o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM SceProjAssumption o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findBySceProjRev", query="SELECT o FROM SceProjAssumption o WHERE o.SCE_PROJ_REV_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIT80_SCE_PROJ_ASSUMPTION o WHERE o.SCE_PROJ_ASSUMPTION_ID=?"),//AUTO-GENERATED
})
public class SceProjAssumption extends SceProjAssumptionDAO implements Serializable{

	private static final long serialVersionUID = -3755579385585730934L;

	public SceProjAssumption(){
	}
	public SceProjAssumption(SceProjAssumption orig) {
		super(orig);
	}

	public SceProjAssumption(int sceProjAssumptionId) {
		super(sceProjAssumptionId);
	}
}
