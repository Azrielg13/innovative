package com.sce.esp.object.model;

import com.sce.esp.object.dao.DataAccessObject;

public abstract class AbstractResource extends DataAccessObject {
	
	public AbstractResource(){}
	public AbstractResource(AbstractResource r){
		super(r);
	}
	public abstract int getId();
	public abstract void setId(int id) throws java.sql.SQLException;
	public abstract int getRamId();
	public abstract void setRamId(int id) throws java.sql.SQLException;
	public abstract LookUp getRam() throws java.sql.SQLException;
	public abstract void setRam(LookUp lu) throws java.sql.SQLException;
	public abstract LookUp getProjRole() throws java.sql.SQLException;
	public abstract void setProjRole(LookUp lu) throws java.sql.SQLException;
	public abstract int getProjRoleId();
	public abstract void setProjRoleId(int id) throws java.sql.SQLException;
	public abstract double getEstEffort();
	public abstract void setEstEffort(double effort) throws java.sql.SQLException;
	public abstract double getEstDuration();
	public abstract void setEstDuration(double duration) throws java.sql.SQLException;
	public abstract String getNote();
	public abstract void setNote(String note) throws java.sql.SQLException;
	public abstract String getData();
	public abstract void setData(String data) throws java.sql.SQLException;
	public abstract boolean isNotify();
	public abstract void setTask(AbstractTask at) throws java.sql.SQLException;

}
