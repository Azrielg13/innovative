package com.sce.esp.hst;

import com.sce.esp.object.model.Bank;

/**
 * Substation Heat Storm Tracker Object is used for displaying real-time
 * values for the Bank object since the information cannot
 * be stored in a <code>BankDaily</code> object.
 * 
 * @author Distribution Staff Engineering
 */
public class RealTimeBank  extends RealTimeObject {

	public RealTimeBank(Bank bank) {
		super(bank);
	}
	
	public Bank getBank() {		
		return (Bank)getAe();
	}
}