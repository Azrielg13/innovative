package com.sce.esp.object.model;

import java.io.Serializable;

import javax.swing.ImageIcon;

import com.sce.esp.object.dao.ChangeDAO;
import com.sce.esp.util.IconsFactory;

import javax.persistence.Entity;
import javax.persistence.Table;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
@Entity
@Table(schema="MDI",name="MDIT30_CHANGE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM Change o WHERE o.CHANGE_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM Change o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM Change o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIT30_CHANGE o WHERE o.CHANGE_ID=?"),//AUTO-GENERATED
})
public class Change extends ChangeDAO implements Serializable{


	/**
	 * 
	 */
	private static final long serialVersionUID = -822772959441290355L;

	public Change(){}
	public Change(int changeId) {
		super(changeId);
	}

	public Change(Change orig) {
		super(orig);
	}

	public ImageIcon getIcon(int changeId){
		switch(changeId){
			case 2: return IconsFactory.getScaledImageIcon("icons/growth_48.png",32);
			case 3: return IconsFactory.getScaledImageIcon("icons/autostart1.png",32);
			case 4:return  IconsFactory.getScaledImageIcon("icons/kwrite.png",32);
			case 5:return  IconsFactory.getScaledImageIcon("icons/xmag.png",32);
			case 6:return  IconsFactory.getScaledImageIcon("icons/folder_red.png",32);
//			case 5: return  IconsFactory.getScaledImageIcon("icons/_lpg.png",32);
			case 7: return  IconsFactory.getScaledImageIcon("icons/lists.png",32);

			default:return IconsFactory.getScaledImageIcon("icons/alien.png",32);
		}
	}

	@Override
	public String toString() {
		return this.getName();
	}
}
