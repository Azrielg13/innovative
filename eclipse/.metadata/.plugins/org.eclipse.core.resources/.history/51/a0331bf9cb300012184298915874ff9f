package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PifProjDriverDAO;
@Entity
@Table(schema="MDI",name="MDI671_PIF_PROJ_DRIVER")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PifProjDriver o WHERE o.PLANYEAR=?1 AND o.PIF_PROJ_DRIVER_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PifProjDriver o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PifProjDriver o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI671_PIF_PROJ_DRIVER o WHERE o.PLANYEAR=? AND o.PIF_PROJ_DRIVER_ID=?"),//AUTO-GENERATED
})
public class PifProjDriver extends PifProjDriverDAO implements Serializable{

	private static final long serialVersionUID = -7963622749800152389L;

	public PifProjDriver(){
	}
	public PifProjDriver(int planyear, int pifProjDriverId) {
		super(planyear, pifProjDriverId);
	}

	public PifProjDriver(PifProjDriver orig) {
		super(orig);
	}

	@Override
	public String toString() {
		return getName();
	}

}
