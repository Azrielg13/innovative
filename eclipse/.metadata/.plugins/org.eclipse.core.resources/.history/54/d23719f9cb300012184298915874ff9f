package com.sce.esp.object.model;
import com.sce.esp.object.dao.IssueDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDI008_ISSUE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM Issue o WHERE o.ISSUE_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM Issue o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM Issue o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI008_ISSUE o WHERE o.ISSUE_ID=?"),//AUTO-GENERATED
})
public class Issue extends IssueDAO{
	public Issue(){
	}
	public Issue(int id){
		super(id);
	}
	public Issue(Issue orig){
		super(orig);
	}
}
