package com.sce.esp.object.model;

import java.io.Serializable;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.ProjMajorMtlDAO;
@Entity
@Table(schema="MDI",name="MDIS60_PROJ_MAJOR_MTL")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM ProjMajorMtl o WHERE o.SCE_PROJ_REV_ID=?1 AND o.MAJOR_MTL_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM ProjMajorMtl o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM ProjMajorMtl o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findBySceProjRev", query="SELECT o FROM ProjMajorMtl o WHERE o.SCE_PROJ_REV_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS60_PROJ_MAJOR_MTL o WHERE o.SCE_PROJ_REV_ID=? AND o.MAJOR_MTL_ID=?"),//AUTO-GENERATED
})
public class ProjMajorMtl extends ProjMajorMtlDAO implements Serializable{
	
	private static final long serialVersionUID = -61600552405969272L;

	public ProjMajorMtl(){
	}
	public ProjMajorMtl(int projRevId, int majorMtlId) {
		super(projRevId, majorMtlId);
	}
	public ProjMajorMtl(ProjMajorMtl projMajorMtl) {
		super(projMajorMtl);
	}
	
	public void delete() throws SQLException{
	    deleteDB();
	}
}
