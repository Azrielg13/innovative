package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PifCustTypeDAO;
@Entity
@Table(schema="MDI",name="MDI672_PIF_CUST_TYPE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PifCustType o WHERE o.PLANYEAR=?1 AND o.PIF_CUST_TYPE_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PifCustType o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PifCustType o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI672_PIF_CUST_TYPE o WHERE o.PLANYEAR=? AND o.PIF_CUST_TYPE_ID=?"),//AUTO-GENERATED
})
public class PifCustType extends PifCustTypeDAO implements Serializable{

	private static final long serialVersionUID = 115891013680802083L;

	@Override
	public String toString() {
		return getName();
	}

	public PifCustType(){
	}
	public PifCustType(int planyear, int pifCustTypeId) {
		super(planyear, pifCustTypeId);
	}

	public PifCustType(PifCustType orig) {
		super(orig);
	}
}
