package com.sce.esp.object.model;
import com.sce.esp.object.dao.UnitCostDetailDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDIU30_UNIT_COST_DETAIL")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM UnitCostDetail o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM UnitCostDetail o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM UnitCostDetail o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByUnitCostBreakDown", query="SELECT o FROM UnitCostDetail o WHERE o.UCBD_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIU30_UNIT_COST_DETAIL o WHERE o.ID=?"),//AUTO-GENERATED
})
public class UnitCostDetail extends UnitCostDetailDAO{
	public UnitCostDetail(){
	}
	public UnitCostDetail(int id){
		super(id);
	}
	public UnitCostDetail(UnitCostDetail orig){
		super(orig);
	}
}
