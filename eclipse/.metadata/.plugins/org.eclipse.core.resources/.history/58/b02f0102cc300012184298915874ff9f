package com.sce.esp.object.dao;
/**Copy Right Frank todo */
/**Description of class, (we need to get this from somewhere, database? xml?)*/
import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.object.jpa.EntityManagerHelper;
import com.sce.esp.object.jpa.PrimaryKey;
import com.sce.esp.object.model.ProjResourceEval;
import com.sce.esp.object.model.ProjWorkGroup;
import com.sce.esp.object.model.SceProjRev;
import java.util.Calendar;
import java.util.Collection;
import java.util.TreeSet;
import java.util.Vector;
import javax.persistence.Cache;
import javax.persistence.Column;
import javax.persistence.EntityManager;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.TypedQuery;
public abstract class ProjResourceEvalDAO extends DataAccessObject{
	public static enum KEY_PROPERTY{RESOURCE_EVAL_ID};
	public static enum PROPERTY{RESOURCE_EVAL_ID,SCE_PROJ_REV_ID,NAME,DESCRIPTION,ALT_DOUBLE1,ALT_VARCHAR1,ALT_DATE1,INSERT_TS,INSERT_USER_ID,MODIFIED_TS,MODIFIED_USER_ID,DELETED_TS,DELETED_USER_ID};
	public final static int RESOURCEEVALID_LIMIT = 9;
	public final static int SCEPROJREVID_LIMIT = 9;
	public final static int NAME_LIMIT = 64;
	public final static int DESCRIPTION_LIMIT = 256;
	public final static int ALTDOUBLE1_LIMIT = 24;
	public final static int ALTVARCHAR1_LIMIT = 256;
	public final static int ALTDATE1_LIMIT = 7;
	private int resourceEvalId;
	private int sceProjRevId;
	private String name;
	private String description;
	private double altDouble1;
	private String altVarchar1;
	private Calendar altDate1;
	private Collection<ProjWorkGroup> projWorkGroups;
	private SceProjRev sceProjRev;
	public static ProjResourceEval getInstance(int resourceEvalId)throws java.sql.SQLException{
		return getInstance(resourceEvalId, true);
	}
	public static ProjResourceEval getInstance(int resourceEvalId, boolean fetch)throws java.sql.SQLException{
		if(isNull(resourceEvalId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(resourceEvalId);
		Cache cache = em.getEntityManagerFactory().getCache();
		ProjResourceEval o = null;
		if(cache != null && cache.contains(ProjResourceEval.class, pk))
			o = em.find(ProjResourceEval.class, pk);
		if(o==null && getAICache().contains(ProjResourceEval.class, pk))
			o = getAICache().find(ProjResourceEval.class, pk);
		if(o==null && fetch)
			o = em.find(ProjResourceEval.class, pk);
		if(o==null && fetch && ProjResourceEval.class.getAnnotation(AutoInsertable.class) != null){
			o = new ProjResourceEval(resourceEvalId);
			getAICache().cache(ProjResourceEval.class, o);
		}
		return o;
	}
	public static Collection<ProjResourceEval> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<ProjResourceEval> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<ProjResourceEval> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM ProjResourceEval o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<ProjResourceEval> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<ProjResourceEval> tq = em.createQuery(jpql,ProjResourceEval.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<ProjResourceEval> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<ProjResourceEval> tq = em.createNamedQuery(name,ProjResourceEval.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public ProjResourceEvalDAO(){}
	public ProjResourceEvalDAO(int resourceEvalId){
		this.resourceEvalId=resourceEvalId;
	}
	public ProjResourceEvalDAO(ProjResourceEvalDAO orig){
		super(orig);
		copyFrom(orig);
	}
	public void copyFrom(ProjResourceEvalDAO orig){
		this.sceProjRevId=orig.getSceProjRevId();
		this.name=orig.getName();
		this.description=orig.getDescription();
		this.altDouble1=orig.getAltDouble1();
		this.altVarchar1=orig.getAltVarchar1();
		this.altDate1=orig.getAltDate1();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{resourceEvalId};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	@Id
	@SequenceGenerator(name="MDIS50_SEQ",sequenceName="MDIS50_SEQ")
	@Column(name="RESOURCE_EVAL_ID",nullable=false,length=9)
	public int getResourceEvalId(){
		return resourceEvalId;
	}
	public void setResourceEvalId(int resourceEvalId) throws java.sql.SQLException{
		if(isSame(resourceEvalId, getResourceEvalId()))return;
		int oldValue = getResourceEvalId();
		this.resourceEvalId=resourceEvalId;
		setProperty("RESOURCE_EVAL_ID", resourceEvalId, oldValue);
	}
	@Column(name="SCE_PROJ_REV_ID",nullable=false,length=9)
	public int getSceProjRevId(){
		return sceProjRevId;
	}
	public void setSceProjRevId(int sceProjRevId) throws java.sql.SQLException{
		if(isSame(sceProjRevId, getSceProjRevId()))return;
		int oldValue = getSceProjRevId();
		this.sceProjRevId=sceProjRevId;
		setProperty("SCE_PROJ_REV_ID", sceProjRevId, oldValue);
		sceProjRev=null;
	}
	@Column(name="NAME",nullable=false,length=64)
	public String getName(){
		return name;
	}
	public void setName(String name) throws java.sql.SQLException{
		if(isSame(name, getName()))return;
		String oldValue = getName();
		this.name=name;
		setProperty("NAME", name, oldValue);
	}
	@Column(name="DESCRIPTION",nullable=true,length=256)
	public String getDescription(){
		return description;
	}
	public void setDescription(String description) throws java.sql.SQLException{
		if(isSame(description, getDescription()))return;
		String oldValue = getDescription();
		this.description=description;
		setProperty("DESCRIPTION", description, oldValue);
	}
	@Column(name="ALT_DOUBLE1",nullable=true,length=24)
	public double getAltDouble1(){
		return altDouble1;
	}
	public void setAltDouble1(double altDouble1) throws java.sql.SQLException{
		if(isSame(altDouble1, getAltDouble1()))return;
		double oldValue = getAltDouble1();
		this.altDouble1=altDouble1;
		setProperty("ALT_DOUBLE1", altDouble1, oldValue);
	}
	@Column(name="ALT_VARCHAR1",nullable=true,length=256)
	public String getAltVarchar1(){
		return altVarchar1;
	}
	public void setAltVarchar1(String altVarchar1) throws java.sql.SQLException{
		if(isSame(altVarchar1, getAltVarchar1()))return;
		String oldValue = getAltVarchar1();
		this.altVarchar1=altVarchar1;
		setProperty("ALT_VARCHAR1", altVarchar1, oldValue);
	}
	@Column(name="ALT_DATE1",nullable=true,length=7)
	public Calendar getAltDate1(){
		return altDate1;
	}
	public void setAltDate1(Calendar altDate1) throws java.sql.SQLException{
		if(isSame(altDate1, getAltDate1()))return;
		Calendar oldValue = getAltDate1();
		this.altDate1=altDate1;
		setProperty("ALT_DATE1", altDate1, oldValue);
	}
	public SceProjRev getSceProjRev()throws java.sql.SQLException{
		if(sceProjRev==null)
			sceProjRev=SceProjRev.getInstance(getSceProjRevId());
		return sceProjRev;
	}
	public void setSceProjRev(SceProjRev sceProjRev)throws java.sql.SQLException{
		setSceProjRevId(sceProjRev==null?0:sceProjRev.getSceProjRevId());
		this.sceProjRev=sceProjRev;
	}
	public Collection<ProjWorkGroup> getProjWorkGroups()throws java.sql.SQLException{
		if(isNewInstance() || projWorkGroups != null){
			if(projWorkGroups == null)
				projWorkGroups = new TreeSet<ProjWorkGroup>();
			return projWorkGroups;
		}
		return ProjWorkGroup.getNamedCollection("findByProjResourceEval",getResourceEvalId());
	}
	public void addProjWorkGroup(ProjWorkGroup projWorkGroup)throws java.sql.SQLException{
		projWorkGroup.setProjResourceEval((ProjResourceEval)this);
		if(isNewInstance() || projWorkGroups != null)
			getProjWorkGroups().add(projWorkGroup);
		else
			projWorkGroup.insert();
	}
	public void removeProjWorkGroup(ProjWorkGroup projWorkGroup)throws java.sql.SQLException{
		if(isNewInstance() || projWorkGroups != null)
			getProjWorkGroups().remove(projWorkGroup);
		else
			projWorkGroup.delete();
	}
	public ProjResourceEval copy()throws java.sql.SQLException{
		ProjResourceEval cp = new ProjResourceEval((ProjResourceEval)this);
		copyChildrenTo(cp);
		return cp;
	}
	public void copyChildrenTo(ProjResourceEvalDAO cp)throws java.sql.SQLException{
		super.copyChildrenTo(cp);
		for(ProjWorkGroup child:getProjWorkGroups())
			cp.addProjWorkGroup(child.copy());
	}
	public Vector<String> getDifference(ProjResourceEvalDAO o){
		Vector<String> diffs = super.getDifference(o);
		if(!isSame(getResourceEvalId(),o.getResourceEvalId())) diffs.add("RESOURCE_EVAL_ID");
		if(!isSame(getSceProjRevId(),o.getSceProjRevId())) diffs.add("SCE_PROJ_REV_ID");
		if(!isSame(getName(),o.getName())) diffs.add("NAME");
		if(!isSame(getDescription(),o.getDescription())) diffs.add("DESCRIPTION");
		if(!isSame(getAltDouble1(),o.getAltDouble1())) diffs.add("ALT_DOUBLE1");
		if(!isSame(getAltVarchar1(),o.getAltVarchar1())) diffs.add("ALT_VARCHAR1");
		if(!isSame(getAltDate1(),o.getAltDate1())) diffs.add("ALT_DATE1");
		return diffs;
	}
	public void insertChildren()throws java.sql.SQLException{
		if(projWorkGroups != null){
			for(ProjWorkGroup projWorkGroup:getProjWorkGroups()){
				if(projWorkGroup.isNewInstance())
					projWorkGroup.setProjResourceEval((ProjResourceEval)this);
				projWorkGroup.insert();
			}
			projWorkGroups = null;
		}
	}
}
