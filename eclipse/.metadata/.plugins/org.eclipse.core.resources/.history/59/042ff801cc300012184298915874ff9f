package com.sce.esp.object.dao;
/**Copy Right Frank todo */
/**Description of class, (we need to get this from somewhere, database? xml?)*/
import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.object.jpa.EntityManagerHelper;
import com.sce.esp.object.jpa.PrimaryKey;
import com.sce.esp.object.model.MDIObject;
import com.sce.esp.object.model.Proj;
import com.sce.esp.object.model.SimAlt;
import com.sce.esp.object.model.Simulation;
import java.util.Collection;
import java.util.Vector;
import javax.persistence.Cache;
import javax.persistence.Column;
import javax.persistence.EntityManager;
import javax.persistence.Id;
import javax.persistence.TypedQuery;
public abstract class SimAltDAO extends MDIObject{
	public static enum KEY_PROPERTY{PLANYEAR,SIM_ID,PROJ_ID,ALT_SIM_ID,ALT_PROJ_ID};
	public static enum PROPERTY{PLANYEAR,SIM_ID,PROJ_ID,ALT_SIM_ID,ALT_PROJ_ID,INSERT_TS,INSERT_USER_ID,MODIFIED_TS,MODIFIED_USER_ID,DELETED_TS,DELETED_USER_ID};
	public final static int PLANYEAR_LIMIT = 4;
	public final static int SIMID_LIMIT = 9;
	public final static int PROJID_LIMIT = 9;
	public final static int ALTSIMID_LIMIT = 9;
	public final static int ALTPROJID_LIMIT = 9;
	private SimAlt ptr;
	private int planyear;
	private int simId;
	private int projId;
	private int altSimId;
	private int altProjId;
	private Proj proj;
	private Proj proj2;
	public static SimAlt getInstance(int planyear, int simId, int projId, int altSimId, int altProjId)throws java.sql.SQLException{
		return getInstance(planyear, simId, projId, altSimId, altProjId, true);
	}
	public static SimAlt getInstance(int planyear, int simId, int projId, int altSimId, int altProjId, boolean fetch)throws java.sql.SQLException{
		if(isNull(planyear, projId, altSimId, altProjId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(planyear, simId, projId, altSimId, altProjId);
		Cache cache = em.getEntityManagerFactory().getCache();
		SimAlt o = null;
		if(cache != null && cache.contains(SimAlt.class, pk))
			o = em.find(SimAlt.class, pk);
		if(o==null && getAICache().contains(SimAlt.class, pk))
			o = getAICache().find(SimAlt.class, pk);
		if(o==null && fetch)
			o = em.find(SimAlt.class, pk);
		if(o==null && fetch && SimAlt.class.getAnnotation(AutoInsertable.class) != null){
			o = new SimAlt(planyear, simId, projId, altSimId, altProjId);
			getAICache().cache(SimAlt.class, o);
			o.setPtr();
		}
		return o;
	}
	public static Collection<SimAlt> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<SimAlt> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<SimAlt> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM SimAlt o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<SimAlt> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<SimAlt> tq = em.createQuery(jpql,SimAlt.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<SimAlt> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<SimAlt> tq = em.createNamedQuery(name,SimAlt.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public SimAltDAO(){}
	public SimAltDAO(int planyear, int simId, int projId, int altSimId, int altProjId){
		this.planyear=planyear;
		this.simId=simId;
		this.projId=projId;
		this.altSimId=altSimId;
		this.altProjId=altProjId;
	}
	public SimAltDAO(SimAltDAO orig){
		super(orig);
		this.planyear=orig.getPlanyear();
		this.simId=orig.getSimId();
		this.projId=orig.getProjId();
		this.altSimId=orig.getAltSimId();
		this.altProjId=orig.getAltProjId();
		copyFrom(orig);
	}
	public void copyFrom(SimAltDAO orig){
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{planyear, simId, projId, altSimId, altProjId};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	public SimAlt getPtr(){
		return ptr;
	}
	public void setPtr(SimAlt ptr){
		if(this.ptr!=null)this.ptr.removeWatcher(this);
		this.ptr=ptr;
		if(ptr!=null)ptr.addWatcher(this);
	}
	public void setPtr()throws java.sql.SQLException{
		if(getSimId()==0)return;
		int bs=0;
		Simulation sim = Simulation.getInstance(getPlanyear(),getSimId());
		if(sim != null)
			bs = sim.getBaseSimId();
		setPtr(getInstance(getPlanyear(), bs, getProjId(), getAltSimId(), getAltProjId()));
	}
	@Id
	@Column(name="PLANYEAR",nullable=false,length=4)
	public int getPlanyear(){
		return planyear;
	}
	public void setPlanyear(int planyear) throws java.sql.SQLException{
		if(isSame(planyear, getPlanyear()))return;
		int oldValue = getPlanyear();
		this.planyear=planyear;
		setProperty("PLANYEAR", planyear, oldValue);
	}
	@Id
	@Column(name="SIM_ID",nullable=false,length=9)
	public int getSimId(){
		return simId;
	}
	public void setSimId(int simId) throws java.sql.SQLException{
		if(isSame(simId, getSimId()))return;
		int oldValue = getSimId();
		this.simId=simId;
		setProperty("SIM_ID", simId, oldValue);
	}
	@Id
	@Column(name="PROJ_ID",nullable=false,length=9)
	public int getProjId(){
		return projId;
	}
	public void setProjId(int projId) throws java.sql.SQLException{
		if(isSame(projId, getProjId()))return;
		int oldValue = getProjId();
		this.projId=projId;
		setProperty("PROJ_ID", projId, oldValue);
		proj=null;
	}
	@Id
	@Column(name="ALT_SIM_ID",nullable=false,length=9)
	public int getAltSimId(){
		return altSimId;
	}
	public void setAltSimId(int altSimId) throws java.sql.SQLException{
		if(isSame(altSimId, getAltSimId()))return;
		int oldValue = getAltSimId();
		this.altSimId=altSimId;
		setProperty("ALT_SIM_ID", altSimId, oldValue);
	}
	@Id
	@Column(name="ALT_PROJ_ID",nullable=false,length=9)
	public int getAltProjId(){
		return altProjId;
	}
	public void setAltProjId(int altProjId) throws java.sql.SQLException{
		if(isSame(altProjId, getAltProjId()))return;
		int oldValue = getAltProjId();
		this.altProjId=altProjId;
		setProperty("ALT_PROJ_ID", altProjId, oldValue);
		proj2=null;
	}
	public Proj getProj()throws java.sql.SQLException{
		if(proj==null)
			proj=Proj.getInstance(getPlanyear(), getSimId(), getProjId());
		return proj;
	}
	public void setProj(Proj proj)throws java.sql.SQLException{
		setProjId(proj==null?0:proj.getProjId());
		this.proj=proj;
	}
	public Proj getProj2()throws java.sql.SQLException{
		if(proj2==null)
			proj2=Proj.getInstance(getPlanyear(), getAltSimId(), getAltProjId());
		return proj2;
	}
	public void setProj2(Proj proj2)throws java.sql.SQLException{
		setAltProjId(proj2==null?0:proj2.getProjId());
		this.proj2=proj2;
	}
	public SimAlt copy()throws java.sql.SQLException{
		SimAlt cp = new SimAlt((SimAlt)this);
		copyChildrenTo(cp);
		return cp;
	}
	public void copyChildrenTo(SimAltDAO cp)throws java.sql.SQLException{
		super.copyChildrenTo(cp);
	}
	public Vector<String> getDifference(SimAltDAO o){
		Vector<String> diffs = super.getDifference(o);
		if(!isSame(getPlanyear(),o.getPlanyear())) diffs.add("PLANYEAR");
		if(!isSame(getSimId(),o.getSimId())) diffs.add("SIM_ID");
		if(!isSame(getProjId(),o.getProjId())) diffs.add("PROJ_ID");
		if(!isSame(getAltSimId(),o.getAltSimId())) diffs.add("ALT_SIM_ID");
		if(!isSame(getAltProjId(),o.getAltProjId())) diffs.add("ALT_PROJ_ID");
		return diffs;
	}
	public void insertChildren()throws java.sql.SQLException{
	}
}
