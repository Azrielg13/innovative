package com.sce.esp.component;

public class ESPRapper<T> {
	private T obj;
	private String disText;
	public ESPRapper(T obj){
		this(obj,null);
	}
	public ESPRapper(T obj, String disText){
		this.obj = obj;
		this.disText = disText;
	}
	public String toString(){
		if(disText!=null)
			return disText;
		return ""+obj;
	}
	public T getObject(){
		return obj;
	}
}
