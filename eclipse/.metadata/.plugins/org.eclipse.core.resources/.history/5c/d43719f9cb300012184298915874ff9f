package com.sce.esp.object.model;

import java.util.Calendar;

public class LastModified {

	private Calendar modifiedTs = null;
	private User modifiedUser = null;
	
	public LastModified(User u, Calendar c){
		modifiedUser = u;
		modifiedTs = c;
	}
	
	public User getModifiedUser(){
		return modifiedUser;
	}
	
	public Calendar getModifiedTs(){
		return modifiedTs;
	}
}
