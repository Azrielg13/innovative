package com.sce.esp.object.model;
import java.io.Serializable;
import java.sql.SQLException;
import java.util.Vector;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.DepartmentYearDAO;
import com.sce.esp.object.jpa.AutoInsertable;
@Entity
@AutoInsertable
@Table(schema="MDI",name="MDI015_DEPARTMENT_YEAR")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM DepartmentYear o WHERE o.PLANYEAR=?1 AND o.YEAR=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM DepartmentYear o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM DepartmentYear o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI015_DEPARTMENT_YEAR o WHERE o.PLANYEAR=? AND o.YEAR=?"),//AUTO-GENERATED
})
public class DepartmentYear extends DepartmentYearDAO implements Serializable{
	
	/**
	 * 
	 */
	private static final long serialVersionUID = -4419770498657266029L;

	/**
	 * The Constructor.
	 * 
	 * @param planyear the planyear
	 * 
	 * @throws SQLException the SQL exception
	 */
	public DepartmentYear(){
	}
	public DepartmentYear(int planyear) throws SQLException {
		super(planyear,0);
	}
	
	/**
	 * The Constructor.
	 * 
	 * @param planyear the planyear
	 * @param year the year
	 * 
	 * @throws SQLException the SQL exception
	 */
	public DepartmentYear(int planyear, int year) throws SQLException{
		super(planyear,year);
	}
	
	public DepartmentYear(DepartmentYear departmentYear) {
		super(departmentYear);
	}

	/**
	 * To string.
	 * 
	 * @return the string
	 */
	public String toString() {
		return ""+getYear();
	}

	/**
	 * Gets the all.
	 * 
	 * @param planyear the planyear
	 * 
	 * @return the all
	 * 
	 * @throws SQLException the SQL exception
	 */
	public static Vector<DepartmentYear> getAll(int planyear) throws SQLException{ //BME ESP Optimize getAll
		Vector<DepartmentYear> years = new Vector<DepartmentYear>();
		for(int x=0; x<User.getActiveUser().getPlanFor(planyear, 0); x++)
			years.add(getInstance(planyear,planyear+x));
		return years;
	}

	public AgObject getAgObject() throws SQLException {
		return getDepartment();
	}

	public int getSimId() {
		return 0;
	}

	public double getAllCktFxKvarExist(int simId) {
		return 0;
	}

	public double getAllCktFxKvarProp(int simId) {
		return 0;
	}

	public double getAllCktFxKvarReq(int simdId) {
		return 0;
	}

	public double getAllCktSwtKvarExist(int simId) {
		return 0;
	}

	public double getAllCktSwtKvarProp(int simId) {
		return 0;
	}

	public double getAllCktSwtKvarReq(int simId) {
		return 0;
	}

}
