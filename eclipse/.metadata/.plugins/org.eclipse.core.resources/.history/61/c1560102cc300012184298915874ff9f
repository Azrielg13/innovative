package com.sce.esp.object.dao;
/**Copy Right Frank todo */
/**Description of class, (we need to get this from somewhere, database? xml?)*/
import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.object.jpa.EntityManagerHelper;
import com.sce.esp.object.jpa.PrimaryKey;
import com.sce.esp.object.model.Publish;
import java.util.Calendar;
import java.util.Collection;
import java.util.Vector;
import javax.persistence.Cache;
import javax.persistence.Column;
import javax.persistence.EntityManager;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.TypedQuery;
public abstract class PublishDAO extends DataAccessObject{
	public static enum KEY_PROPERTY{PUBLISH_ID};
	public static enum PROPERTY{PUBLISH_ID,PUBLISH_DATE,NAME,DESCRIPTION,ALT_DOUBLE1,ALT_VARCHAR1,ALT_DATE1,INSERT_TS,INSERT_USER_ID,MODIFIED_TS,MODIFIED_USER_ID,DELETED_TS,DELETED_USER_ID};
	public final static int PUBLISHID_LIMIT = 9;
	public final static int PUBLISHDATE_LIMIT = 7;
	public final static int NAME_LIMIT = 64;
	public final static int DESCRIPTION_LIMIT = 256;
	public final static int ALTDOUBLE1_LIMIT = 24;
	public final static int ALTVARCHAR1_LIMIT = 256;
	public final static int ALTDATE1_LIMIT = 7;
	private int publishId;
	private Calendar publishDate;
	private String name;
	private String description;
	private double altDouble1;
	private String altVarchar1;
	private Calendar altDate1;
	public static Publish getInstance(int publishId)throws java.sql.SQLException{
		return getInstance(publishId, true);
	}
	public static Publish getInstance(int publishId, boolean fetch)throws java.sql.SQLException{
		if(isNull(publishId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(publishId);
		Cache cache = em.getEntityManagerFactory().getCache();
		Publish o = null;
		if(cache != null && cache.contains(Publish.class, pk))
			o = em.find(Publish.class, pk);
		if(o==null && getAICache().contains(Publish.class, pk))
			o = getAICache().find(Publish.class, pk);
		if(o==null && fetch)
			o = em.find(Publish.class, pk);
		if(o==null && fetch && Publish.class.getAnnotation(AutoInsertable.class) != null){
			o = new Publish(publishId);
			getAICache().cache(Publish.class, o);
		}
		return o;
	}
	public static Collection<Publish> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<Publish> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<Publish> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM Publish o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<Publish> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<Publish> tq = em.createQuery(jpql,Publish.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<Publish> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<Publish> tq = em.createNamedQuery(name,Publish.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public PublishDAO(){}
	public PublishDAO(int publishId){
		this.publishId=publishId;
	}
	public PublishDAO(PublishDAO orig){
		super(orig);
		copyFrom(orig);
	}
	public void copyFrom(PublishDAO orig){
		this.publishDate=orig.getPublishDate();
		this.name=orig.getName();
		this.description=orig.getDescription();
		this.altDouble1=orig.getAltDouble1();
		this.altVarchar1=orig.getAltVarchar1();
		this.altDate1=orig.getAltDate1();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{publishId};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	@Id
	@SequenceGenerator(name="MDIS07_SEQ",sequenceName="MDIS07_SEQ")
	@Column(name="PUBLISH_ID",nullable=false,length=9)
	public int getPublishId(){
		return publishId;
	}
	public void setPublishId(int publishId) throws java.sql.SQLException{
		if(isSame(publishId, getPublishId()))return;
		int oldValue = getPublishId();
		this.publishId=publishId;
		setProperty("PUBLISH_ID", publishId, oldValue);
	}
	@Column(name="PUBLISH_DATE",nullable=false,length=7)
	public Calendar getPublishDate(){
		return publishDate;
	}
	public void setPublishDate(Calendar publishDate) throws java.sql.SQLException{
		if(isSame(publishDate, getPublishDate()))return;
		Calendar oldValue = getPublishDate();
		this.publishDate=publishDate;
		setProperty("PUBLISH_DATE", publishDate, oldValue);
	}
	@Column(name="NAME",nullable=true,length=64)
	public String getName(){
		return name;
	}
	public void setName(String name) throws java.sql.SQLException{
		if(isSame(name, getName()))return;
		String oldValue = getName();
		this.name=name;
		setProperty("NAME", name, oldValue);
	}
	@Column(name="DESCRIPTION",nullable=true,length=256)
	public String getDescription(){
		return description;
	}
	public void setDescription(String description) throws java.sql.SQLException{
		if(isSame(description, getDescription()))return;
		String oldValue = getDescription();
		this.description=description;
		setProperty("DESCRIPTION", description, oldValue);
	}
	@Column(name="ALT_DOUBLE1",nullable=true,length=24)
	public double getAltDouble1(){
		return altDouble1;
	}
	public void setAltDouble1(double altDouble1) throws java.sql.SQLException{
		if(isSame(altDouble1, getAltDouble1()))return;
		double oldValue = getAltDouble1();
		this.altDouble1=altDouble1;
		setProperty("ALT_DOUBLE1", altDouble1, oldValue);
	}
	@Column(name="ALT_VARCHAR1",nullable=true,length=256)
	public String getAltVarchar1(){
		return altVarchar1;
	}
	public void setAltVarchar1(String altVarchar1) throws java.sql.SQLException{
		if(isSame(altVarchar1, getAltVarchar1()))return;
		String oldValue = getAltVarchar1();
		this.altVarchar1=altVarchar1;
		setProperty("ALT_VARCHAR1", altVarchar1, oldValue);
	}
	@Column(name="ALT_DATE1",nullable=true,length=7)
	public Calendar getAltDate1(){
		return altDate1;
	}
	public void setAltDate1(Calendar altDate1) throws java.sql.SQLException{
		if(isSame(altDate1, getAltDate1()))return;
		Calendar oldValue = getAltDate1();
		this.altDate1=altDate1;
		setProperty("ALT_DATE1", altDate1, oldValue);
	}
	public Publish copy()throws java.sql.SQLException{
		Publish cp = new Publish((Publish)this);
		copyChildrenTo(cp);
		return cp;
	}
	public void copyChildrenTo(PublishDAO cp)throws java.sql.SQLException{
		super.copyChildrenTo(cp);
	}
	public Vector<String> getDifference(PublishDAO o){
		Vector<String> diffs = super.getDifference(o);
		if(!isSame(getPublishId(),o.getPublishId())) diffs.add("PUBLISH_ID");
		if(!isSame(getPublishDate(),o.getPublishDate())) diffs.add("PUBLISH_DATE");
		if(!isSame(getName(),o.getName())) diffs.add("NAME");
		if(!isSame(getDescription(),o.getDescription())) diffs.add("DESCRIPTION");
		if(!isSame(getAltDouble1(),o.getAltDouble1())) diffs.add("ALT_DOUBLE1");
		if(!isSame(getAltVarchar1(),o.getAltVarchar1())) diffs.add("ALT_VARCHAR1");
		if(!isSame(getAltDate1(),o.getAltDate1())) diffs.add("ALT_DATE1");
		return diffs;
	}
	public void insertChildren()throws java.sql.SQLException{
	}
}
