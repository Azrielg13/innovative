package com.sce.esp.object.model;

import java.io.Serializable;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.SimAltDAO;
@Entity
@Table(schema="MDI",name="MDI910_SIM_ALT")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM SimAlt o WHERE o.PLANYEAR=?1 AND o.SIM_ID=?2 AND o.PROJ_ID=?3 AND o.ALT_SIM_ID=?4 AND o.ALT_PROJ_ID=?5"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM SimAlt o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM SimAlt o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByProj", query="SELECT o FROM SimAlt o WHERE o.PLANYEAR=?1 AND o.SIM_ID=?2 AND o.PROJ_ID=?3 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI910_SIM_ALT o WHERE o.PLANYEAR=? AND o.SIM_ID=? AND o.PROJ_ID=? AND o.ALT_SIM_ID=? AND o.ALT_PROJ_ID=?"),//AUTO-GENERATED
})
public class SimAlt extends SimAltDAO implements Serializable{
	
	private static final long serialVersionUID = 4651841671984150097L;

	/**
	 * The Constructor.
	 * 
	 * @param projId the proj id
	 * @param altSimId the alt sim id
	 * @param simId the sim id
	 * @param planyear the planyear
	 * @param altProjId the alt proj id
	 */
	public SimAlt(){
	}
	public SimAlt(int planyear, int simId, int projId, int altSimId, int altProjId){
		super(planyear,simId,projId,altSimId,altProjId);
	}
	
	public SimAlt(SimAlt simAlt) {
		super(simAlt);
	}

	/**
	 * Gets the alt proj.
	 * 
	 * @return the alt proj
	 * 
	 * @throws SQLException the SQL exception
	 */
	public Proj getAltProj() throws SQLException{
		return Proj.getInstance(getPlanyear(), getAltSimId(), getAltProjId());
	}
}

