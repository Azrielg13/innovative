package com.sce.esp.object.model;

import java.io.Serializable;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
import javax.swing.ImageIcon;

import com.sce.esp.object.dao.FileCatDAO;
@Entity
@Table(schema="MDI",name="MDI003_FILE_CAT")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM FileCat o WHERE o.FILE_CAT_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM FileCat o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM FileCat o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI003_FILE_CAT o WHERE o.FILE_CAT_ID=?"),//AUTO-GENERATED
})
public class FileCat extends FileCatDAO implements Serializable{

	private static final long serialVersionUID = 6464653602221402443L;

	public static final int SIGNATURE = 1;
	
	public ImageIcon icon;
	
	public FileCat(){
	}
	public FileCat(int fileTypeId) {
		super(fileTypeId);
	}

	public FileCat(FileCat fileCat) {
		super(fileCat);
	}

	public String toString(){
		return getName();
	}

	public String getFileName() {
		return null;
	}

	public int getFileSize() {
		return 0;
	}

	public boolean isEditable() {
		return true;
	}

	public void setFileSize(int fileSize) throws SQLException {
		
	}
}
