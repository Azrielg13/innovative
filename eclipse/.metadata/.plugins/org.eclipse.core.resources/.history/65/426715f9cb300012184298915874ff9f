package com.sce.esp.object.model;

import java.io.Serializable;
import java.sql.PreparedStatement;
import java.sql.ResultSet;
import java.sql.SQLException;
import java.util.Hashtable;
import java.util.Map;

public class AgYearExtender extends AgYear implements Serializable{
	private static Hashtable<String,AgYearExtender> hash = new Hashtable<String,AgYearExtender>();
	private AgObject ao;
	private int simId;
	private int year;
	
	public AgYearExtender(AgObject ao, int simId, int year){
		this.ao = ao;
		this.simId = simId;
		this.year = year;
	}
	
	public static AgYearExtender getInstance(String key){
		return hash.get(key);
	}
	public static AgYearExtender getInstance(AgObject ao, int simId, int year){
		return getInstance(ao, simId, year, true);
	}
	public static AgYearExtender getInstance(AgObject ao, int simId, int year, boolean fetch){
		if(isNull(new Object[]{ao, year}))return null;
		String key = getHashKey(new Object[]{ao.getPlanyear(), ao, simId, year});
		AgYearExtender o = hash.get(key);
		if(o == null && fetch){
			o = new AgYearExtender(ao, simId, year);
			hash.put(key,o);
		}
		return o;
	}

	public AgObject getAgObject() throws SQLException {
		return ao;
	}
	public int getPlanyear() {
		return ao.getPlanyear();
	}
	
	public int getSimId() {
		return simId;
	}
	public int getYear() {
		return year;
	}

	public String getHashKey() {
		return null;
	}

	public void refresh(ResultSet rs) throws SQLException {
	}

	public void setPSKeys(PreparedStatement ps, int startIndex) throws SQLException {
	}

	public String getDBTable() {
		return null;
	}

	public String[] getPKSet() {
		return null;
	}

	public double getAllCktFxKvarExist(int simId) {
		return 0;
	}

	public double getAllCktFxKvarProp(int simId) {
		return 0;
	}

	public double getAllCktFxKvarReq(int simId) {
		return 0;
	}

	public double getAllCktSwtKvarExist(int simId) {
		return 0;
	}

	public double getAllCktSwtKvarProp(int simId) {
		return 0;
	}

	public double getAllCktSwtKvarReq(int simId) {
		return 0;
	}
}
