package com.sce.esp.object.model;
import com.sce.esp.object.dao.MitigationRiskDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDIV20_MITIGATION_RISK")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM MitigationRisk o WHERE o.MITIGATION_RISK_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM MitigationRisk o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM MitigationRisk o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIV20_MITIGATION_RISK o WHERE o.MITIGATION_RISK_ID=?"),//AUTO-GENERATED
})
public class MitigationRisk extends MitigationRiskDAO{
	public MitigationRisk(){
	}
	public MitigationRisk(int mitigationRiskId){
		super(mitigationRiskId);
	}
	public MitigationRisk(MitigationRisk orig){
		super(orig);
	}

	@Override
	public String toString() {
		return getName();
	}
}
