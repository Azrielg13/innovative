package com.sce.esp.object.model;

import java.sql.SQLException;

import com.sce.esp.log.EspLogger;
import com.sce.esp.object.ESPTestCase;

public class SceProjRevBundleTest extends ESPTestCase {
	
	public void testGetSceProjRevBundles() {
		try {
			SceProj rev = SceProj.getInstance(3532);
			for(SceProjBundle revBundle : rev.getSceProjBundles())
				EspLogger.warning(this, "bundle id = " + revBundle.getBundleId() + ", rev id = " + rev.getSceProjId());
		} catch (SQLException e) {
			e.printStackTrace();
		}
	}
	
