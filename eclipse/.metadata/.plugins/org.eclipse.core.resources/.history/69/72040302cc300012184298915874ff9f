	public static SysYearSceProj getInstance(int planyear, int simId, int sysId, int year, int programId)throws java.sql.SQLException{
		return getInstance(planyear, simId, sysId, year, programId, true);
	}
	public static SysYearSceProj getInstance(int planyear, int simId, int sysId, int year, int programId, boolean fetch)throws java.sql.SQLException{
		if(isNull(planyear, sysId, year, programId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(planyear, simId, sysId, year, programId);
		Cache cache = em.getEntityManagerFactory().getCache();
		SysYearSceProj o = null;
		if(cache != null && cache.contains(SysYearSceProj.class, pk))
			o = em.find(SysYearSceProj.class, pk);
		if(o==null && getAICache().contains(SysYearSceProj.class, pk))
			o = getAICache().find(SysYearSceProj.class, pk);
		if(o==null && fetch)
			o = em.find(SysYearSceProj.class, pk);
		if(o==null && fetch && SysYearSceProj.class.getAnnotation(AutoInsertable.class) != null){
			o = new SysYearSceProj(planyear, simId, sysId, year, programId);
			getAICache().cache(SysYearSceProj.class, o);
			o.setPtr();
		}
		return o;
	}
	public static Collection<SysYearSceProj> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<SysYearSceProj> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<SysYearSceProj> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM SysYearSceProj o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<SysYearSceProj> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<SysYearSceProj> tq = em.createQuery(jpql,SysYearSceProj.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<SysYearSceProj> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<SysYearSceProj> tq = em.createNamedQuery(name,SysYearSceProj.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public SysYearSceProjDAO(){}
	public SysYearSceProjDAO(int planyear, int simId, int sysId, int year, int programId){
		this.planyear=planyear;
		this.simId=simId;
		this.sysId=sysId;
		this.year=year;
		this.programId=programId;
	}
	public SysYearSceProjDAO(SysYearSceProjDAO orig){
		super(orig);
		this.planyear=orig.getPlanyear();
		this.simId=orig.getSimId();
		this.sysId=orig.getSysId();
		this.year=orig.getYear();
		this.programId=orig.getProgramId();
		copyFrom(orig);
	}
	public void copyFrom(SysYearSceProjDAO orig){
		this.projCount=orig.getProjCount();
		this.capAdd=orig.getCapAdd();
		this.cktAdd=orig.getCktAdd();
		this.varAdd=orig.getVarAdd();
		this.lastCalc=orig.getLastCalc();
		this.altDouble1=orig.getAltDouble1();
		this.altDouble2=orig.getAltDouble2();
		this.altDouble3=orig.getAltDouble3();
		this.altVarchar1=orig.getAltVarchar1();
		this.altVarchar2=orig.getAltVarchar2();
		this.altVarchar3=orig.getAltVarchar3();
		this.altDate1=orig.getAltDate1();
		this.altDate2=orig.getAltDate2();
		this.altDate3=orig.getAltDate3();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{planyear, simId, sysId, year, programId};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	public SysYearSceProj getPtr(){
		return ptr;
	}
	public void setPtr(SysYearSceProj ptr){
		if(this.ptr!=null)this.ptr.removeWatcher(this);
		this.ptr=ptr;
		if(ptr!=null)ptr.addWatcher(this);
	}
	public void setPtr()throws java.sql.SQLException{
		if(getSimId()==0)return;
		int bs=0;
		Simulation sim = Simulation.getInstance(getPlanyear(),getSimId());
		if(sim != null)
			bs = sim.getBaseSimId();
		setPtr(getInstance(getPlanyear(), bs, getSysId(), getYear(), getProgramId()));
	}
	@Id
	@Column(name="PLANYEAR",nullable=false,length=4)
	public int getPlanyear(){
		return planyear;
	}
	public void setPlanyear(int planyear) throws java.sql.SQLException{
		if(isSame(planyear, getPlanyear()))return;
		int oldValue = getPlanyear();
		this.planyear=planyear;
		setProperty("PLANYEAR", planyear, oldValue);
	}
	@Id
	@Column(name="SIM_ID",nullable=false,length=9)
	public int getSimId(){
		return simId;
	}
	public void setSimId(int simId) throws java.sql.SQLException{
		if(isSame(simId, getSimId()))return;
		int oldValue = getSimId();
		this.simId=simId;
		setProperty("SIM_ID", simId, oldValue);
		simulation=null;
	}
	@Id
