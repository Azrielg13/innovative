package com.sce.esp.object.dao;
/**Copy Right Frank todo */
/**Description of class, (we need to get this from somewhere, database? xml?)*/
import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.object.jpa.EntityManagerHelper;
import com.sce.esp.object.jpa.PrimaryKey;
import com.sce.esp.object.model.Criteria;
import com.sce.esp.object.model.CriteriaWs;
import com.sce.esp.object.model.MDIObject;
import com.sce.esp.object.model.Weatherstation;
import java.util.Collection;
import java.util.Vector;
import javax.persistence.Cache;
import javax.persistence.Column;
import javax.persistence.EntityManager;
import javax.persistence.Id;
import javax.persistence.TypedQuery;
public abstract class CriteriaWsDAO extends MDIObject{
	public static enum KEY_PROPERTY{PLANYEAR,STATION_ID,CRITERIA_ID};
	public static enum PROPERTY{PLANYEAR,STATION_ID,CRITERIA_ID,TEMP_RANGE,NOTE,INSERT_TS,INSERT_USER_ID,MODIFIED_TS,MODIFIED_USER_ID,DELETED_TS,DELETED_USER_ID};
	public final static int PLANYEAR_LIMIT = 4;
	public final static int STATIONID_LIMIT = 9;
	public final static int CRITERIAID_LIMIT = 9;
	public final static int TEMPRANGE_LIMIT = 24;
	public final static int NOTE_LIMIT = 128;
	private int planyear;
	private int stationId;
	private int criteriaId;
	private double tempRange;
	private String note;
	private Criteria criteria;
	private Weatherstation weatherstation;
	public static CriteriaWs getInstance(int planyear, int stationId, int criteriaId)throws java.sql.SQLException{
		return getInstance(planyear, stationId, criteriaId, true);
	}
	public static CriteriaWs getInstance(int planyear, int stationId, int criteriaId, boolean fetch)throws java.sql.SQLException{
		if(isNull(planyear, stationId, criteriaId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(planyear, stationId, criteriaId);
		Cache cache = em.getEntityManagerFactory().getCache();
		CriteriaWs o = null;
		if(cache != null && cache.contains(CriteriaWs.class, pk))
			o = em.find(CriteriaWs.class, pk);
		if(o==null && getAICache().contains(CriteriaWs.class, pk))
			o = getAICache().find(CriteriaWs.class, pk);
		if(o==null && fetch)
			o = em.find(CriteriaWs.class, pk);
		if(o==null && fetch && CriteriaWs.class.getAnnotation(AutoInsertable.class) != null){
			o = new CriteriaWs(planyear, stationId, criteriaId);
			getAICache().cache(CriteriaWs.class, o);
		}
		return o;
	}
	public static Collection<CriteriaWs> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<CriteriaWs> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<CriteriaWs> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM CriteriaWs o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<CriteriaWs> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<CriteriaWs> tq = em.createQuery(jpql,CriteriaWs.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<CriteriaWs> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<CriteriaWs> tq = em.createNamedQuery(name,CriteriaWs.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public CriteriaWsDAO(){}
	public CriteriaWsDAO(int planyear, int stationId, int criteriaId){
		this.planyear=planyear;
		this.stationId=stationId;
		this.criteriaId=criteriaId;
	}
	public CriteriaWsDAO(CriteriaWsDAO orig){
		super(orig);
		this.planyear=orig.getPlanyear();
		this.stationId=orig.getStationId();
		this.criteriaId=orig.getCriteriaId();
		copyFrom(orig);
	}
	public void copyFrom(CriteriaWsDAO orig){
		this.tempRange=orig.getTempRange();
		this.note=orig.getNote();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{planyear, stationId, criteriaId};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	@Id
	@Column(name="PLANYEAR",nullable=false,length=4)
	public int getPlanyear(){
		return planyear;
	}
	public void setPlanyear(int planyear) throws java.sql.SQLException{
		if(isSame(planyear, getPlanyear()))return;
		int oldValue = getPlanyear();
		this.planyear=planyear;
		setProperty("PLANYEAR", planyear, oldValue);
	}
	@Id
	@Column(name="STATION_ID",nullable=false,length=9)
	public int getStationId(){
		return stationId;
	}
	public void setStationId(int stationId) throws java.sql.SQLException{
		if(isSame(stationId, getStationId()))return;
		int oldValue = getStationId();
		this.stationId=stationId;
		setProperty("STATION_ID", stationId, oldValue);
		weatherstation=null;
	}
	@Id
	@Column(name="CRITERIA_ID",nullable=false,length=9)
	public int getCriteriaId(){
		return criteriaId;
	}
	public void setCriteriaId(int criteriaId) throws java.sql.SQLException{
		if(isSame(criteriaId, getCriteriaId()))return;
		int oldValue = getCriteriaId();
		this.criteriaId=criteriaId;
		setProperty("CRITERIA_ID", criteriaId, oldValue);
		criteria=null;
	}
	@Column(name="TEMP_RANGE",nullable=true,length=24)
	public double getTempRange(){
		return tempRange;
	}
	public void setTempRange(double tempRange) throws java.sql.SQLException{
		if(isSame(tempRange, getTempRange()))return;
		double oldValue = getTempRange();
		this.tempRange=tempRange;
		setProperty("TEMP_RANGE", tempRange, oldValue);
	}
	@Column(name="NOTE",nullable=true,length=128)
	public String getNote(){
		return note;
	}
	public void setNote(String note) throws java.sql.SQLException{
		if(isSame(note, getNote()))return;
		String oldValue = getNote();
		this.note=note;
		setProperty("NOTE", note, oldValue);
	}
	public Criteria getCriteria()throws java.sql.SQLException{
		if(criteria==null)
			criteria=Criteria.getInstance(getPlanyear(), getCriteriaId());
		return criteria;
	}
	public void setCriteria(Criteria criteria)throws java.sql.SQLException{
		setCriteriaId(criteria==null?0:criteria.getCriteriaId());
		this.criteria=criteria;
	}
	public Weatherstation getWeatherstation()throws java.sql.SQLException{
		if(weatherstation==null)
			weatherstation=Weatherstation.getInstance(getPlanyear(), getStationId());
		return weatherstation;
	}
	public void setWeatherstation(Weatherstation weatherstation)throws java.sql.SQLException{
		setStationId(weatherstation==null?0:weatherstation.getStationId());
		this.weatherstation=weatherstation;
	}
	public CriteriaWs copy()throws java.sql.SQLException{
		CriteriaWs cp = new CriteriaWs((CriteriaWs)this);
		copyChildrenTo(cp);
		return cp;
	}
	public void copyChildrenTo(CriteriaWsDAO cp)throws java.sql.SQLException{
		super.copyChildrenTo(cp);
	}
	public Vector<String> getDifference(CriteriaWsDAO o){
		Vector<String> diffs = super.getDifference(o);
		if(!isSame(getPlanyear(),o.getPlanyear())) diffs.add("PLANYEAR");
		if(!isSame(getStationId(),o.getStationId())) diffs.add("STATION_ID");
		if(!isSame(getCriteriaId(),o.getCriteriaId())) diffs.add("CRITERIA_ID");
		if(!isSame(getTempRange(),o.getTempRange())) diffs.add("TEMP_RANGE");
		if(!isSame(getNote(),o.getNote())) diffs.add("NOTE");
		return diffs;
	}
	public void insertChildren()throws java.sql.SQLException{
	}
}
