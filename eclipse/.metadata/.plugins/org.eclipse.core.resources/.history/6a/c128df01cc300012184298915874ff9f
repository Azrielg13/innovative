package com.sce.esp.object.dao;
/**Copy Right Frank todo */
/**Description of class, (we need to get this from somewhere, database? xml?)*/
import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.object.jpa.EntityManagerHelper;
import com.sce.esp.object.jpa.PrimaryKey;
import com.sce.esp.object.model.Department;
import com.sce.esp.object.model.MDIObject;
import com.sce.esp.object.model.PifLabor;
import java.util.Collection;
import java.util.Vector;
import javax.persistence.Cache;
import javax.persistence.Column;
import javax.persistence.EntityManager;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.TypedQuery;
public abstract class PifLaborDAO extends MDIObject{
	public static enum KEY_PROPERTY{PLANYEAR,LABOR_TYPE_ID};
	public static enum PROPERTY{PLANYEAR,LABOR_TYPE_ID,LABOR_TYPE,L_FACTOR,INSERT_TS,INSERT_USER_ID,MODIFIED_TS,MODIFIED_USER_ID,DELETED_TS,DELETED_USER_ID};
	public final static int PLANYEAR_LIMIT = 4;
	public final static int LABORTYPEID_LIMIT = 9;
	public final static int LABORTYPE_LIMIT = 20;
	public final static int LFACTOR_LIMIT = 6;
	private int planyear;
	private int laborTypeId;
	private String laborType;
	private int lFactor;
	private Department department;
	public static PifLabor getInstance(int planyear, int laborTypeId)throws java.sql.SQLException{
		return getInstance(planyear, laborTypeId, true);
	}
	public static PifLabor getInstance(int planyear, int laborTypeId, boolean fetch)throws java.sql.SQLException{
		if(isNull(planyear, laborTypeId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(planyear, laborTypeId);
		Cache cache = em.getEntityManagerFactory().getCache();
		PifLabor o = null;
		if(cache != null && cache.contains(PifLabor.class, pk))
			o = em.find(PifLabor.class, pk);
		if(o==null && getAICache().contains(PifLabor.class, pk))
			o = getAICache().find(PifLabor.class, pk);
		if(o==null && fetch)
			o = em.find(PifLabor.class, pk);
		if(o==null && fetch && PifLabor.class.getAnnotation(AutoInsertable.class) != null){
			o = new PifLabor(planyear, laborTypeId);
			getAICache().cache(PifLabor.class, o);
		}
		return o;
	}
	public static Collection<PifLabor> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<PifLabor> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<PifLabor> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM PifLabor o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<PifLabor> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<PifLabor> tq = em.createQuery(jpql,PifLabor.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<PifLabor> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<PifLabor> tq = em.createNamedQuery(name,PifLabor.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public PifLaborDAO(){}
	public PifLaborDAO(int planyear, int laborTypeId){
		this.planyear=planyear;
		this.laborTypeId=laborTypeId;
	}
	public PifLaborDAO(PifLaborDAO orig){
		super(orig);
		this.planyear=orig.getPlanyear();
		copyFrom(orig);
	}
	public void copyFrom(PifLaborDAO orig){
		this.laborType=orig.getLaborType();
		this.lFactor=orig.getLFactor();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{planyear, laborTypeId};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	@Id
	@Column(name="PLANYEAR",nullable=false,length=4)
	public int getPlanyear(){
		return planyear;
	}
	public void setPlanyear(int planyear) throws java.sql.SQLException{
		if(isSame(planyear, getPlanyear()))return;
		int oldValue = getPlanyear();
		this.planyear=planyear;
		setProperty("PLANYEAR", planyear, oldValue);
		department=null;
	}
	@Id
	@SequenceGenerator(name="MDI640_SEQ",sequenceName="MDI640_SEQ")
	@Column(name="LABOR_TYPE_ID",nullable=false,length=9)
	public int getLaborTypeId(){
		return laborTypeId;
	}
	public void setLaborTypeId(int laborTypeId) throws java.sql.SQLException{
		if(isSame(laborTypeId, getLaborTypeId()))return;
		int oldValue = getLaborTypeId();
		this.laborTypeId=laborTypeId;
		setProperty("LABOR_TYPE_ID", laborTypeId, oldValue);
	}
	@Column(name="LABOR_TYPE",nullable=false,length=20)
	public String getLaborType(){
		return laborType;
	}
	public void setLaborType(String laborType) throws java.sql.SQLException{
		if(isSame(laborType, getLaborType()))return;
		String oldValue = getLaborType();
		this.laborType=laborType;
		setProperty("LABOR_TYPE", laborType, oldValue);
	}
	@Column(name="L_FACTOR",nullable=false,length=6)
	public int getLFactor(){
		return lFactor;
	}
	public void setLFactor(int lFactor) throws java.sql.SQLException{
		if(isSame(lFactor, getLFactor()))return;
		int oldValue = getLFactor();
		this.lFactor=lFactor;
		setProperty("L_FACTOR", lFactor, oldValue);
	}
	public Department getDepartment()throws java.sql.SQLException{
		if(department==null)
			department=Department.getInstance(getPlanyear());
		return department;
	}
	public void setDepartment(Department department)throws java.sql.SQLException{
		setPlanyear(department==null?0:department.getPlanyear());
		this.department=department;
	}
	public PifLabor copy()throws java.sql.SQLException{
		PifLabor cp = new PifLabor((PifLabor)this);
		copyChildrenTo(cp);
		return cp;
	}
	public void copyChildrenTo(PifLaborDAO cp)throws java.sql.SQLException{
		super.copyChildrenTo(cp);
	}
	public Vector<String> getDifference(PifLaborDAO o){
		Vector<String> diffs = super.getDifference(o);
		if(!isSame(getPlanyear(),o.getPlanyear())) diffs.add("PLANYEAR");
		if(!isSame(getLaborTypeId(),o.getLaborTypeId())) diffs.add("LABOR_TYPE_ID");
		if(!isSame(getLaborType(),o.getLaborType())) diffs.add("LABOR_TYPE");
		if(!isSame(getLFactor(),o.getLFactor())) diffs.add("L_FACTOR");
		return diffs;
	}
	public void insertChildren()throws java.sql.SQLException{
	}
}
