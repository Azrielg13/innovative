package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PifBudgetCatDAO;
@Entity
@Table(schema="MDI",name="MDI665_PIF_BUDGET_CAT")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PifBudgetCat o WHERE o.PLANYEAR=?1 AND o.PIF_BUDGET_CAT_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PifBudgetCat o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PifBudgetCat o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI665_PIF_BUDGET_CAT o WHERE o.PLANYEAR=? AND o.PIF_BUDGET_CAT_ID=?"),//AUTO-GENERATED
})
public class PifBudgetCat extends PifBudgetCatDAO implements Serializable{

	private static final long serialVersionUID = -4285958014736524447L;

	@Override
	public String toString() {
		return getName();
	}

	public PifBudgetCat(){
	}
	public PifBudgetCat(int planyear, int pifBudgetCatId) {
		super(planyear, pifBudgetCatId);
	}

	public PifBudgetCat(PifBudgetCat pifBudgetCat) {
		super(pifBudgetCat);
	}
}
