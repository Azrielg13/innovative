package com.sce.esp.object.model;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.CprPlantDAO;
@Entity
@Table(schema="MDI",name="MDISC0_CPR_PLANT")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM CprPlant o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM CprPlant o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM CprPlant o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDISC0_CPR_PLANT o WHERE o.ID=?"),//AUTO-GENERATED
})
public class CprPlant extends CprPlantDAO{
	public static final int TRANSMISSION = 42;
	public static final int DISTRIBUTION = 43;
	public static final int GENERAL = 44;
	public static final int CATALINA = 45;
	public CprPlant(){
	}
	public CprPlant(int id){
		super(id);
	}
	public CprPlant(CprPlant orig){
		super(orig);
	}
	public void addCprPrime(CprPrime cprPrime)throws SQLException{
		for(CprPrime cprP:getCprPrimes())
			if(cprP.getCprPrimeAcct() == cprPrime.getCprPrimeAcct())
				throw new SQLException("Duplicate Prime Account: \""+cprPrime.getCprPrimeAcct()+"\" for Plant: \""+this+"\"");
		super.addCprPrime(cprPrime);
	}
	public String toString(){
		return getName();
	}
}
