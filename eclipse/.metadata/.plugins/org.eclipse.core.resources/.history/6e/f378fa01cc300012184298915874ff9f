package com.sce.esp.object.dao;
/**Copy Right Frank todo */
/**Description of class, (we need to get this from somewhere, database? xml?)*/
import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.object.jpa.EntityManagerHelper;
import com.sce.esp.object.jpa.PrimaryKey;
import com.sce.esp.object.model.Discipline;
import com.sce.esp.object.model.WorkGroup;
import com.sce.esp.object.model.WorkGroupDis;
import java.util.Calendar;
import java.util.Collection;
import java.util.Vector;
import javax.persistence.Cache;
import javax.persistence.Column;
import javax.persistence.EntityManager;
import javax.persistence.Id;
import javax.persistence.TypedQuery;
public abstract class WorkGroupDisDAO extends DataAccessObject{
	public static enum KEY_PROPERTY{WORK_GROUP_ID,TYPE,DISCIPLINE_ID};
	public static enum PROPERTY{WORK_GROUP_ID,TYPE,DISCIPLINE_ID,ALT_DOUBLE1,ALT_VARCHAR1,ALT_DATE1};
	public final static int WORKGROUPID_LIMIT = 9;
	public final static int TYPE_LIMIT = 2;
	public final static int DISCIPLINEID_LIMIT = 9;
	public final static int ALTDOUBLE1_LIMIT = 24;
	public final static int ALTVARCHAR1_LIMIT = 256;
	public final static int ALTDATE1_LIMIT = 7;
	private int workGroupId;
	private int type;
	private int disciplineId;
	private double altDouble1;
	private String altVarchar1;
	private Calendar altDate1;
	private WorkGroup workGroup;
	private Discipline discipline;
	public static WorkGroupDis getInstance(int workGroupId, int type, int disciplineId)throws java.sql.SQLException{
		return getInstance(workGroupId, type, disciplineId, true);
	}
	public static WorkGroupDis getInstance(int workGroupId, int type, int disciplineId, boolean fetch)throws java.sql.SQLException{
		if(isNull(workGroupId, type, disciplineId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(workGroupId, type, disciplineId);
		Cache cache = em.getEntityManagerFactory().getCache();
		WorkGroupDis o = null;
		if(cache != null && cache.contains(WorkGroupDis.class, pk))
			o = em.find(WorkGroupDis.class, pk);
		if(o==null && getAICache().contains(WorkGroupDis.class, pk))
			o = getAICache().find(WorkGroupDis.class, pk);
		if(o==null && fetch)
			o = em.find(WorkGroupDis.class, pk);
		if(o==null && fetch && WorkGroupDis.class.getAnnotation(AutoInsertable.class) != null){
			o = new WorkGroupDis(workGroupId, type, disciplineId);
			getAICache().cache(WorkGroupDis.class, o);
		}
		return o;
	}
	public static Collection<WorkGroupDis> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<WorkGroupDis> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<WorkGroupDis> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM WorkGroupDis o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<WorkGroupDis> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<WorkGroupDis> tq = em.createQuery(jpql,WorkGroupDis.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<WorkGroupDis> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<WorkGroupDis> tq = em.createNamedQuery(name,WorkGroupDis.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public WorkGroupDisDAO(){}
	public WorkGroupDisDAO(int workGroupId, int type, int disciplineId){
		this.workGroupId=workGroupId;
		this.type=type;
		this.disciplineId=disciplineId;
	}
	public WorkGroupDisDAO(WorkGroupDisDAO orig){
		super(orig);
		this.workGroupId=orig.getWorkGroupId();
		this.type=orig.getType();
		this.disciplineId=orig.getDisciplineId();
		copyFrom(orig);
	}
	public void copyFrom(WorkGroupDisDAO orig){
		this.altDouble1=orig.getAltDouble1();
		this.altVarchar1=orig.getAltVarchar1();
		this.altDate1=orig.getAltDate1();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{workGroupId, type, disciplineId};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	@Id
	@Column(name="WORK_GROUP_ID",nullable=false,length=9)
	public int getWorkGroupId(){
		return workGroupId;
	}
	public void setWorkGroupId(int workGroupId) throws java.sql.SQLException{
		if(isSame(workGroupId, getWorkGroupId()))return;
		int oldValue = getWorkGroupId();
		this.workGroupId=workGroupId;
		setProperty("WORK_GROUP_ID", workGroupId, oldValue);
		workGroup=null;
	}
	@Id
	@Column(name="TYPE",nullable=false,length=2)
	public int getType(){
		return type;
	}
	public void setType(int type) throws java.sql.SQLException{
		if(isSame(type, getType()))return;
		int oldValue = getType();
		this.type=type;
		setProperty("TYPE", type, oldValue);
	}
	@Id
	@Column(name="DISCIPLINE_ID",nullable=false,length=9)
	public int getDisciplineId(){
		return disciplineId;
	}
	public void setDisciplineId(int disciplineId) throws java.sql.SQLException{
		if(isSame(disciplineId, getDisciplineId()))return;
		int oldValue = getDisciplineId();
		this.disciplineId=disciplineId;
		setProperty("DISCIPLINE_ID", disciplineId, oldValue);
		discipline=null;
	}
	@Column(name="ALT_DOUBLE1",nullable=true,length=24)
	public double getAltDouble1(){
		return altDouble1;
	}
	public void setAltDouble1(double altDouble1) throws java.sql.SQLException{
		if(isSame(altDouble1, getAltDouble1()))return;
		double oldValue = getAltDouble1();
		this.altDouble1=altDouble1;
		setProperty("ALT_DOUBLE1", altDouble1, oldValue);
	}
	@Column(name="ALT_VARCHAR1",nullable=true,length=256)
	public String getAltVarchar1(){
		return altVarchar1;
	}
	public void setAltVarchar1(String altVarchar1) throws java.sql.SQLException{
		if(isSame(altVarchar1, getAltVarchar1()))return;
		String oldValue = getAltVarchar1();
		this.altVarchar1=altVarchar1;
		setProperty("ALT_VARCHAR1", altVarchar1, oldValue);
	}
	@Column(name="ALT_DATE1",nullable=true,length=7)
	public Calendar getAltDate1(){
		return altDate1;
	}
	public void setAltDate1(Calendar altDate1) throws java.sql.SQLException{
		if(isSame(altDate1, getAltDate1()))return;
		Calendar oldValue = getAltDate1();
		this.altDate1=altDate1;
		setProperty("ALT_DATE1", altDate1, oldValue);
	}
	public WorkGroup getWorkGroup()throws java.sql.SQLException{
		if(workGroup==null)
			workGroup=WorkGroup.getInstance(getWorkGroupId());
		return workGroup;
	}
	public void setWorkGroup(WorkGroup workGroup)throws java.sql.SQLException{
		setWorkGroupId(workGroup==null?0:workGroup.getWorkGroupId());
		this.workGroup=workGroup;
	}
	public Discipline getDiscipline()throws java.sql.SQLException{
		if(discipline==null)
			discipline=Discipline.getInstance(getDisciplineId());
		return discipline;
	}
	public void setDiscipline(Discipline discipline)throws java.sql.SQLException{
		setDisciplineId(discipline==null?0:discipline.getDisciplineId());
		this.discipline=discipline;
	}
	public WorkGroupDis copy()throws java.sql.SQLException{
		WorkGroupDis cp = new WorkGroupDis((WorkGroupDis)this);
		copyChildrenTo(cp);
		return cp;
	}
	public void copyChildrenTo(WorkGroupDisDAO cp)throws java.sql.SQLException{
		super.copyChildrenTo(cp);
	}
	public Vector<String> getDifference(WorkGroupDisDAO o){
		Vector<String> diffs = super.getDifference(o);
		if(!isSame(getWorkGroupId(),o.getWorkGroupId())) diffs.add("WORK_GROUP_ID");
		if(!isSame(getType(),o.getType())) diffs.add("TYPE");
		if(!isSame(getDisciplineId(),o.getDisciplineId())) diffs.add("DISCIPLINE_ID");
		if(!isSame(getAltDouble1(),o.getAltDouble1())) diffs.add("ALT_DOUBLE1");
		if(!isSame(getAltVarchar1(),o.getAltVarchar1())) diffs.add("ALT_VARCHAR1");
		if(!isSame(getAltDate1(),o.getAltDate1())) diffs.add("ALT_DATE1");
		return diffs;
	}
	public void insertChildren()throws java.sql.SQLException{
	}
}
