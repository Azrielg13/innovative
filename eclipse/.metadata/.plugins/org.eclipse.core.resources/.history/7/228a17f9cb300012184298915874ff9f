package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.CriteriaWsDAO;
@Entity
@Table(schema="MDI",name="MDI610_CRITERIA_WS")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM CriteriaWs o WHERE o.PLANYEAR=?1 AND o.STATION_ID=?2 AND o.CRITERIA_ID=?3"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM CriteriaWs o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM CriteriaWs o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByWeatherstation", query="SELECT o FROM CriteriaWs o WHERE o.PLANYEAR=?1 AND o.STATION_ID=?2 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI610_CRITERIA_WS o WHERE o.PLANYEAR=? AND o.STATION_ID=? AND o.CRITERIA_ID=?"),//AUTO-GENERATED
})
public class CriteriaWs extends CriteriaWsDAO implements Serializable{
	

	/**
	 * The Constructor.
	 * 
	 * @param planyear the planyear
	 */
	public CriteriaWs(){
	}
	public CriteriaWs(int planyear) {
		super(planyear,0,0);
	}
	
	/**
	 * The Constructor.
	 * 
	 * @param stationId the station id
	 * @param criteriaId the criteria id
	 * @param planyear the planyear
	 */
	public CriteriaWs(int planyear, int stationId, int criteriaId){
		super(planyear,stationId,criteriaId);
	}

	public CriteriaWs(CriteriaWs criteriaWs) {
		super(criteriaWs);
	}
}
