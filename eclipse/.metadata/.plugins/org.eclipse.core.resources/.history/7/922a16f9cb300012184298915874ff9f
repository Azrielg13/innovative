package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.CktContDAO;
@Entity
@Table(schema="MDI",name="MDI270_CKT_CONT")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM CktCont o WHERE o.PLANYEAR=?1 AND o.CKT_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM CktCont o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM CktCont o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByCkt", query="SELECT o FROM CktCont o WHERE o.PLANYEAR=?1 AND o.CKT_ID=?2 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI270_CKT_CONT o WHERE o.PLANYEAR=? AND o.CKT_ID=?"),//AUTO-GENERATED
})
public class CktCont extends CktContDAO implements Serializable{
	
	/**
	 * 
	 */
	private static final long serialVersionUID = -3766954108449349675L;

	/**
	 * The Constructor.
	 * 
	 * @param cktId the ckt id
	 * @param planyear the planyear
	 */
	public CktCont(){
	}
	public CktCont(int planyear, int cktId){
		super(planyear,cktId);
	}

	public CktCont(CktCont cktCont) {
		super(cktCont);
	}
}
