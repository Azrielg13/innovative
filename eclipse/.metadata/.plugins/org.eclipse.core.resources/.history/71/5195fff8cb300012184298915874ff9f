package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PifLaborDAO;
@Entity
@Table(schema="MDI",name="MDI640_PIF_LABOR")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PifLabor o WHERE o.PLANYEAR=?1 AND o.LABOR_TYPE_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PifLabor o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PifLabor o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByDepartment", query="SELECT o FROM PifLabor o WHERE o.PLANYEAR=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI640_PIF_LABOR o WHERE o.PLANYEAR=? AND o.LABOR_TYPE_ID=?"),//AUTO-GENERATED
})
public class PifLabor extends PifLaborDAO implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = -4536209255034166876L;

	/** The SCE. */
	public static int SCE=1;
	
	/** The CONT. */
	public static int CONT=2;
	
	public PifLabor(){
	}
	public PifLabor(int planyear) {
		super(planyear,0);
	}
	
	/**
	 * The Constructor.
	 * 
	 * @param laborTypeId the labor type id
	 * @param planyear the planyear
	 */
	public PifLabor(int planyear, int laborTypeId){
		super(planyear,laborTypeId);
	}
	public PifLabor(PifLabor pifLabor) {
		super(pifLabor);
	}

	public String toString(){
		if(getLaborTypeId()==0)
			return "User Specified";
		return getLaborType();	
	}
}
