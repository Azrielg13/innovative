package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.SysYearSceCostDAO;
import com.sce.esp.object.jpa.AutoInsertable;
@Entity
@AutoInsertable
@Table(schema="MDI",name="MDI547_SYS_YEAR_SCE_COST")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM SysYearSceCost o WHERE o.PLANYEAR=?1 AND o.SIM_ID=?2 AND o.SYS_ID=?3 AND o.YEAR=?4 AND o.PROGRAM_ID=?5 AND o.SCE_ELEM_ID=?6 AND o.STAGE=?7"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM SysYearSceCost o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM SysYearSceCost o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI547_SYS_YEAR_SCE_COST o WHERE o.PLANYEAR=? AND o.SIM_ID=? AND o.SYS_ID=? AND o.YEAR=? AND o.PROGRAM_ID=? AND o.SCE_ELEM_ID=? AND o.STAGE=?"),//AUTO-GENERATED
})
public class SysYearSceCost extends SysYearSceCostDAO implements Serializable{

	private static final long serialVersionUID = 942112651830467626L;

	/**
	 * @param planyear
	 * @param simId
	 * @param sysId
	 * @param year
	 * @param programId
	 * @param sceElemId
	 * @param stage
	 */
	public SysYearSceCost(){
	}
	public SysYearSceCost(int planyear, int simId, int sysId, int year, int programId, int sceElemId, int stage) {
		super(planyear, simId, sysId, year, programId, sceElemId, stage);
	}

	/**
	 * @param orig
	 */
	public SysYearSceCost(SysYearSceCost orig) {
		super(orig);
	}

}
