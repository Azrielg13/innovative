package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PifBiDAO;
@Entity
@Table(schema="MDI",name="MDI620_PIF_BI")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PifBi o WHERE o.PLANYEAR=?1 AND o.BI_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PifBi o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PifBi o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByDepartment", query="SELECT o FROM PifBi o WHERE o.PLANYEAR=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI620_PIF_BI o WHERE o.PLANYEAR=? AND o.BI_ID=?"),//AUTO-GENERATED
})
public class PifBi extends PifBiDAO implements Serializable{
	private static final long serialVersionUID = -3249639688033400349L;
	public final static int BI_350 = 1;
	public final static int BI_353 = 2;
	
	public PifBi(){
	}
	public PifBi(int planyear, int biId){
		super(planyear,biId);
	}
	public PifBi(int planyear) {
		super(planyear,0);
	}
	
	public PifBi(PifBi pifBi) {
		super(pifBi);
	}
	/**
	 * To string.
	 * 
	 * @return the string
	 */
//	public getBis(PifCategory pc){
//		if(pc == "RGT Project")
//			return ;
//		return ;
//	}
//	
	
	public String toString(){
		return getBi();
	}
}
