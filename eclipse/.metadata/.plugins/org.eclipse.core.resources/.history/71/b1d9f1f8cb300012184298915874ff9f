package com.sce.esp.object.model;

import java.io.Serializable;

import com.sce.esp.object.dao.LocDAO;

import javax.persistence.Entity;
import javax.persistence.Table;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
@Entity
@Table(schema="MDI",name="MDIT70_LOC")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM Loc o WHERE o.LOC_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM Loc o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM Loc o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIT70_LOC o WHERE o.LOC_ID=?"),//AUTO-GENERATED
})
public class Loc extends LocDAO implements Serializable{

	private static final long serialVersionUID = 2197051931828320342L;

	public Loc(){
	}
	public Loc(int locId) {
		super(locId);
	}

	public Loc(Loc orig) {
		super(orig);
	}

	@Override
	public String toString() {
		return getName();
	}
	
	public int compareTo(Object o){
		if(o instanceof Loc){
			Loc loc = (Loc)o;
			if(getLocId()>loc.getLocId())
				return -1;
			if(getLocId()<loc.getLocId())
				return 1;
		}
		return super.compareTo(o);
	}
}
