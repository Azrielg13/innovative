package com.sce.esp.object.model;

import java.io.Serializable;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.SceProgElemDAO;
@Entity
@Table(schema="MDI",name="MDIS73_SCE_PROG_ELEM")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM SceProgElem o WHERE o.PROGRAM_ID=?1 AND o.SCE_ELEM_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM SceProgElem o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM SceProgElem o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByProgram", query="SELECT o FROM SceProgElem o WHERE o.PROGRAM_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS73_SCE_PROG_ELEM o WHERE o.PROGRAM_ID=? AND o.SCE_ELEM_ID=?"),//AUTO-GENERATED
})
public class SceProgElem extends SceProgElemDAO implements Serializable{

	private static final long serialVersionUID = 4233106820220189074L;

	public SceProgElem(){
		super(0,0);
	}
	
	public SceProgElem(int programId, int sceElemId) {
		super(programId, sceElemId);
	}

	public SceProgElem(SceProgElem sceProgElem) {
		super(sceProgElem);
	}
	
	
	public String toString(){
		try {
			return getSceElem().getName();
		} catch (SQLException e) {
			e.printStackTrace();
		}
		return super.toString();
	}
	

}
