import javax.persistence.Id;
import javax.persistence.TypedQuery;
public abstract class AssumptionLocDAO extends DataAccessObject{
	public static enum KEY_PROPERTY{LOC_ID,ASSUMPTION_ID};
	public static enum PROPERTY{ASSUMPTION_ID,LOC_ID,NAME,DESCRIPTION,HELP_TEXT,ALT_VARCHAR1,ALT_DOUBLE1,INSERT_TS,INSERT_USER_ID,MODIFIED_TS,MODIFIED_USER_ID,DELETED_TS,DELETED_USER_ID};
	public final static int ASSUMPTIONID_LIMIT = 9;
	public final static int LOCID_LIMIT = 9;
	public final static int NAME_LIMIT = 64;
	public final static int DESCRIPTION_LIMIT = 1024;
	public final static int HELPTEXT_LIMIT = 1024;
	public final static int ALTVARCHAR1_LIMIT = 512;
	public final static int ALTDOUBLE1_LIMIT = 24;
	private int assumptionId;
	private int locId;
	private String name;
	private String description;
	private String helpText;
	private String altVarchar1;
	private double altDouble1;
	private Assumption assumption;
	private Loc loc;
	public static AssumptionLoc getInstance(int locId, int assumptionId)throws java.sql.SQLException{
		return getInstance(locId, assumptionId, true);
	}
	public static AssumptionLoc getInstance(int locId, int assumptionId, boolean fetch)throws java.sql.SQLException{
		if(isNull(locId, assumptionId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(locId, assumptionId);
		Cache cache = em.getEntityManagerFactory().getCache();
		AssumptionLoc o = null;
		if(cache != null && cache.contains(AssumptionLoc.class, pk))
			o = em.find(AssumptionLoc.class, pk);
		if(o==null && getAICache().contains(AssumptionLoc.class, pk))
			o = getAICache().find(AssumptionLoc.class, pk);
		if(o==null && fetch)
			o = em.find(AssumptionLoc.class, pk);
		if(o==null && fetch && AssumptionLoc.class.getAnnotation(AutoInsertable.class) != null){
			o = new AssumptionLoc(locId, assumptionId);
			getAICache().cache(AssumptionLoc.class, o);
		}
		return o;
	}
	public static Collection<AssumptionLoc> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<AssumptionLoc> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<AssumptionLoc> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM AssumptionLoc o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<AssumptionLoc> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<AssumptionLoc> tq = em.createQuery(jpql,AssumptionLoc.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<AssumptionLoc> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<AssumptionLoc> tq = em.createNamedQuery(name,AssumptionLoc.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public AssumptionLocDAO(){}
	public AssumptionLocDAO(int locId, int assumptionId){
		this.locId=locId;
		this.assumptionId=assumptionId;
	}
