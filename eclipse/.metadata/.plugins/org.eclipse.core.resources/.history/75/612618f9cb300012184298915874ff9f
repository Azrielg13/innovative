package com.sce.esp.object.model;

import java.io.Serializable;

import com.sce.esp.object.dao.DriverCriteriaDAO;

import javax.persistence.Entity;
import javax.persistence.Table;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
@Entity
@Table(schema="MDI",name="MDIT35_DRIVER_CRITERIA")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM DriverCriteria o WHERE o.DRIVER_CRITERIA_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM DriverCriteria o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM DriverCriteria o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIT35_DRIVER_CRITERIA o WHERE o.DRIVER_CRITERIA_ID=?"),//AUTO-GENERATED
})
public class DriverCriteria extends DriverCriteriaDAO implements Serializable{

	private static final long serialVersionUID = 7609265454107565593L;

	public DriverCriteria(){
	}
	public DriverCriteria(int driverCriteriaId) {
		super(driverCriteriaId);
	}

	public DriverCriteria(DriverCriteria orig) {
		super(orig);
	}

	@Override
	public String toString() {
		return getName();
	}

}
