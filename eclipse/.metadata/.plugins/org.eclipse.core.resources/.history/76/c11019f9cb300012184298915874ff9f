package com.sce.esp.object.model;
import java.sql.SQLException;
import java.util.Collection;
import java.util.Date;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.GiStudyDAO;
@Entity
@Table(schema="MDI",name="MDIZ70_GI_STUDY")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM GiStudy o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM GiStudy o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM GiStudy o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIZ70_GI_STUDY o WHERE o.ID=?"),//AUTO-GENERATED
})
public class GiStudy extends GiStudyDAO{
	public GiStudy(){
	}
	public GiStudy(int id){
		super(id);
	}
	public GiStudy(GiStudy orig){
		super(orig);
	}
	public Collection<GiStep> getGiSteps() {
		String[] params = new String[] { "STUDY_ID" };
		try {
			return GiStep.getCollection(params, getId());
		} catch (SQLException e) {
			e.printStackTrace();
		}
		return null;
	}

	public Date getQueueDateD() {
		return super.getQueueDate().getTime();
	}
	
}
