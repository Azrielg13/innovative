package com.sce.esp.object.model;

import java.io.Serializable;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.LgProjFileDAO;
@Entity
@Table(schema="MDI",name="MDI907_LG_PROJ_FILE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM LgProjFile o WHERE o.PLANYEAR=?1 AND o.FILE_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM LgProjFile o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM LgProjFile o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByLgProj", query="SELECT o FROM LgProjFile o WHERE o.PLANYEAR=?1 AND o.LG_PROJ_ID=?2 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI907_LG_PROJ_FILE o WHERE o.PLANYEAR=? AND o.FILE_ID=?"),//AUTO-GENERATED
})
public class LgProjFile extends LgProjFileDAO implements Serializable{
	private static final long serialVersionUID = 7419587559391535630L;
	/**
	 * The Constructor.
	 *
	 * @param planyear the planyear
	 */
	public LgProjFile(){
	}
	public LgProjFile(int planyear){
		super(planyear,0);
	}
	public LgProjFile(int planyear, int fileId){
		super(planyear,fileId);
	}
	
	public LgProjFile(LgProjFile lgProjFile) {
		super(lgProjFile);
	}
	public boolean isEditable(){
		return getPlanyear() == Department.getActivePlanYear();
	}
	public void delete() throws SQLException{
		deleteDB();
	}
}


