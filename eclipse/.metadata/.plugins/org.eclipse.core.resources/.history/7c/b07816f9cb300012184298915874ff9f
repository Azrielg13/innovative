package com.sce.esp.object.model;

import java.io.Serializable;
import java.sql.SQLException;
import java.util.Collection;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.CktSwtDAO;
@Entity
@Table(schema="MDI",name="MDI250_CKT_SWT")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM CktSwt o WHERE o.PLANYEAR=?1 AND o.SWT_TYPE_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM CktSwt o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM CktSwt o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByDepartment", query="SELECT o FROM CktSwt o WHERE o.PLANYEAR=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI250_CKT_SWT o WHERE o.PLANYEAR=? AND o.SWT_TYPE_ID=?"),//AUTO-GENERATED
})
public class CktSwt extends CktSwtDAO implements Serializable{
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 435468890821829514L;

	/**
	 * The Constructor.
	 * 
	 * @param planyear the planyear
	 */
	public CktSwt(){
	}
	public CktSwt(int planyear) {
		super(planyear,0);
	}
	
	/**
	 * The Constructor.
	 * 
	 * @param swtTypeId the swt type id
	 * @param planyear the planyear
	 */
	public CktSwt(int planyear, int swtTypeId){
		super(planyear,swtTypeId);
	}
	
	public CktSwt(CktSwt cktSwt) {
		super(cktSwt);
	}

	/**
	 * To string.
	 * 
	 * @return the string
	 */
	public String toString(){
		return getSwtType();
	}
	
	/**
	 * Gets the all.
	 * 
	 * @param planyear the planyear
	 * 
	 * @return the all
	 * 
	 * @throws SQLException the SQL exception
	 */
	public static Collection<CktSwt> getAll(int planyear)throws SQLException {
		return Department.getInstance(planyear, true).getCktSwts();
	}
}
