package com.sce.esp.object.model;
import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.SysYearProjDAO;
@Entity
@Table(schema="MDI",name="MDI210_SYS_YEAR_PROJ")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM SysYearProj o WHERE o.PLANYEAR=?1 AND o.SIM_ID=?2 AND o.SYS_ID=?3 AND o.YEAR=?4 AND o.PROG_ID=?5"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM SysYearProj o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM SysYearProj o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI210_SYS_YEAR_PROJ o WHERE o.PLANYEAR=? AND o.SIM_ID=? AND o.SYS_ID=? AND o.YEAR=? AND o.PROG_ID=?"),//AUTO-GENERATED
})
public class SysYearProj extends SysYearProjDAO implements Serializable{

	private static final long serialVersionUID = 5236462643688626581L;
	public SysYearProj(){
	}
	public SysYearProj(int planyear, int simId, int sysId, int year, int progId){
		super(planyear, simId, sysId, year, progId);
	}
	public SysYearProj(SysYearProj orig){
		super(orig);
	}
}
