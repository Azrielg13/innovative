package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.DisciplineDAO;
@Entity
@Table(schema="MDI",name="MDIS15_DISCIPLINE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM Discipline o WHERE o.DISCIPLINE_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM Discipline o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM Discipline o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS15_DISCIPLINE o WHERE o.DISCIPLINE_ID=?"),//AUTO-GENERATED
})
public class Discipline extends DisciplineDAO implements Serializable{
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 8912229057997013823L;

	public Discipline(){
		super(0);
	}
	
	public Discipline(int disciplineId) {
		super(disciplineId);
	}
	
	public Discipline(Discipline discipline) {
		super(discipline);
	}

	public String toString(){
		return getName();
	}

}
