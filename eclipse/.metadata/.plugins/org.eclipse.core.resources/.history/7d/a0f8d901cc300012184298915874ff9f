package com.sce.esp.object.dao;
/**Copy Right Frank todo */
/**Description of class, (we need to get this from somewhere, database? xml?)*/
import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.object.jpa.EntityManagerHelper;
import com.sce.esp.object.jpa.PrimaryKey;
import com.sce.esp.object.model.MDIObject;
import com.sce.esp.object.model.Simulation;
import com.sce.esp.object.model.Sys;
import com.sce.esp.object.model.SysMonthly;
import java.util.Calendar;
import java.util.Collection;
import java.util.Vector;
import javax.persistence.Cache;
import javax.persistence.Column;
import javax.persistence.EntityManager;
import javax.persistence.Id;
import javax.persistence.TypedQuery;
public abstract class SysMonthlyDAO extends MDIObject{
	public static enum KEY_PROPERTY{PLANYEAR,SIM_ID,SYS_ID,YEAR,MONTH};
	public static enum PROPERTY{PLANYEAR,SYS_ID,YEAR,MONTH,MONTHLY_PEAK,LAST_CALC,SIM_ID,INSERT_TS,INSERT_USER_ID,MODIFIED_TS,MODIFIED_USER_ID,DELETED_TS,DELETED_USER_ID};
	public final static int PLANYEAR_LIMIT = 4;
	public final static int SYSID_LIMIT = 9;
	public final static int YEAR_LIMIT = 4;
	public final static int MONTH_LIMIT = 2;
	public final static int MONTHLYPEAK_LIMIT = 24;
	public final static int LASTCALC_LIMIT = 7;
	public final static int SIMID_LIMIT = 9;
	private SysMonthly ptr;
	private int planyear;
	private int sysId;
	private int year;
	private int month;
	private double monthlyPeak;
	private Calendar lastCalc;
	private int simId = 0;
	private Simulation simulation;
	private Sys sys;
	public static SysMonthly getInstance(int planyear, int simId, int sysId, int year, int month)throws java.sql.SQLException{
		return getInstance(planyear, simId, sysId, year, month, true);
	}
	public static SysMonthly getInstance(int planyear, int simId, int sysId, int year, int month, boolean fetch)throws java.sql.SQLException{
		if(isNull(planyear, sysId, year, month))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(planyear, simId, sysId, year, month);
		Cache cache = em.getEntityManagerFactory().getCache();
		SysMonthly o = null;
		if(cache != null && cache.contains(SysMonthly.class, pk))
			o = em.find(SysMonthly.class, pk);
		if(o==null && getAICache().contains(SysMonthly.class, pk))
			o = getAICache().find(SysMonthly.class, pk);
		if(o==null && fetch)
			o = em.find(SysMonthly.class, pk);
		if(o==null && fetch && SysMonthly.class.getAnnotation(AutoInsertable.class) != null){
			o = new SysMonthly(planyear, simId, sysId, year, month);
			getAICache().cache(SysMonthly.class, o);
			o.setPtr();
		}
		return o;
	}
	public static Collection<SysMonthly> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<SysMonthly> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<SysMonthly> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM SysMonthly o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<SysMonthly> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<SysMonthly> tq = em.createQuery(jpql,SysMonthly.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<SysMonthly> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<SysMonthly> tq = em.createNamedQuery(name,SysMonthly.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public SysMonthlyDAO(){}
	public SysMonthlyDAO(int planyear, int simId, int sysId, int year, int month){
		this.planyear=planyear;
		this.simId=simId;
		this.sysId=sysId;
		this.year=year;
		this.month=month;
	}
	public SysMonthlyDAO(SysMonthlyDAO orig){
		super(orig);
		this.planyear=orig.getPlanyear();
		this.simId=orig.getSimId();
		this.sysId=orig.getSysId();
		this.year=orig.getYear();
		this.month=orig.getMonth();
		copyFrom(orig);
	}
	public void copyFrom(SysMonthlyDAO orig){
		this.monthlyPeak=orig.getMonthlyPeak();
		this.lastCalc=orig.getLastCalc();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{planyear, simId, sysId, year, month};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	public SysMonthly getPtr(){
		return ptr;
	}
	public void setPtr(SysMonthly ptr){
		if(this.ptr!=null)this.ptr.removeWatcher(this);
		this.ptr=ptr;
		if(ptr!=null)ptr.addWatcher(this);
	}
	public void setPtr()throws java.sql.SQLException{
		if(getSimId()==0)return;
		int bs=0;
		Simulation sim = Simulation.getInstance(getPlanyear(),getSimId());
		if(sim != null)
			bs = sim.getBaseSimId();
		setPtr(getInstance(getPlanyear(), bs, getSysId(), getYear(), getMonth()));
	}
	@Id
	@Column(name="PLANYEAR",nullable=false,length=4)
	public int getPlanyear(){
		return planyear;
	}
	public void setPlanyear(int planyear) throws java.sql.SQLException{
		if(isSame(planyear, getPlanyear()))return;
		int oldValue = getPlanyear();
		this.planyear=planyear;
		setProperty("PLANYEAR", planyear, oldValue);
	}
	@Id
	@Column(name="SYS_ID",nullable=false,length=9)
	public int getSysId(){
		return sysId;
	}
	public void setSysId(int sysId) throws java.sql.SQLException{
		if(isSame(sysId, getSysId()))return;
		int oldValue = getSysId();
		this.sysId=sysId;
		setProperty("SYS_ID", sysId, oldValue);
		sys=null;
	}
	@Id
	@Column(name="YEAR",nullable=false,length=4)
	public int getYear(){
		return year;
	}
	public void setYear(int year) throws java.sql.SQLException{
		if(isSame(year, getYear()))return;
		int oldValue = getYear();
		this.year=year;
		setProperty("YEAR", year, oldValue);
	}
	@Id
	@Column(name="MONTH",nullable=false,length=2)
	public int getMonth(){
		return month;
	}
	public void setMonth(int month) throws java.sql.SQLException{
		if(isSame(month, getMonth()))return;
		int oldValue = getMonth();
		this.month=month;
		setProperty("MONTH", month, oldValue);
	}
	@Column(name="MONTHLY_PEAK",nullable=true,length=24)
	public double getMonthlyPeak(){
		if(getPtr()!=null)
			return getPtr().getMonthlyPeak();
		return monthlyPeak;
	}
	public void setMonthlyPeak(double monthlyPeak) throws java.sql.SQLException{
		if(isSame(monthlyPeak, getMonthlyPeak()))return;
		if(getPtr()!=null){
			copyFrom(getPtr());
			setPtr(null);
		}
		double oldValue = getMonthlyPeak();
		this.monthlyPeak=monthlyPeak;
		setProperty("MONTHLY_PEAK", monthlyPeak, oldValue);
	}
	@Column(name="LAST_CALC",nullable=true,length=7)
	public Calendar getLastCalc(){
		if(getPtr()!=null)
			return getPtr().getLastCalc();
		return lastCalc;
	}
	public void setLastCalc(Calendar lastCalc) throws java.sql.SQLException{
		if(isSame(lastCalc, getLastCalc()))return;
		if(getPtr()!=null){
			copyFrom(getPtr());
			setPtr(null);
		}
		Calendar oldValue = getLastCalc();
		this.lastCalc=lastCalc;
		setProperty("LAST_CALC", lastCalc, oldValue);
	}
	@Id
	@Column(name="SIM_ID",nullable=false,length=9)
	public int getSimId(){
		return simId;
	}
	public void setSimId(int simId) throws java.sql.SQLException{
		if(isSame(simId, getSimId()))return;
		int oldValue = getSimId();
		this.simId=simId;
		setProperty("SIM_ID", simId, oldValue);
		simulation=null;
	}
	public Simulation getSimulation()throws java.sql.SQLException{
		if(simulation==null)
			simulation=Simulation.getInstance(getPlanyear(), getSimId());
		return simulation;
	}
	public void setSimulation(Simulation simulation)throws java.sql.SQLException{
		setSimId(simulation==null?0:simulation.getSimId());
		this.simulation=simulation;
	}
	public Sys getSys()throws java.sql.SQLException{
		if(sys==null)
			sys=Sys.getInstance(getPlanyear(), getSysId());
		return sys;
	}
	public void setSys(Sys sys)throws java.sql.SQLException{
		setSysId(sys==null?0:sys.getSysId());
		this.sys=sys;
	}
	public SysMonthly copy()throws java.sql.SQLException{
		SysMonthly cp = new SysMonthly((SysMonthly)this);
		copyChildrenTo(cp);
		return cp;
	}
	public void copyChildrenTo(SysMonthlyDAO cp)throws java.sql.SQLException{
		super.copyChildrenTo(cp);
	}
	public Vector<String> getDifference(SysMonthlyDAO o){
		Vector<String> diffs = super.getDifference(o);
		if(!isSame(getPlanyear(),o.getPlanyear())) diffs.add("PLANYEAR");
		if(!isSame(getSysId(),o.getSysId())) diffs.add("SYS_ID");
		if(!isSame(getYear(),o.getYear())) diffs.add("YEAR");
		if(!isSame(getMonth(),o.getMonth())) diffs.add("MONTH");
		if(!isSame(getMonthlyPeak(),o.getMonthlyPeak())) diffs.add("MONTHLY_PEAK");
		if(!isSame(getLastCalc(),o.getLastCalc())) diffs.add("LAST_CALC");
		if(!isSame(getSimId(),o.getSimId())) diffs.add("SIM_ID");
		return diffs;
	}
	public void insertChildren()throws java.sql.SQLException{
	}
}
