package com.sce.esp.object.model;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.ScopeItemDescriptiveDAO;
@Entity
@Table(schema="MDI",name="MDIU62_SCOPE_ITEM_DESCRIPTIVE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM ScopeItemDescriptive o WHERE o.SCOPE_ITEM_ID=?1 AND o.DESC_TYPE_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM ScopeItemDescriptive o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM ScopeItemDescriptive o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByScopeItem", query="SELECT o FROM ScopeItemDescriptive o WHERE o.SCOPE_ITEM_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIU62_SCOPE_ITEM_DESCRIPTIVE o WHERE o.SCOPE_ITEM_ID=? AND o.DESC_TYPE_ID=?"),//AUTO-GENERATED
})
public class ScopeItemDescriptive extends ScopeItemDescriptiveDAO{
	public ScopeItemDescriptive(){
	}
	public ScopeItemDescriptive(int scopeItemId, int descTypeId){
		super(scopeItemId, descTypeId);
	}
	public ScopeItemDescriptive(ScopeItemDescriptive orig){
		super(orig);
	}
//	public String getValue() throws SQLException {
//		if(getUserValue()==null)
//			return getScopeItem().getScopeType().getDefaultValue(getDescriptiveType());
//		return getUserValue();
//	}
	public void setUserValue(Object o) throws SQLException {
		super.setUserValue(""+o);
	}
//	public boolean isValue() throws SQLException {
//		String value = getValue();
//		return value!=null && value.length() > 0 && !value.equals("0") && !value.equalsIgnoreCase("false");
//	}
}
