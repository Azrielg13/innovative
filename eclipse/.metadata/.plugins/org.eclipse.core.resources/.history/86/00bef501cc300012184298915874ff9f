		if(isSame(dependencyId, getDependencyId()))return;
		int oldValue = getDependencyId();
		this.dependencyId=dependencyId;
		setProperty("DEPENDENCY_ID", dependencyId, oldValue);
	}
	@Column(name="UPGRADE_TYPE",nullable=true,length=2)
	public int getUpgradeType(){
		return upgradeType;
	}
	public void setUpgradeType(int upgradeType) throws java.sql.SQLException{
		if(isSame(upgradeType, getUpgradeType()))return;
		int oldValue = getUpgradeType();
		this.upgradeType=upgradeType;
		setProperty("UPGRADE_TYPE", upgradeType, oldValue);
	}
	@Column(name="COST_ALLOCATION",nullable=true,length=24)
	public double getCostAllocation(){
		return costAllocation;
	}
	public void setCostAllocation(double costAllocation) throws java.sql.SQLException{
		if(isSame(costAllocation, getCostAllocation()))return;
		double oldValue = getCostAllocation();
		this.costAllocation=costAllocation;
		setProperty("COST_ALLOCATION", costAllocation, oldValue);
	}
	@Column(name="LOCKED",nullable=true,length=1)
	public boolean isLocked(){
		return locked;
	}
	public void setLocked(boolean locked) throws java.sql.SQLException{
		if(isSame(locked, isLocked()))return;
		boolean oldValue = isLocked();
		this.locked=locked;
		setProperty("LOCKED", locked, oldValue);
	}
