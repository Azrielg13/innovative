		String oldValue = getProjDescription();
		this.projDescription=projDescription;
		setProperty("PROJ_DESCRIPTION", projDescription, oldValue);
	}
	@Column(name="PROJ_NUMBER",nullable=true,length=9)
	public int getProjNumber(){
		return projNumber;
	}
	public void setProjNumber(int projNumber) throws java.sql.SQLException{
		if(isSame(projNumber, getProjNumber()))return;
		int oldValue = getProjNumber();
		this.projNumber=projNumber;
		setProperty("PROJ_NUMBER", projNumber, oldValue);
	}
	@Column(name="PROJ_SIZE",nullable=true,length=24)
	public double getProjSize(){
		return projSize;
	}
	public void setProjSize(double projSize) throws java.sql.SQLException{
		if(isSame(projSize, getProjSize()))return;
		double oldValue = getProjSize();
		this.projSize=projSize;
		setProperty("PROJ_SIZE", projSize, oldValue);
	}
	@Column(name="PROJ_PHASE",nullable=true,length=2)
	public int getProjPhase(){
		return projPhase;
	}
	public void setProjPhase(int projPhase) throws java.sql.SQLException{
		if(isSame(projPhase, getProjPhase()))return;
		int oldValue = getProjPhase();
		this.projPhase=projPhase;
		setProperty("PROJ_PHASE", projPhase, oldValue);
	}
	@Column(name="PROJ_VERSION",nullable=true,length=9)
	public int getProjVersion(){
		return projVersion;
