public abstract class PortfolioGroupDAO extends DataAccessObject{
	public static enum KEY_PROPERTY{PORTFOLIO_ID,COST_ID};
	public static enum PROPERTY{PORTFOLIO_ID,COST_ID,CUSTOMER_ID,IREQ_ID,PROJ_DESCRIPTION,PROJ_NUMBER,PROJ_SIZE,PROJ_PHASE,PROJ_VERSION,DEPENDENCY_ID,UPGRADE_TYPE,COST_ALLOCATION,LOCKED,ALT_DOUBLE1,ALT_INT1,ALT_VARCHAR1,ALT_DATE1,INSERT_TS,INSERT_USER_ID,MODIFIED_T,MODIFIED_USER_ID,DELETED_TS,DELETED_USER_ID};
	public final static int PORTFOLIOID_LIMIT = 9;
	public final static int COSTID_LIMIT = 9;
	public final static int CUSTOMERID_LIMIT = 9;
	public final static int IREQID_LIMIT = 9;
	public final static int PROJDESCRIPTION_LIMIT = 512;
	public final static int PROJNUMBER_LIMIT = 9;
	public final static int PROJSIZE_LIMIT = 24;
	public final static int PROJPHASE_LIMIT = 2;
	public final static int PROJVERSION_LIMIT = 9;
	public final static int DEPENDENCYID_LIMIT = 9;
	public final static int UPGRADETYPE_LIMIT = 2;
	public final static int COSTALLOCATION_LIMIT = 24;
	public final static int LOCKED_LIMIT = 1;
	public final static int ALTDOUBLE1_LIMIT = 24;
	public final static int ALTINT1_LIMIT = 9;
	public final static int ALTVARCHAR1_LIMIT = 256;
	public final static int ALTDATE1_LIMIT = 7;
	public final static int MODIFIEDT_LIMIT = 7;
	private int portfolioId;
	private int costId;
	private int customerId;
	private int ireqId;
	private String projDescription;
	private int projNumber;
	private double projSize;
	private int projPhase;
	private int projVersion;
	private int dependencyId;
	private int upgradeType;
	private double costAllocation;
	private boolean locked;
	private double altDouble1;
	private int altInt1;
	private String altVarchar1;
	private Calendar altDate1;
	private Calendar modifiedT;
	private Portfolio portfolio;
	private SceProjElemCost sceProjElemCost;
	public static PortfolioGroup getInstance(int portfolioId, int costId)throws java.sql.SQLException{
		return getInstance(portfolioId, costId, true);
	}
	public static PortfolioGroup getInstance(int portfolioId, int costId, boolean fetch)throws java.sql.SQLException{
		if(isNull(portfolioId, costId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(portfolioId, costId);
		Cache cache = em.getEntityManagerFactory().getCache();
		PortfolioGroup o = null;
		if(cache != null && cache.contains(PortfolioGroup.class, pk))
			o = em.find(PortfolioGroup.class, pk);
		if(o==null && getAICache().contains(PortfolioGroup.class, pk))
			o = getAICache().find(PortfolioGroup.class, pk);
		if(o==null && fetch)
			o = em.find(PortfolioGroup.class, pk);
		if(o==null && fetch && PortfolioGroup.class.getAnnotation(AutoInsertable.class) != null){
			o = new PortfolioGroup(portfolioId, costId);
			getAICache().cache(PortfolioGroup.class, o);
		}
		return o;
	}
	public static Collection<PortfolioGroup> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<PortfolioGroup> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<PortfolioGroup> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM PortfolioGroup o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<PortfolioGroup> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<PortfolioGroup> tq = em.createQuery(jpql,PortfolioGroup.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<PortfolioGroup> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<PortfolioGroup> tq = em.createNamedQuery(name,PortfolioGroup.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public PortfolioGroupDAO(){}
	public PortfolioGroupDAO(int portfolioId, int costId){
		this.portfolioId=portfolioId;
		this.costId=costId;
	}
	public PortfolioGroupDAO(PortfolioGroupDAO orig){
		super(orig);
		this.portfolioId=orig.getPortfolioId();
		this.costId=orig.getCostId();
		copyFrom(orig);
	}
	public void copyFrom(PortfolioGroupDAO orig){
		this.customerId=orig.getCustomerId();
		this.ireqId=orig.getIreqId();
		this.projDescription=orig.getProjDescription();
		this.projNumber=orig.getProjNumber();
		this.projSize=orig.getProjSize();
		this.projPhase=orig.getProjPhase();
		this.projVersion=orig.getProjVersion();
		this.dependencyId=orig.getDependencyId();
		this.upgradeType=orig.getUpgradeType();
		this.costAllocation=orig.getCostAllocation();
		this.locked=orig.isLocked();
		this.altDouble1=orig.getAltDouble1();
		this.altInt1=orig.getAltInt1();
		this.altVarchar1=orig.getAltVarchar1();
		this.altDate1=orig.getAltDate1();
		this.modifiedT=orig.getModifiedT();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{portfolioId, costId};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	@Id
	@Column(name="PORTFOLIO_ID",nullable=false,length=9)
	public int getPortfolioId(){
		return portfolioId;
	}
	public void setPortfolioId(int portfolioId) throws java.sql.SQLException{
		if(isSame(portfolioId, getPortfolioId()))return;
		int oldValue = getPortfolioId();
		this.portfolioId=portfolioId;
		setProperty("PORTFOLIO_ID", portfolioId, oldValue);
		portfolio=null;
	}
	@Id
	@Column(name="COST_ID",nullable=false,length=9)
	public int getCostId(){
		return costId;
	}
	public void setCostId(int costId) throws java.sql.SQLException{
		if(isSame(costId, getCostId()))return;
		int oldValue = getCostId();
		this.costId=costId;
		setProperty("COST_ID", costId, oldValue);
		sceProjElemCost=null;
	}
	@Column(name="CUSTOMER_ID",nullable=true,length=9)
	public int getCustomerId(){
		return customerId;
	}
	public void setCustomerId(int customerId) throws java.sql.SQLException{
		if(isSame(customerId, getCustomerId()))return;
		int oldValue = getCustomerId();
		this.customerId=customerId;
		setProperty("CUSTOMER_ID", customerId, oldValue);
	}
	@Column(name="IREQ_ID",nullable=true,length=9)
	public int getIreqId(){
