		int oldValue = getIreqId();
		this.ireqId=ireqId;
		setProperty("IREQ_ID", ireqId, oldValue);
	}
	@Column(name="PROJ_DESCRIPTION",nullable=true,length=512)
	public String getProjDescription(){
		return projDescription;
	}
	public void setProjDescription(String projDescription) throws java.sql.SQLException{
