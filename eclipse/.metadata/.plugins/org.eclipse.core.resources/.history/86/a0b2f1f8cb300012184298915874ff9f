package com.sce.esp.object.model;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.TtResourceDAO;
@Entity
@Table(schema="MDI",name="MDIT02_TT_RESOURCE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM TtResource o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM TtResource o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM TtResource o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByTaskType", query="SELECT o FROM TtResource o WHERE o.TASK_TYPE_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIT02_TT_RESOURCE o WHERE o.ID=?"),//AUTO-GENERATED
})
public class TtResource extends TtResourceDAO{
	public TtResource(){
	}
	public TtResource(int id){
		super(id);
	}
	public TtResource(TtResource orig){
		super(orig);
	}
	@Override
	public void setTask(AbstractTask at) throws SQLException {
		setTaskType((TaskType)at);
	}
}
