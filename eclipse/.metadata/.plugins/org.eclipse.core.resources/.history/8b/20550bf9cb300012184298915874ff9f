/**
 * This Class is used specifically with the Task Object to identify the task of an object
 * through the code logic
 *  */

public enum SubTaskType{
	NEED_ID_STAGE(1001),
	PROJ_INIT_STAGE(1002),
	SITE_SEL_STAGE(1003),
	APP_PROD_STAGE(1004),
	PRE_CONST_PLAN_STAGE(1005),
	CONST_APP_STAGE(1006),
	PROJ_INIT_LIC(1007),
	LIC_LIC(1008),
	EHS_EHS(1009),
	RP_EXEC(1010),
	SUB_EXEC(1011),
	TRANS_EXEC(1012),
	PSC_EXEC(1013),
	ECS_EXEC(1014),
	TELECOM_EXEC(1015),
	CLOSE_OUT_EXEC(1016),
	PMWIF(1017);
	
	private int id;

	SubTaskType(int id){
		this.id = id;
	}
	public int getId(){
		return id;
	}
	}

