package com.sce.esp.object.model;
import java.sql.SQLException;

import com.sce.esp.object.dao.ChangeRequestDAO;
import com.sce.esp.object.jpa.AutoInsertable;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@AutoInsertable
@Table(schema="MDI",name="MDIS54_CHANGE_REQUEST")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM ChangeRequest o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM ChangeRequest o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM ChangeRequest o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS54_CHANGE_REQUEST o WHERE o.ID=?"),//AUTO-GENERATED
})
public class ChangeRequest extends ChangeRequestDAO{
	public ChangeRequest(){
	}
	public ChangeRequest(int id){
		super(id);
	}
	public ChangeRequest(ChangeRequest orig){
		super(orig);
	}
	public String getLookUpName(){
		try {
			return getLookUp()!=null ? getLookUp().getName() : "";
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		return "";
	}
}
