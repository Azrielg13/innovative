package com.sce.esp.object.model;
import com.sce.esp.object.dao.ProjCostRateDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDIU35_PROJ_COST_RATE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM ProjCostRate o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM ProjCostRate o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM ProjCostRate o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findBySceProjElemCost", query="SELECT o FROM ProjCostRate o WHERE o.SPEC_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByCostType", query="SELECT o FROM ProjCostRate o WHERE o.COST_TYPE_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIU35_PROJ_COST_RATE o WHERE o.ID=?"),//AUTO-GENERATED
})
public class ProjCostRate extends ProjCostRateDAO{
	public ProjCostRate(){
	}
	public ProjCostRate(int id){
		super(id);
	}
	public ProjCostRate(ProjCostRate orig){
		super(orig);
	}
}
