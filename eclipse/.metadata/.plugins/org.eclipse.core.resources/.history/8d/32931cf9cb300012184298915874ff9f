package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.ProjResourceEvalDAO;
@Entity
@Table(schema="MDI",name="MDIS50_PROJ_RESOURCE_EVAL")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM ProjResourceEval o WHERE o.RESOURCE_EVAL_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM ProjResourceEval o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM ProjResourceEval o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findBySceProjRev", query="SELECT o FROM ProjResourceEval o WHERE o.SCE_PROJ_REV_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS50_PROJ_RESOURCE_EVAL o WHERE o.RESOURCE_EVAL_ID=?"),//AUTO-GENERATED
})
public class ProjResourceEval extends ProjResourceEvalDAO implements Serializable{
	
	private static final long serialVersionUID = -7939759716327914833L;
	public static final int SUB = 1;
	public static final int TRANS = 2;
	public static final int SUBTRANS = 3;
	public static final int DIST = 4;
	public static final int IT = 5;
	

	public ProjResourceEval(){
	}
	public ProjResourceEval(ProjResourceEval orig) {
		super(orig);
	}

	public ProjResourceEval(int resourceEvalId) {
		super(resourceEvalId);
	}
		
	public String toString(){
		return getName();
	}
}
