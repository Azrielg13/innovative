package com.sce.esp.object.model;
import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.ProgElemDAO;
@Entity
@Table(schema="MDI",name="MDI540_PROG_ELEM")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM ProgElem o WHERE o.PLANYEAR=?1 AND o.PROG_ID=?2 AND o.ELEM_ID=?3"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM ProgElem o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM ProgElem o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByProg", query="SELECT o FROM ProgElem o WHERE o.PLANYEAR=?1 AND o.PROG_ID=?2 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI540_PROG_ELEM o WHERE o.PLANYEAR=? AND o.PROG_ID=? AND o.ELEM_ID=?"),//AUTO-GENERATED
})
public class ProgElem extends ProgElemDAO implements Serializable{
	private static final long serialVersionUID = 1L;
	public ProgElem(){
	}
	public ProgElem(int planyear, int progId, int elemId){
		super(planyear, progId, elemId);
	}
	public ProgElem(ProgElem orig){
		super(orig);
	}
}
