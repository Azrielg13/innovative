package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PifMajorCustTypeDAO;
@Entity
@Table(schema="MDI",name="MDI679_PIF_MAJOR_CUST_TYPE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PifMajorCustType o WHERE o.PLANYEAR=?1 AND o.PIF_MAJOR_CUST_TYPE_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PifMajorCustType o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PifMajorCustType o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI679_PIF_MAJOR_CUST_TYPE o WHERE o.PLANYEAR=? AND o.PIF_MAJOR_CUST_TYPE_ID=?"),//AUTO-GENERATED
})
public class PifMajorCustType extends PifMajorCustTypeDAO implements Serializable{

	private static final long serialVersionUID = 5395581137137947831L;

	@Override
	public String toString() {
		return getCustName();
	}

	public PifMajorCustType(){
	}
	public PifMajorCustType(int planyear, int pifMajorCustTypeId) {
		super(planyear, pifMajorCustTypeId);
	}

	public PifMajorCustType(PifMajorCustType pifMajorCustType) {
		super(pifMajorCustType);
	}

}
