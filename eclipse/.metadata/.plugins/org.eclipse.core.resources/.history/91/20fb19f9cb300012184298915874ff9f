package com.sce.esp.object.model;
import com.sce.esp.object.dao.MitigationDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDIV10_MITIGATION")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM Mitigation o WHERE o.MITIGATION_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM Mitigation o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM Mitigation o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIV10_MITIGATION o WHERE o.MITIGATION_ID=?"),//AUTO-GENERATED
})
public class Mitigation extends MitigationDAO{
	@Override
	public String toString() {
		return getType();
	}
	public Mitigation(){
	}
	public Mitigation(int mitigationId){
		super(mitigationId);
	}
	public Mitigation(Mitigation orig){
		super(orig);
	}
	
	public int getPlanyear() {
		// TODO Auto-generated method stub
		return 0;
	}
}
