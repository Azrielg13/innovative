package com.sce.esp.object.model;

import java.io.Serializable;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.CktEdnaReadDAO;
import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.util.FormatText;
@Entity
@AutoInsertable
@Table(schema="MDI",name="MDI245_CKT_EDNA_READ")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM CktEdnaRead o WHERE o.PLANYEAR=?1 AND o.CKT_ID=?2 AND o.PERIOD=?3"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM CktEdnaRead o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM CktEdnaRead o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI245_CKT_EDNA_READ o WHERE o.PLANYEAR=? AND o.CKT_ID=? AND o.PERIOD=?"),//AUTO-GENERATED
})
public class CktEdnaRead extends CktEdnaReadDAO implements Serializable{

	public CktEdnaRead(){
	}
	public CktEdnaRead(int planyear, int cktId, String period) {
		super(planyear, cktId, period);
	}

	public CktEdnaRead(CktEdnaRead orig) {
		super(orig);
	}
	public String toString(){
		try {
			return getCkt()+" "+getPeriod()+" "+FormatText.formatDate(getMaxTime())+" "+getMaxValue()+" "+FormatText.formatDate(getMinTime())+" "+getMinValue();
		} catch (SQLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		return super.toString();
	}
}
