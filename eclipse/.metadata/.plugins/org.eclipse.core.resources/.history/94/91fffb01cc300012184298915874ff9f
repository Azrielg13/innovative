	public BbankTransN1 getBbankTransN1()throws java.sql.SQLException{
		if(bbankTransN1==null)
			bbankTransN1=BbankTransN1.getInstance(getPlanyear(), getBbankSubId(), getTieId(), getYear());
		return bbankTransN1;
	}
	public void setBbankTransN1(BbankTransN1 bbankTransN1)throws java.sql.SQLException{
		setYear(bbankTransN1==null?0:bbankTransN1.getYear());
		this.bbankTransN1=bbankTransN1;
	}
	public Ckt getCkt()throws java.sql.SQLException{
		if(ckt==null)
			ckt=Ckt.getInstance(getPlanyear(), getFromCktId());
		return ckt;
	}
	public void setCkt(Ckt ckt)throws java.sql.SQLException{
		setFromCktId(ckt==null?0:ckt.getCktId());
		this.ckt=ckt;
	}
	public Ckt getCkt2()throws java.sql.SQLException{
		if(ckt2==null)
			ckt2=Ckt.getInstance(getPlanyear(), getToCktId());
