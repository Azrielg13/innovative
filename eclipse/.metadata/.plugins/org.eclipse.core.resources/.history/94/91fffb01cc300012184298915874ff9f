	public static CktTransN1 getInstance(int planyear, int transN1Id)throws java.sql.SQLException{
		return getInstance(planyear, transN1Id, true);
	}
	public static CktTransN1 getInstance(int planyear, int transN1Id, boolean fetch)throws java.sql.SQLException{
		if(isNull(planyear, transN1Id))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(planyear, transN1Id);
		Cache cache = em.getEntityManagerFactory().getCache();
		CktTransN1 o = null;
		if(cache != null && cache.contains(CktTransN1.class, pk))
			o = em.find(CktTransN1.class, pk);
		if(o==null && getAICache().contains(CktTransN1.class, pk))
			o = getAICache().find(CktTransN1.class, pk);
		if(o==null && fetch)
			o = em.find(CktTransN1.class, pk);
		if(o==null && fetch && CktTransN1.class.getAnnotation(AutoInsertable.class) != null){
			o = new CktTransN1(planyear, transN1Id);
			getAICache().cache(CktTransN1.class, o);
		}
		return o;
	}
	public static Collection<CktTransN1> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<CktTransN1> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<CktTransN1> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM CktTransN1 o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<CktTransN1> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<CktTransN1> tq = em.createQuery(jpql,CktTransN1.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<CktTransN1> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<CktTransN1> tq = em.createNamedQuery(name,CktTransN1.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public CktTransN1DAO(){}
	public CktTransN1DAO(int planyear, int transN1Id){
		this.planyear=planyear;
		this.transN1Id=transN1Id;
	}
	public CktTransN1DAO(CktTransN1DAO orig){
		super(orig);
		this.planyear=orig.getPlanyear();
		copyFrom(orig);
	}
	public void copyFrom(CktTransN1DAO orig){
		this.bbankSubId=orig.getBbankSubId();
		this.tieId=orig.getTieId();
		this.year=orig.getYear();
		this.step=orig.getStep();
		this.fromCktId=orig.getFromCktId();
		this.toCktId=orig.getToCktId();
		this.cswtTypeId=orig.getCswtTypeId();
		this.cswtNum=orig.getCswtNum();
		this.cswtPos=orig.getCswtPos();
		this.oswtTypeId=orig.getOswtTypeId();
		this.oswtNum=orig.getOswtNum();
		this.oswtPos=orig.getOswtPos();
		this.transfer=orig.getTransfer();
		this.actualTrans=orig.getActualTrans();
		this.transDate=orig.getTransDate();
		this.note=orig.getNote();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{planyear, transN1Id};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	@Id
	@Column(name="PLANYEAR",nullable=false,length=4)
	public int getPlanyear(){
		return planyear;
	}
	public void setPlanyear(int planyear) throws java.sql.SQLException{
		if(isSame(planyear, getPlanyear()))return;
		int oldValue = getPlanyear();
		this.planyear=planyear;
		setProperty("PLANYEAR", planyear, oldValue);
	}
	@Id
