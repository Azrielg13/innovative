package com.sce.esp.object.model;
import java.sql.SQLException;
import java.util.Calendar;

import com.sce.esp.object.dao.OverheadLoaderRateDAO;
import com.sce.esp.util.Calculate;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDIU75_OVERHEAD_LOADER_RATE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM OverheadLoaderRate o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM OverheadLoaderRate o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM OverheadLoaderRate o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByCostSheetOH", query="SELECT o FROM OverheadLoaderRate o WHERE o.COST_SHEET_O_H_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIU75_OVERHEAD_LOADER_RATE o WHERE o.ID=?"),//AUTO-GENERATED
})
public class OverheadLoaderRate extends OverheadLoaderRateDAO{
	public OverheadLoaderRate(){
	}
	public OverheadLoaderRate(int id){
		super(id);
	}
	public OverheadLoaderRate(OverheadLoaderRate orig){
		super(orig);
	}	
}
