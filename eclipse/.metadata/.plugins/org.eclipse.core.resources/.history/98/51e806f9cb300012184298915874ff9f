package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PifImpactTypeDAO;
@Entity
@Table(schema="MDI",name="MDI677_PIF_IMPACT_TYPE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PifImpactType o WHERE o.PLANYEAR=?1 AND o.PIF_IMPACT_TYPE_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PifImpactType o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PifImpactType o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI677_PIF_IMPACT_TYPE o WHERE o.PLANYEAR=? AND o.PIF_IMPACT_TYPE_ID=?"),//AUTO-GENERATED
})
public class PifImpactType extends PifImpactTypeDAO implements Serializable{
	private static final long serialVersionUID = 2762328076155941369L;
	public static final  int RELIABILITY = 1;
	public static final  int MAINTAINABILITY = 2;
	public static final  int SAFETY = 3;
	public static final  int COMMUNITY = 4;
	public static final  int GOODWORK = 5;

	@Override
	public String toString() {
		return getImpactTypeName();
	}

	public PifImpactType(){
	}
	public PifImpactType(int planyear, int pifImpactTypeId) {
		super(planyear, pifImpactTypeId);
	}

	public PifImpactType(PifImpactType pifImpactType) {
		super(pifImpactType);
	}

}
