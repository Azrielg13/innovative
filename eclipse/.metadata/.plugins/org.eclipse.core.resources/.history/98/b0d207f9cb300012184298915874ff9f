package com.sce.esp.object.model;
import com.sce.esp.object.dao.UnitCostDetailYearDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDIU35_UNIT_COST_DETAIL_YEAR")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM UnitCostDetailYear o WHERE o.UNIT_COST_DETAIL_ID=?1 AND o.OFFSET_YEAR=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM UnitCostDetailYear o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM UnitCostDetailYear o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByUnitCostDetail", query="SELECT o FROM UnitCostDetailYear o WHERE o.UNIT_COST_DETAIL_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIU35_UNIT_COST_DETAIL_YEAR o WHERE o.UNIT_COST_DETAIL_ID=? AND o.OFFSET_YEAR=?"),//AUTO-GENERATED
})
public class UnitCostDetailYear extends UnitCostDetailYearDAO{
	public UnitCostDetailYear(){
	}
	public UnitCostDetailYear(int unitCostDetailId, int offsetYear){
		super(unitCostDetailId, offsetYear);
	}
	public UnitCostDetailYear(UnitCostDetailYear orig){
		super(orig);
	}
}
