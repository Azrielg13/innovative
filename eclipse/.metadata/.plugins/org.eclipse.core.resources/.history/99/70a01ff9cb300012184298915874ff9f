package com.sce.esp.object.model;

import java.io.Serializable;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.SubContShedDAO;
@Entity
@Table(schema="MDI",name="MDI330_SUB_CONT_SHED")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM SubContShed o WHERE o.PLANYEAR=?1 AND o.SHED_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM SubContShed o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM SubContShed o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findBySubCont", query="SELECT o FROM SubContShed o WHERE o.PLANYEAR=?1 AND o.SUB_ID=?2 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI330_SUB_CONT_SHED o WHERE o.PLANYEAR=? AND o.SHED_ID=?"),//AUTO-GENERATED
})
public class SubContShed extends SubContShedDAO implements Comparable<Object>, Serializable {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 4561036568885994822L;
	/**
	 * The Constructor.
	 * 
	 * @param planyear the planyear
	 */
	public SubContShed(){
	}
	public SubContShed(int planyear){
		super(planyear,0);
	}
	
	/**
	 * The Constructor.
	 * 
	 * @param planyear the planyear
	 * @param shedId the shed id
	 */
	public SubContShed(int planyear, int shedId){
		super(planyear,shedId);
	}
	
	public SubContShed(SubContShed subContShed) {
		super(subContShed);
	}

	/**
	 * Gets the O ckt swt.
	 * 
	 * @return the O ckt swt
	 * 
	 * @throws SQLException the SQL exception
	 */
	public CktSwt getOCktSwt()throws SQLException{
		return getCktSwt();
	}

	public Sub getSub() throws SQLException {
		return Sub.getInstance(getPlanyear(), getSubId());
	}
	/* (non-Javadoc)
	 * @see java.lang.Comparable#compareTo(java.lang.Object)
	 */
	public int compareTo(Object o) {
		SubContShed usc = (SubContShed)o;
		
		try{
			if(getCont().getContCode()<usc.getCont().getContCode())
				return -1;
			if(getCont().getContCode()>usc.getCont().getContCode())
				return 1;
			}catch(SQLException e){
				//continue
			}
		
		if(getStep()<usc.getStep())
			return -1;
		if(getStep()>usc.getStep())
			return 1;
		
		return super.compareTo(o);
	}
}
