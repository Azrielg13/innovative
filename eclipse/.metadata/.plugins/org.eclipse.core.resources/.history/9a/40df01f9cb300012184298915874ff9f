package com.sce.esp.object.model;
import com.sce.esp.object.dao.TspocRefListDAO;
import com.sce.esp.object.jpa.AutoInsertable;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@AutoInsertable
@Table(schema="MDI",name="MDIS52_TSPOC_REF_LIST")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM TspocRefList o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM TspocRefList o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM TspocRefList o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS52_TSPOC_REF_LIST o WHERE o.ID=?"),//AUTO-GENERATED
})
public class TspocRefList extends TspocRefListDAO{
	public final static int PROJ_INIT = 1;
	public final static int SITE_ROUTE_SEL = 2;
	public final static int APPLIC_PROD = 3;
	public final static int PRE_CON_PLAN = 4;
	public final static int APP_FOR_CON = 5;
	
	public TspocRefList(){
	}
	public TspocRefList(int id){
		super(id);
	}
	public TspocRefList(TspocRefList orig){
		super(orig);
	}
	public String toString() {
		return getName();
	}
}
