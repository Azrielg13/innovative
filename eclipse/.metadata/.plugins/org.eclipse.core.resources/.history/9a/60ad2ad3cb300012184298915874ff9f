package com.sce.esp.hst;

import com.sce.esp.object.model.Sub;


/**
 * Substation Heat Storm Tracker Object is used for displaying real-time
 * values for the substation object since the information cannot
 * be stored in a <code>SubDaily</code> object.
 * 
 * @author Distribution Staff Engineering
 */
public class RealTimeSub extends RealTimeObject {

	/**
	 * 
	 * @param sub the <code>Sub</code>
	 * @param date the date
	 */
	public RealTimeSub(Sub sub){
		super(sub);
	}

	/**
	 * Returns the sub
	 * @return The <code>Sub</code>
	 */
	public Sub getSub(){
		return (Sub)getAe();
	}
	
}