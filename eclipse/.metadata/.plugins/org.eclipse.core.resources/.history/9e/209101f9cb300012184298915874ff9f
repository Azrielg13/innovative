package com.sce.esp.object.model;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PqVehicleDAO;
import com.sce.esp.object.jpa.EntityManagerHelper;
import com.sce.esp.util.ESPProperties;
@Entity
@Table(schema="MDI",name="MDI988_PQ_VEHICLE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PqVehicle o WHERE o.VEHICLE_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PqVehicle o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PqVehicle o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI988_PQ_VEHICLE o WHERE o.VEHICLE_ID=?"),//AUTO-GENERATED
})
public class PqVehicle extends PqVehicleDAO{
	public PqVehicle(){
	}
	public PqVehicle(int vehicleId){
		super(vehicleId);
	}
	public PqVehicle(PqVehicle orig){
		super(orig);
	}
	public PqVehicle(int vehicleNum, int vehicleType, int userId) throws SQLException{
		super(vehicleNum, vehicleType, userId);
	}
	
	@Override
	public int getPlanyear() {
		// TODO Auto-generated method stub
		return 0;
	}
	
	public static void insertBasic(){
		try {
			new PqVehicle(28708, 1, 176);
			new PqVehicle(28784, 1, 798);
			new PqVehicle(28532, 1, 108);
			new PqVehicle(28785, 1, 122);
			new PqVehicle(28709, 1, 110);
			new PqVehicle(28782, 1, 738);
			new PqVehicle(28783, 1, 104);
		} catch (SQLException e) {
			e.printStackTrace();
		}
	}
	
	public static void main(String[] args){
		try {
			EntityManagerHelper.init(ESPProperties.getInstance().getGusDbUrl(), "suwannn", "edison");
			com.jidesoft.utils.Lm.verifyLicense("Southern California Edison", "MDI", "JsG5JpcDR.eLtDhvTX04TKrH.z:KrM71");
			
			insertBasic();
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
		
}
