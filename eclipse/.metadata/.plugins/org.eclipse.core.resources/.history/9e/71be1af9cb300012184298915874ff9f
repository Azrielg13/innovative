package com.sce.esp.object.model;

import java.io.Serializable;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PifCktJustDAO;
@Entity
@Table(schema="MDI",name="MDI685_PIF_CKT_JUST")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PifCktJust o WHERE o.PLANYEAR=?1 AND o.PIF_ID=?2 AND o.CKT_ID=?3"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PifCktJust o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PifCktJust o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByPif", query="SELECT o FROM PifCktJust o WHERE o.PLANYEAR=?1 AND o.PIF_ID=?2 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI685_PIF_CKT_JUST o WHERE o.PLANYEAR=? AND o.PIF_ID=? AND o.CKT_ID=?"),//AUTO-GENERATED
})
public class PifCktJust extends PifCktJustDAO implements Serializable{
	
	private static final long serialVersionUID = 2892303152153840196L;
	
	/**
	 * Instantiates a new pif ckt just.
	 * 
	 * @param planyear the planyear
	 * @param pifId the pif id
	 * @param cktId the ckt id
	 */
	public PifCktJust(){
	}
	public PifCktJust(int planyear, int pifId, int cktId){
		super(planyear,pifId,cktId);
	}
	
	public PifCktJust(PifCktJust pifCktJust) {
		super(pifCktJust);
	}

	/**
	 * Gets the before util.
	 * 
	 * @return the before util
	 */
	public double getBeforeUtil(){
		if(getBeforePll()==0)
			return 0;
		return getBeforeLoad()/getBeforePll();
	}
	
	/**
	 * Gets the after util.
	 * 
	 * @return the after util
	 */
	public double getAfterUtil(){
		if(getAfterPll()==0)
			return 0;
		return getAfterLoad()/getAfterPll();
	}
	
	/**
	 * Gets the b bank.
	 * 
	 * @return the b bank
	 * 
	 * @throws SQLException the SQL exception
	 */
	public Bbank getBbank() throws SQLException{
		if(getCkt()!=null)
			return getCkt().getBbank(0);
		return null;
	}
}


