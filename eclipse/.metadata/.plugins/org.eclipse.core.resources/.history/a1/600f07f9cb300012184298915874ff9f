package com.sce.esp.object.model;
import com.sce.esp.object.dao.PortfolioProjDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDIS34_PORTFOLIO_PROJ")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PortfolioProj o WHERE o.PORTFOLIO_ID=?1 AND o.PLAN_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PortfolioProj o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PortfolioProj o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findBySceProj", query="SELECT o FROM PortfolioProj o WHERE o.SCE_PROJ_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS34_PORTFOLIO_PROJ o WHERE o.PORTFOLIO_ID=? AND o.PLAN_ID=?"),//AUTO-GENERATED
})
public class PortfolioProj extends PortfolioProjDAO{
	public PortfolioProj(){
	}
	public PortfolioProj(int portfolioId, int planId){
		super(portfolioId, planId);
	}
	public PortfolioProj(PortfolioProj orig){
		super(orig);
	}
}
