package com.sce.esp.object.model;

import java.io.Serializable;
import java.sql.SQLException;
import java.util.Calendar;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.UserSysDAO;
@Entity
@Table(schema="MDI",name="MDI490_USER_SYS")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM UserSys o WHERE o.PLANYEAR=?1 AND o.USERNAME_ID=?2 AND o.SYS_ID=?3"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM UserSys o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM UserSys o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByUser", query="SELECT o FROM UserSys o WHERE o.USERNAME_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI490_USER_SYS o WHERE o.PLANYEAR=? AND o.USERNAME_ID=? AND o.SYS_ID=?"),//AUTO-GENERATED
})
public class UserSys extends UserSysDAO implements Serializable{
	
	private static final long serialVersionUID = 1999692015582732625L;

	/**
	 * The Constructor.
	 * 
	 * @param planyear the planyear
	 */
	public UserSys(){
	}
	public UserSys(int planyear) {
		super(planyear, 0, 0);
	}
	
	/**
	 * The Constructor.
	 * 
	 * @param sysId the sys id
	 * @param usernameId the username id
	 * @param planyear the planyear
	 */
	public UserSys(int planyear, int usernameId, int sysId){
		super(planyear,usernameId,sysId);
	}

	public UserSys(UserSys userSys) {
		super(userSys);
	}
	
	public void delete() {
		try {
			deleteDB();
		} catch (SQLException e) {
			e.printStackTrace();
		}
	}
}
