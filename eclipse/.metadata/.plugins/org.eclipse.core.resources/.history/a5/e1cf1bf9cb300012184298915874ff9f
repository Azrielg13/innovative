package com.sce.esp.object.model;
import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PqEquipmentDAO;
@Entity
@Table(schema="MDI",name="MDI950_PQ_EQUIPMENT")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PqEquipment o WHERE o.EQUIP_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PqEquipment o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PqEquipment o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI950_PQ_EQUIPMENT o WHERE o.EQUIP_ID=?"),//AUTO-GENERATED
})
public class PqEquipment extends PqEquipmentDAO implements Serializable{

	private static final long serialVersionUID = 246097643623733296L;
	public PqEquipment(){
	}
	public PqEquipment(int equipId){
		super(equipId);
	}

	public PqEquipment(PqEquipment pqEquipment) {
		super(pqEquipment);
	}
	
	/**
	 * String representation for combo boxes.
	 * 
	 * @return the string
	 */
	public String toString(){
		return getEquipment();
	}
	public int getPlanyear(){
    	return Department.getActivePlanYear();
    }
}
