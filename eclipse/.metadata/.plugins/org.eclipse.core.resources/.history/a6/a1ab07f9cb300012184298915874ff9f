package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PifPriorityDAO;
@Entity
@Table(schema="MDI",name="MDI630_PIF_PRIORITY")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PifPriority o WHERE o.PLANYEAR=?1 AND o.PRIORITY_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PifPriority o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PifPriority o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByDepartment", query="SELECT o FROM PifPriority o WHERE o.PLANYEAR=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI630_PIF_PRIORITY o WHERE o.PLANYEAR=? AND o.PRIORITY_ID=?"),//AUTO-GENERATED
})
public class PifPriority extends PifPriorityDAO implements Serializable{

	private static final long serialVersionUID = -502175165157470930L;
	public PifPriority(){
	}
	public PifPriority(int planyear, int priorityId){
		super(planyear,priorityId);
	}
	public PifPriority(int planyear) {
		super(planyear,0);
	}
	public PifPriority(PifPriority pifPriority) {
		super(pifPriority);
	}
	/**
	 * To string.
	 * 
	 * @return the string
	 */
	public String toString() {
		if(getPriorityId()==0)
			return "";
		return getPriority()+" - "+getTitle();
	}
}
