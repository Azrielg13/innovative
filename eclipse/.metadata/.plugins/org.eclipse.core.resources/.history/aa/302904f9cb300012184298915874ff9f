package com.sce.esp.object.model;

import java.io.Serializable;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.ProjElemCostYearDAO;
import com.sce.esp.object.jpa.AutoInsertable;
@Entity
@AutoInsertable
@Table(schema="MDI",name="MDI565_PROJ_ELEM_COST_YEAR")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM ProjElemCostYear o WHERE o.PLANYEAR=?1 AND o.SIM_ID=?2 AND o.COST_ID=?3 AND o.YEAR=?4"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM ProjElemCostYear o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM ProjElemCostYear o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI565_PROJ_ELEM_COST_YEAR o WHERE o.PLANYEAR=? AND o.SIM_ID=? AND o.COST_ID=? AND o.YEAR=?"),//AUTO-GENERATED
})
public class ProjElemCostYear extends ProjElemCostYearDAO implements Serializable{
	
	private static final long serialVersionUID = 1731728724150513041L;

	/**
	 * The Constructor.
	 * 
	 * @param planyear the planyear
	 * @param simId the sim id
	 * @param year the year
	 * @param costId the cost id
	 */
	public ProjElemCostYear(){
	}
	public ProjElemCostYear(int planyear, int simId, int costId, int year){
		super(planyear,simId,costId,year);
	}
	
	public ProjElemCostYear(ProjElemCostYear projElemCostYear) {
		super(projElemCostYear);
	}

	/**
	 * @deprecated use new class
	 * @see com.sce.iwp.util.Proj2SceProj
	 * 
	 * @return an <code>SceProjElemCostYear</code> object.
	 * @throws SQLException
	 */
	public SceProjElemCostYear genSceProjElemCostYear() throws SQLException {
		SceProjElemCostYear specy = new SceProjElemCostYear(0,getYear());
		specy.setCost(getCost()*1000);
		return specy;
	}
}
