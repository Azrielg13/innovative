package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PqUseEquipDAO;
@Entity
@Table(schema="MDI",name="MDI985_PQ_USE_EQUIP")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PqUseEquip o WHERE o.PQ_ID=?1 AND o.EQUIP_ID=?2 AND o.EQUIP_SERIAL=?3"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PqUseEquip o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PqUseEquip o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByPqTroubleLog", query="SELECT o FROM PqUseEquip o WHERE o.PQ_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI985_PQ_USE_EQUIP o WHERE o.PQ_ID=? AND o.EQUIP_ID=? AND o.EQUIP_SERIAL=?"),//AUTO-GENERATED
})
public class PqUseEquip extends PqUseEquipDAO implements Serializable{

	private static final long serialVersionUID = 5437598184695995650L;
	
	public PqUseEquip(){
	}
	public PqUseEquip(int pqId, int equipId, String equipSerial) {
		super(pqId, equipId, equipSerial);
	}
	public PqUseEquip(PqUseEquip pqUseEquip) {
		super(pqUseEquip);
	}
	public String toString() {
		try {
			if(getPqEquipment()!=null)	return getPqEquipment().toString();
		} catch (Exception e) {
			e.printStackTrace();
		}
		return null;
	}
	public int getPlanyear(){
    	return Department.getActivePlanYear();
    }
}
