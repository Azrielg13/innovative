package com.sce.esp.component;

import java.awt.Color;

import javax.swing.Icon;

import com.jidesoft.grid.DefaultExpandableRow;
import com.jidesoft.swing.StyleRange;
import com.sce.esp.util.ESPFont;

public abstract class ESPExpandableRow extends DefaultExpandableRow {

	/**
	 * The Constructor.
	 */
	public ESPExpandableRow() {
	}

	/**
	 * Gets the font.
	 * 
	 * @param col
	 *            the col
	 * 
	 * @return the font
	 */
	public ESPFont getFont(int col) {
		return ESPFont.DEFAULT;
	}

	/**
	 * Gets the foreground.
	 * 
	 * @param col
	 *            the col
	 * 
	 * @return the foreground
	 */
	public Color getForeground(int col) {
		return null;
	}

	/**
	 * Gets the background.
	 * 
	 * @param col
	 *            the col
	 * 
	 * @return the background
	 */
	public Color getBackground(int col) {
		return null;
	}

	/**
	 * Gets the icon.
	 * 
	 * @param col
	 *            the col
	 * 
	 * @return the icon
	 */
	public Icon getIcon(int col) {
		return null;
	}

	/**
	 * Gets the tool tip.
	 * 
	 * @param col
	 *            the col
	 * 
	 * @return the tool tip
	 */
	public String getToolTip(int col) {
		return null;
	}

	/**
	 * Gets the style.
	 * 
	 * @param col
	 *            the col
	 * 
	 * @return the style
	 */
	public StyleRange getStyle(int col) {
		return null;
	}
}
