package com.sce.esp.object.model;
import com.sce.esp.object.dao.PortGroupAreaDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDIS34_PORT_GROUP_AREA")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PortGroupArea o WHERE o.AREA_ID=?1 AND o.PORTFOLIO_ID=?2 AND o.COST_ID=?3"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PortGroupArea o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PortGroupArea o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByPortfolio", query="SELECT o FROM PortGroupArea o WHERE o.PORTFOLIO_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findBySceProjElemCost", query="SELECT o FROM PortGroupArea o WHERE o.COST_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS34_PORT_GROUP_AREA o WHERE o.AREA_ID=? AND o.PORTFOLIO_ID=? AND o.COST_ID=?"),//AUTO-GENERATED
})
public class PortGroupArea extends PortGroupAreaDAO{
	public PortGroupArea(){
	}
	public PortGroupArea(int areaId, int portfolioId, int costId){
		super(areaId, portfolioId, costId);
	}
	public PortGroupArea(PortGroupArea orig){
		super(orig);
	}
}
