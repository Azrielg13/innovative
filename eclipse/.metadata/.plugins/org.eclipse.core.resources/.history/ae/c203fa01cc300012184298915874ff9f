/**Description of class, (we need to get this from somewhere, database? xml?)*/
import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.object.jpa.EntityManagerHelper;
import com.sce.esp.object.jpa.PrimaryKey;
import com.sce.esp.object.model.TrackingDefault;
import com.sce.esp.object.model.TrackingDepDefault;
import java.util.Calendar;
import java.util.Collection;
import java.util.Vector;
import javax.persistence.Cache;
import javax.persistence.Column;
import javax.persistence.EntityManager;
import javax.persistence.Id;
import javax.persistence.TypedQuery;
public abstract class TrackingDepDefaultDAO extends DataAccessObject{
	public static enum KEY_PROPERTY{T_ID,DEP_T_ID};
	public static enum PROPERTY{T_ID,DEP_T_ID,ALT_DOUBLE1,ALT_VARCHAR1,ALT_DATE1};
	public final static int TID_LIMIT = 9;
	public final static int DEPTID_LIMIT = 9;
	public final static int ALTDOUBLE1_LIMIT = 24;
	public final static int ALTVARCHAR1_LIMIT = 256;
	public final static int ALTDATE1_LIMIT = 7;
	private int tId;
	private int depTId;
	private double altDouble1;
	private String altVarchar1;
	private Calendar altDate1;
	private TrackingDefault trackingDefault;
	private TrackingDefault trackingDefault2;
	public static TrackingDepDefault getInstance(int tId, int depTId)throws java.sql.SQLException{
		return getInstance(tId, depTId, true);
	}
	public static TrackingDepDefault getInstance(int tId, int depTId, boolean fetch)throws java.sql.SQLException{
		if(isNull(tId, depTId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(tId, depTId);
		Cache cache = em.getEntityManagerFactory().getCache();
		TrackingDepDefault o = null;
		if(cache != null && cache.contains(TrackingDepDefault.class, pk))
			o = em.find(TrackingDepDefault.class, pk);
		if(o==null && getAICache().contains(TrackingDepDefault.class, pk))
			o = getAICache().find(TrackingDepDefault.class, pk);
		if(o==null && fetch)
			o = em.find(TrackingDepDefault.class, pk);
		if(o==null && fetch && TrackingDepDefault.class.getAnnotation(AutoInsertable.class) != null){
			o = new TrackingDepDefault(tId, depTId);
			getAICache().cache(TrackingDepDefault.class, o);
		}
		return o;
	}
	public static Collection<TrackingDepDefault> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<TrackingDepDefault> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<TrackingDepDefault> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM TrackingDepDefault o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<TrackingDepDefault> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<TrackingDepDefault> tq = em.createQuery(jpql,TrackingDepDefault.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<TrackingDepDefault> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<TrackingDepDefault> tq = em.createNamedQuery(name,TrackingDepDefault.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public TrackingDepDefaultDAO(){}
	public TrackingDepDefaultDAO(int tId, int depTId){
		this.tId=tId;
		this.depTId=depTId;
	}
	public TrackingDepDefaultDAO(TrackingDepDefaultDAO orig){
		super(orig);
		this.tId=orig.getTId();
		this.depTId=orig.getDepTId();
		copyFrom(orig);
	}
	public void copyFrom(TrackingDepDefaultDAO orig){
		this.altDouble1=orig.getAltDouble1();
		this.altVarchar1=orig.getAltVarchar1();
		this.altDate1=orig.getAltDate1();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{tId, depTId};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	@Id
	@Column(name="T_ID",nullable=false,length=9)
	public int getTId(){
		return tId;
	}
	public void setTId(int tId) throws java.sql.SQLException{
		if(isSame(tId, getTId()))return;
		int oldValue = getTId();
		this.tId=tId;
		setProperty("T_ID", tId, oldValue);
		trackingDefault=null;
	}
	@Id
	@Column(name="DEP_T_ID",nullable=false,length=9)
	public int getDepTId(){
		return depTId;
	}
	public void setDepTId(int depTId) throws java.sql.SQLException{
		if(isSame(depTId, getDepTId()))return;
		int oldValue = getDepTId();
		this.depTId=depTId;
		setProperty("DEP_T_ID", depTId, oldValue);
		trackingDefault2=null;
	}
	@Column(name="ALT_DOUBLE1",nullable=true,length=24)
	public double getAltDouble1(){
		return altDouble1;
	}
	public void setAltDouble1(double altDouble1) throws java.sql.SQLException{
		if(isSame(altDouble1, getAltDouble1()))return;
		double oldValue = getAltDouble1();
		this.altDouble1=altDouble1;
		setProperty("ALT_DOUBLE1", altDouble1, oldValue);
	}
	@Column(name="ALT_VARCHAR1",nullable=true,length=256)
	public String getAltVarchar1(){
		return altVarchar1;
	}
	public void setAltVarchar1(String altVarchar1) throws java.sql.SQLException{
		if(isSame(altVarchar1, getAltVarchar1()))return;
		String oldValue = getAltVarchar1();
		this.altVarchar1=altVarchar1;
		setProperty("ALT_VARCHAR1", altVarchar1, oldValue);
	}
	@Column(name="ALT_DATE1",nullable=true,length=7)
	public Calendar getAltDate1(){
		return altDate1;
	}
	public void setAltDate1(Calendar altDate1) throws java.sql.SQLException{
		if(isSame(altDate1, getAltDate1()))return;
		Calendar oldValue = getAltDate1();
		this.altDate1=altDate1;
		setProperty("ALT_DATE1", altDate1, oldValue);
	}
	public TrackingDefault getTrackingDefault()throws java.sql.SQLException{
		if(trackingDefault==null)
			trackingDefault=TrackingDefault.getInstance(getTId());
		return trackingDefault;
	}
	public void setTrackingDefault(TrackingDefault trackingDefault)throws java.sql.SQLException{
		setTId(trackingDefault==null?0:trackingDefault.getTId());
		this.trackingDefault=trackingDefault;
	}
	public TrackingDefault getTrackingDefault2()throws java.sql.SQLException{
		if(trackingDefault2==null)
			trackingDefault2=TrackingDefault.getInstance(getDepTId());
		return trackingDefault2;
	}
	public void setTrackingDefault2(TrackingDefault trackingDefault2)throws java.sql.SQLException{
		setDepTId(trackingDefault2==null?0:trackingDefault2.getTId());
		this.trackingDefault2=trackingDefault2;
	}
	public TrackingDepDefault copy()throws java.sql.SQLException{
		TrackingDepDefault cp = new TrackingDepDefault((TrackingDepDefault)this);
		copyChildrenTo(cp);
		return cp;
	}
	public void copyChildrenTo(TrackingDepDefaultDAO cp)throws java.sql.SQLException{
		super.copyChildrenTo(cp);
	}
	public Vector<String> getDifference(TrackingDepDefaultDAO o){
		Vector<String> diffs = super.getDifference(o);
		if(!isSame(getTId(),o.getTId())) diffs.add("T_ID");
		if(!isSame(getDepTId(),o.getDepTId())) diffs.add("DEP_T_ID");
		if(!isSame(getAltDouble1(),o.getAltDouble1())) diffs.add("ALT_DOUBLE1");
		if(!isSame(getAltVarchar1(),o.getAltVarchar1())) diffs.add("ALT_VARCHAR1");
		if(!isSame(getAltDate1(),o.getAltDate1())) diffs.add("ALT_DATE1");
		return diffs;
	}
	public void insertChildren()throws java.sql.SQLException{
	}
}
