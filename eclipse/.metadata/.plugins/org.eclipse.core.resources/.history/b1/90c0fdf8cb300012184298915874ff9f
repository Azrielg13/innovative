package com.sce.esp.object.model;
import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.SysYearCostDAO;
@Entity
@Table(schema="MDI",name="MDI545_SYS_YEAR_COST")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM SysYearCost o WHERE o.PLANYEAR=?1 AND o.SIM_ID=?2 AND o.SYS_ID=?3 AND o.YEAR=?4 AND o.PROG_ID=?5 AND o.ELEM_ID=?6"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM SysYearCost o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM SysYearCost o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI545_SYS_YEAR_COST o WHERE o.PLANYEAR=? AND o.SIM_ID=? AND o.SYS_ID=? AND o.YEAR=? AND o.PROG_ID=? AND o.ELEM_ID=?"),//AUTO-GENERATED
})
public class SysYearCost extends SysYearCostDAO implements Serializable{
	private static final long serialVersionUID = -9120462248008651506L;
	public SysYearCost(){
	}
	public SysYearCost(int planyear, int simId, int sysId, int year, int progId, int elemId){
		super(planyear, simId, sysId, year, progId, elemId);
	}
	public SysYearCost(SysYearCost orig){
		super(orig);
	}
}
