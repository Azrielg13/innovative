package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.SceSimAltDAO;
@Entity
@Table(schema="MDI",name="MDIS42_SCE_SIM_ALT")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM SceSimAlt o WHERE o.PLANYEAR=?1 AND o.SIM_ID=?2 AND o.SCE_PROJ_ID=?3 AND o.ALT_SIM_ID=?4 AND o.ALT_SCE_PROJ_ID=?5"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM SceSimAlt o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM SceSimAlt o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findBySceProj", query="SELECT o FROM SceSimAlt o WHERE o.SCE_PROJ_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS42_SCE_SIM_ALT o WHERE o.PLANYEAR=? AND o.SIM_ID=? AND o.SCE_PROJ_ID=? AND o.ALT_SIM_ID=? AND o.ALT_SCE_PROJ_ID=?"),//AUTO-GENERATED
})
public class SceSimAlt extends SceSimAltDAO implements Serializable{

	private static final long serialVersionUID = 3951354905943200173L;

	public SceSimAlt(){
	}
	public SceSimAlt(int planyear, int simId, int sceProjId, int altSimId, int altSceProjId) {
		super(planyear, simId, sceProjId, altSimId, altSceProjId);
	}

	public SceSimAlt(SceSimAlt sceSimAlt) {
		super(sceSimAlt);
	}

}
