package com.sce.esp.component;

import javax.swing.table.TableCellRenderer;

import com.jidesoft.grid.TreeTableCellRenderer;

/**
 * The Class MDITreeTableCellRenderer.
 * 
 * @author Distribution Staff Engineering
 */
public class ESPTreeTableCellRenderer extends TreeTableCellRenderer {
	/** The renderer. */
	private static ESPTableCellRenderer renderer = new ESPTableCellRenderer();

	/*
	 * (non-Javadoc)
	 * 
	 * @see com.jidesoft.grid.TreeTableCellRenderer#getActualCellRenderer()
	 */
	public TableCellRenderer getActualCellRenderer() {
		return renderer;
	}
}
