package com.sce.esp.object.model;
import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PqCustClassDAO;
@Entity
@Table(schema="MDI",name="MDI975_PQ_CUST_CLASS")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PqCustClass o WHERE o.CUST_CLASS_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PqCustClass o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PqCustClass o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI975_PQ_CUST_CLASS o WHERE o.CUST_CLASS_ID=?"),//AUTO-GENERATED
})
public class PqCustClass extends PqCustClassDAO implements Serializable{	

	private static final long serialVersionUID = -6833493309890003512L;
	public PqCustClass(){
	}
	public PqCustClass(int custClassId){
		super(custClassId);
	}
	
	public PqCustClass(PqCustClass pqCustClass) {
		super(pqCustClass);
	}
	
	public String toString(){
		return this.getTitle();
	}
	public int getPlanyear(){
    	return Department.getActivePlanYear();
    }
}
