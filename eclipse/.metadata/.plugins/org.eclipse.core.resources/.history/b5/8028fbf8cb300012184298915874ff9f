package com.sce.esp.object.model;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.CprSubDAO;
@Entity
@Table(schema="MDI",name="MDISC8_CPR_SUB")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM CprSub o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM CprSub o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM CprSub o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByCprPlant", query="SELECT o FROM CprSub o WHERE o.CPR_PLANT_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByCprSubAcct", query="SELECT o FROM CprSub o WHERE o.CPR_SUB_ACCT_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDISC8_CPR_SUB o WHERE o.ID=?"),//AUTO-GENERATED
})
public class CprSub extends CprSubDAO{
	public CprSub(){
	}
	public CprSub(int id){
		super(id);
	}
	public CprSub(CprSub orig){
		super(orig);
	}
	@Override
	public int compareTo(Object o){
		int c2 = super.compareTo(o);
		if(c2==0 && o instanceof CprSub){
			try {
				CprSub cs = (CprSub)o;
				c2 = getCprSubAcct().compareTo(cs.getCprSubAcct());
				if(c2==0)
					c2 = getCprPlant().compareTo(cs.getCprPlant());
			} catch (SQLException e) {
				e.printStackTrace();
			}
		}
		return c2;
	}
	public String toString(){
		String out=getSub();
		try {
			out += " -- "+getCprSubAcct();
			if(getFundingType()!=null)
				out += "/"+getFundingType();
		} catch (SQLException e) {
			e.printStackTrace();
		}
		return out;
	}
}
