package com.sce.esp.object.dao;
/**Copy Right Frank todo */
/**Description of class, (we need to get this from somewhere, database? xml?)*/
import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.object.jpa.EntityManagerHelper;
import com.sce.esp.object.jpa.PrimaryKey;
import com.sce.esp.object.model.Change;
import java.util.Calendar;
import java.util.Collection;
import java.util.Vector;
import javax.persistence.Cache;
import javax.persistence.Column;
import javax.persistence.EntityManager;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.TypedQuery;
public abstract class ChangeDAO extends DataAccessObject{
	public static enum KEY_PROPERTY{CHANGE_ID};
	public static enum PROPERTY{CHANGE_ID,NAME,DESCRIPTION,HELP_TEXT,DEFAULT_CHANGE,ALT_DOUBLE1,ALT_VARCHAR1,ALT_DATE1,INSERT_TS,INSERT_USER_ID,MODIFIED_TS,MODIFIED_USER_ID,DELETED_TS,DELETED_USER_ID};
	public final static int CHANGEID_LIMIT = 9;
	public final static int NAME_LIMIT = 64;
	public final static int DESCRIPTION_LIMIT = 1024;
	public final static int HELPTEXT_LIMIT = 1024;
	public final static int DEFAULTCHANGE_LIMIT = 1024;
	public final static int ALTDOUBLE1_LIMIT = 24;
	public final static int ALTVARCHAR1_LIMIT = 1024;
	public final static int ALTDATE1_LIMIT = 7;
	private int changeId;
	private String name;
	private String description;
	private String helpText;
	private String defaultChange;
	private double altDouble1;
	private String altVarchar1;
	private Calendar altDate1;
	public static Change getInstance(int changeId)throws java.sql.SQLException{
		return getInstance(changeId, true);
	}
	public static Change getInstance(int changeId, boolean fetch)throws java.sql.SQLException{
		if(isNull(changeId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(changeId);
		Cache cache = em.getEntityManagerFactory().getCache();
		Change o = null;
		if(cache != null && cache.contains(Change.class, pk))
			o = em.find(Change.class, pk);
		if(o==null && getAICache().contains(Change.class, pk))
			o = getAICache().find(Change.class, pk);
		if(o==null && fetch)
			o = em.find(Change.class, pk);
		if(o==null && fetch && Change.class.getAnnotation(AutoInsertable.class) != null){
			o = new Change(changeId);
			getAICache().cache(Change.class, o);
		}
		return o;
	}
	public static Collection<Change> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<Change> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<Change> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM Change o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<Change> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<Change> tq = em.createQuery(jpql,Change.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<Change> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<Change> tq = em.createNamedQuery(name,Change.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public ChangeDAO(){}
	public ChangeDAO(int changeId){
		this.changeId=changeId;
	}
	public ChangeDAO(ChangeDAO orig){
		super(orig);
		copyFrom(orig);
	}
	public void copyFrom(ChangeDAO orig){
		this.name=orig.getName();
		this.description=orig.getDescription();
		this.helpText=orig.getHelpText();
		this.defaultChange=orig.getDefaultChange();
		this.altDouble1=orig.getAltDouble1();
		this.altVarchar1=orig.getAltVarchar1();
		this.altDate1=orig.getAltDate1();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{changeId};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	@Id
	@SequenceGenerator(name="MDIT30_SEQ",sequenceName="MDIT30_SEQ")
	@Column(name="CHANGE_ID",nullable=false,length=9)
	public int getChangeId(){
		return changeId;
	}
	public void setChangeId(int changeId) throws java.sql.SQLException{
		if(isSame(changeId, getChangeId()))return;
		int oldValue = getChangeId();
		this.changeId=changeId;
		setProperty("CHANGE_ID", changeId, oldValue);
	}
	@Column(name="NAME",nullable=false,length=64)
	public String getName(){
		return name;
	}
	public void setName(String name) throws java.sql.SQLException{
		if(isSame(name, getName()))return;
		String oldValue = getName();
		this.name=name;
		setProperty("NAME", name, oldValue);
	}
	@Column(name="DESCRIPTION",nullable=true,length=1024)
	public String getDescription(){
		return description;
	}
	public void setDescription(String description) throws java.sql.SQLException{
		if(isSame(description, getDescription()))return;
		String oldValue = getDescription();
		this.description=description;
		setProperty("DESCRIPTION", description, oldValue);
	}
	@Column(name="HELP_TEXT",nullable=true,length=1024)
	public String getHelpText(){
		return helpText;
	}
	public void setHelpText(String helpText) throws java.sql.SQLException{
		if(isSame(helpText, getHelpText()))return;
		String oldValue = getHelpText();
		this.helpText=helpText;
		setProperty("HELP_TEXT", helpText, oldValue);
	}
	@Column(name="DEFAULT_CHANGE",nullable=true,length=1024)
	public String getDefaultChange(){
		return defaultChange;
	}
	public void setDefaultChange(String defaultChange) throws java.sql.SQLException{
		if(isSame(defaultChange, getDefaultChange()))return;
		String oldValue = getDefaultChange();
		this.defaultChange=defaultChange;
		setProperty("DEFAULT_CHANGE", defaultChange, oldValue);
	}
	@Column(name="ALT_DOUBLE1",nullable=true,length=24)
	public double getAltDouble1(){
		return altDouble1;
	}
	public void setAltDouble1(double altDouble1) throws java.sql.SQLException{
		if(isSame(altDouble1, getAltDouble1()))return;
		double oldValue = getAltDouble1();
		this.altDouble1=altDouble1;
		setProperty("ALT_DOUBLE1", altDouble1, oldValue);
	}
	@Column(name="ALT_VARCHAR1",nullable=true,length=1024)
	public String getAltVarchar1(){
		return altVarchar1;
	}
	public void setAltVarchar1(String altVarchar1) throws java.sql.SQLException{
		if(isSame(altVarchar1, getAltVarchar1()))return;
		String oldValue = getAltVarchar1();
		this.altVarchar1=altVarchar1;
		setProperty("ALT_VARCHAR1", altVarchar1, oldValue);
	}
	@Column(name="ALT_DATE1",nullable=true,length=7)
	public Calendar getAltDate1(){
		return altDate1;
	}
	public void setAltDate1(Calendar altDate1) throws java.sql.SQLException{
		if(isSame(altDate1, getAltDate1()))return;
		Calendar oldValue = getAltDate1();
		this.altDate1=altDate1;
		setProperty("ALT_DATE1", altDate1, oldValue);
	}
	public Change copy()throws java.sql.SQLException{
		Change cp = new Change((Change)this);
		copyChildrenTo(cp);
		return cp;
	}
	public void copyChildrenTo(ChangeDAO cp)throws java.sql.SQLException{
		super.copyChildrenTo(cp);
	}
	public Vector<String> getDifference(ChangeDAO o){
		Vector<String> diffs = super.getDifference(o);
		if(!isSame(getChangeId(),o.getChangeId())) diffs.add("CHANGE_ID");
		if(!isSame(getName(),o.getName())) diffs.add("NAME");
		if(!isSame(getDescription(),o.getDescription())) diffs.add("DESCRIPTION");
		if(!isSame(getHelpText(),o.getHelpText())) diffs.add("HELP_TEXT");
		if(!isSame(getDefaultChange(),o.getDefaultChange())) diffs.add("DEFAULT_CHANGE");
		if(!isSame(getAltDouble1(),o.getAltDouble1())) diffs.add("ALT_DOUBLE1");
		if(!isSame(getAltVarchar1(),o.getAltVarchar1())) diffs.add("ALT_VARCHAR1");
		if(!isSame(getAltDate1(),o.getAltDate1())) diffs.add("ALT_DATE1");
		return diffs;
	}
	public void insertChildren()throws java.sql.SQLException{
	}
}
