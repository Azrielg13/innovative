package com.sce.esp.object.model;

public class DependencyType {
	    private static int enumCount = 1;
	    private int enumVal;
	    private String name;

	    private DependencyType( String str ) {
	      name = str;
	      enumVal = enumCount;
	      enumCount++;
	    }
	    
	    public String toString() { return name; }
	    public int toInt() { return enumVal; }

	    public static final DependencyType FINISH_TO_START  = new DependencyType("Finish to Start");//1
	    public static final DependencyType START_TO_START  = new DependencyType("Start to Start");//2
	    public static final DependencyType FINISH_TO_FINISH  = new DependencyType("Finish to Finish");//3
	    public static final DependencyType START_TO_FINISH  = new DependencyType("Start to Finish");//4
	}
