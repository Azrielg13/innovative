package com.sce.esp.object.model;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.TestResponseDAO;
import com.sce.esp.object.jpa.AutoInsertable;
@Entity
@AutoInsertable
@Table(schema="MDI",name="MDI994_TEST_RESPONSE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM TestResponse o WHERE o.SCRIPT_ID=?1 AND o.QUESTION_ID=?2 AND o.USERNAME_ID=?3"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM TestResponse o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM TestResponse o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByTestUser", query="SELECT o FROM TestResponse o WHERE o.SCRIPT_ID=?1 AND o.USERNAME_ID=?2 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI994_TEST_RESPONSE o WHERE o.SCRIPT_ID=? AND o.QUESTION_ID=? AND o.USERNAME_ID=?"),//AUTO-GENERATED
})
public class TestResponse extends TestResponseDAO{
	

	public final static String[] TR_SEVERITY = {"None (0)","Low (1)","Medium (2)","High (3)","Critical (4)"};
	
	/** The Constant TR_STATUS. */
	public final static String[] TR_STATUS = {"Open","Re-Test","Closed","Future Enhancement"};

	public TestResponse(){
	}
	public TestResponse(int scriptId, int questionId, int usernameId){
		super(scriptId,questionId,usernameId);
	}
	
	public TestResponse(TestResponse testResponse) {
		super(testResponse);
	}
	public int getPlanyear(){
    	return Department.getActivePlanYear();
    }
}

