package com.sce.esp.object.dao;
/**Copy Right Frank todo */
/**Description of class, (we need to get this from somewhere, database? xml?)*/
import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.object.jpa.EntityManagerHelper;
import com.sce.esp.object.jpa.PrimaryKey;
import com.sce.esp.object.model.LicList;
import com.sce.esp.object.model.SceProjLicList;
import java.util.Calendar;
import java.util.Collection;
import java.util.TreeSet;
import java.util.Vector;
import javax.persistence.Cache;
import javax.persistence.Column;
import javax.persistence.EntityManager;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.TypedQuery;
public abstract class LicListDAO extends DataAccessObject{
	public static enum KEY_PROPERTY{LIC_LIST_ID};
	public static enum PROPERTY{LIC_LIST_ID,NAME,DESCRIPTION,ACTIVE,ALT_DOUBLE1,ALT_VARCHAR1,ALT_DATE1,INSERT_TS,INSERT_USER_ID,MODIFIED_TS,MODIFIED_USER_ID,DELETED_TS,DELETED_USER_ID};
	public final static int LICLISTID_LIMIT = 9;
	public final static int NAME_LIMIT = 128;
	public final static int DESCRIPTION_LIMIT = 1024;
	public final static int ACTIVE_LIMIT = 1;
	public final static int ALTDOUBLE1_LIMIT = 24;
	public final static int ALTVARCHAR1_LIMIT = 512;
	public final static int ALTDATE1_LIMIT = 7;
	private int licListId;
	private String name;
	private String description;
	private boolean active = true;
	private double altDouble1;
	private String altVarchar1;
	private Calendar altDate1;
	private Collection<SceProjLicList> sceProjLicLists;
	public static LicList getInstance(int licListId)throws java.sql.SQLException{
		return getInstance(licListId, true);
	}
	public static LicList getInstance(int licListId, boolean fetch)throws java.sql.SQLException{
		if(isNull(licListId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(licListId);
		Cache cache = em.getEntityManagerFactory().getCache();
		LicList o = null;
		if(cache != null && cache.contains(LicList.class, pk))
			o = em.find(LicList.class, pk);
		if(o==null && getAICache().contains(LicList.class, pk))
			o = getAICache().find(LicList.class, pk);
		if(o==null && fetch)
			o = em.find(LicList.class, pk);
		if(o==null && fetch && LicList.class.getAnnotation(AutoInsertable.class) != null){
			o = new LicList(licListId);
			getAICache().cache(LicList.class, o);
		}
		return o;
	}
	public static Collection<LicList> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<LicList> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<LicList> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM LicList o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<LicList> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<LicList> tq = em.createQuery(jpql,LicList.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<LicList> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<LicList> tq = em.createNamedQuery(name,LicList.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public LicListDAO(){}
	public LicListDAO(int licListId){
		this.licListId=licListId;
	}
	public LicListDAO(LicListDAO orig){
		super(orig);
		copyFrom(orig);
	}
	public void copyFrom(LicListDAO orig){
		this.name=orig.getName();
		this.description=orig.getDescription();
		this.active=orig.isActive();
		this.altDouble1=orig.getAltDouble1();
		this.altVarchar1=orig.getAltVarchar1();
		this.altDate1=orig.getAltDate1();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{licListId};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	@Id
	@SequenceGenerator(name="MDIT02_SEQ",sequenceName="MDIT02_SEQ")
	@Column(name="LIC_LIST_ID",nullable=false,length=9)
	public int getLicListId(){
		return licListId;
	}
	public void setLicListId(int licListId) throws java.sql.SQLException{
		if(isSame(licListId, getLicListId()))return;
		int oldValue = getLicListId();
		this.licListId=licListId;
		setProperty("LIC_LIST_ID", licListId, oldValue);
	}
	@Column(name="NAME",nullable=false,length=128)
	public String getName(){
		return name;
	}
	public void setName(String name) throws java.sql.SQLException{
		if(isSame(name, getName()))return;
		String oldValue = getName();
		this.name=name;
		setProperty("NAME", name, oldValue);
	}
	@Column(name="DESCRIPTION",nullable=true,length=1024)
	public String getDescription(){
		return description;
	}
	public void setDescription(String description) throws java.sql.SQLException{
		if(isSame(description, getDescription()))return;
		String oldValue = getDescription();
		this.description=description;
		setProperty("DESCRIPTION", description, oldValue);
	}
	@Column(name="ACTIVE",nullable=true,length=1)
	public boolean isActive(){
		return active;
	}
	public void setActive(boolean active) throws java.sql.SQLException{
		if(isSame(active, isActive()))return;
		boolean oldValue = isActive();
		this.active=active;
		setProperty("ACTIVE", active, oldValue);
	}
	@Column(name="ALT_DOUBLE1",nullable=true,length=24)
	public double getAltDouble1(){
		return altDouble1;
	}
	public void setAltDouble1(double altDouble1) throws java.sql.SQLException{
		if(isSame(altDouble1, getAltDouble1()))return;
		double oldValue = getAltDouble1();
		this.altDouble1=altDouble1;
		setProperty("ALT_DOUBLE1", altDouble1, oldValue);
	}
	@Column(name="ALT_VARCHAR1",nullable=true,length=512)
	public String getAltVarchar1(){
		return altVarchar1;
	}
	public void setAltVarchar1(String altVarchar1) throws java.sql.SQLException{
		if(isSame(altVarchar1, getAltVarchar1()))return;
		String oldValue = getAltVarchar1();
		this.altVarchar1=altVarchar1;
		setProperty("ALT_VARCHAR1", altVarchar1, oldValue);
	}
	@Column(name="ALT_DATE1",nullable=true,length=7)
	public Calendar getAltDate1(){
		return altDate1;
	}
	public void setAltDate1(Calendar altDate1) throws java.sql.SQLException{
		if(isSame(altDate1, getAltDate1()))return;
		Calendar oldValue = getAltDate1();
		this.altDate1=altDate1;
		setProperty("ALT_DATE1", altDate1, oldValue);
	}
	public Collection<SceProjLicList> getSceProjLicLists()throws java.sql.SQLException{
		if(isNewInstance() || sceProjLicLists != null){
			if(sceProjLicLists == null)
				sceProjLicLists = new TreeSet<SceProjLicList>();
			return sceProjLicLists;
		}
		return SceProjLicList.getNamedCollection("findByLicList",getLicListId());
	}
	public void addSceProjLicList(SceProjLicList sceProjLicList)throws java.sql.SQLException{
		sceProjLicList.setLicList((LicList)this);
		if(isNewInstance() || sceProjLicLists != null)
			getSceProjLicLists().add(sceProjLicList);
		else
			sceProjLicList.insert();
	}
	public void removeSceProjLicList(SceProjLicList sceProjLicList)throws java.sql.SQLException{
		if(isNewInstance() || sceProjLicLists != null)
			getSceProjLicLists().remove(sceProjLicList);
		else
			sceProjLicList.delete();
	}
	public LicList copy()throws java.sql.SQLException{
		LicList cp = new LicList((LicList)this);
		copyChildrenTo(cp);
		return cp;
	}
	public void copyChildrenTo(LicListDAO cp)throws java.sql.SQLException{
		super.copyChildrenTo(cp);
		for(SceProjLicList child:getSceProjLicLists())
			cp.addSceProjLicList(child.copy());
	}
	public Vector<String> getDifference(LicListDAO o){
		Vector<String> diffs = super.getDifference(o);
		if(!isSame(getLicListId(),o.getLicListId())) diffs.add("LIC_LIST_ID");
		if(!isSame(getName(),o.getName())) diffs.add("NAME");
		if(!isSame(getDescription(),o.getDescription())) diffs.add("DESCRIPTION");
		if(!isSame(isActive(),o.isActive())) diffs.add("ACTIVE");
		if(!isSame(getAltDouble1(),o.getAltDouble1())) diffs.add("ALT_DOUBLE1");
		if(!isSame(getAltVarchar1(),o.getAltVarchar1())) diffs.add("ALT_VARCHAR1");
		if(!isSame(getAltDate1(),o.getAltDate1())) diffs.add("ALT_DATE1");
		return diffs;
	}
	public void insertChildren()throws java.sql.SQLException{
		if(sceProjLicLists != null){
			for(SceProjLicList sceProjLicList:getSceProjLicLists()){
				if(sceProjLicList.isNewInstance())
					sceProjLicList.setLicList((LicList)this);
				sceProjLicList.insert();
			}
			sceProjLicLists = null;
		}
	}
}
