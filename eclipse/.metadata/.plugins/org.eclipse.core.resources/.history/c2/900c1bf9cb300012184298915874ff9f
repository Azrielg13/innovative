package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PifImpactValueDAO;
@Entity
@Table(schema="MDI",name="MDI678_PIF_IMPACT_VALUE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PifImpactValue o WHERE o.PLANYEAR=?1 AND o.PIF_IMPACT_VALUE_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PifImpactValue o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PifImpactValue o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI678_PIF_IMPACT_VALUE o WHERE o.PLANYEAR=? AND o.PIF_IMPACT_VALUE_ID=?"),//AUTO-GENERATED
})
public class PifImpactValue extends PifImpactValueDAO implements Serializable{

	private static final long serialVersionUID = 2846620536543060861L;

	@Override
	public String toString() {
		return getName();
	}

	public PifImpactValue(){
	}
	public PifImpactValue(int planyear, int pifImpactValueId) {
		super(planyear, pifImpactValueId);
	}

	public PifImpactValue(PifImpactValue pifImpactValue) {
		super(pifImpactValue);
	}
}
