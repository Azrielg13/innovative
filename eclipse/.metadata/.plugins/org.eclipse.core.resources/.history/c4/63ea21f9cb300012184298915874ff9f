package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.WorkGroupDisDAO;
@Entity
@Table(schema="MDI",name="MDIS17_WORK_GROUP_DIS")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM WorkGroupDis o WHERE o.WORK_GROUP_ID=?1 AND o.TYPE=?2 AND o.DISCIPLINE_ID=?3"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM WorkGroupDis o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM WorkGroupDis o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByWorkGroup", query="SELECT o FROM WorkGroupDis o WHERE o.WORK_GROUP_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS17_WORK_GROUP_DIS o WHERE o.WORK_GROUP_ID=? AND o.TYPE=? AND o.DISCIPLINE_ID=?"),//AUTO-GENERATED
})
public class WorkGroupDis extends WorkGroupDisDAO implements Serializable{
	
	private static final long serialVersionUID = -1242762598289759638L;

	public WorkGroupDis(){
		super(0,0,0);
	}
	
	public WorkGroupDis(int workGroupId, int disciplineId, int type) {
		super(workGroupId, disciplineId, type);
	}

	public WorkGroupDis(WorkGroupDis workGroupDis) {
		super(workGroupDis);
	}

}
