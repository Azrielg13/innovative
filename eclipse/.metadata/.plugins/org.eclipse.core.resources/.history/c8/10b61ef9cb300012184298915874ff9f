package com.sce.esp.object.model;
import com.sce.esp.object.dao.ScopeTypeElemDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDISX6_SCOPE_TYPE_ELEM")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM ScopeTypeElem o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM ScopeTypeElem o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM ScopeTypeElem o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByScopeType", query="SELECT o FROM ScopeTypeElem o WHERE o.SCOPE_TYPE_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findBySceElem", query="SELECT o FROM ScopeTypeElem o WHERE o.SCE_ELEM_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDISX6_SCOPE_TYPE_ELEM o WHERE o.ID=?"),//AUTO-GENERATED
})
public class ScopeTypeElem extends ScopeTypeElemDAO{
	public ScopeTypeElem(){
	}
	public ScopeTypeElem(int id){
		super(id);
	}
	public ScopeTypeElem(ScopeTypeElem orig){
		super(orig);
	}
}
