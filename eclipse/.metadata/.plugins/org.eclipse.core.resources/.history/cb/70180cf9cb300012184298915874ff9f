package com.sce.esp.object.model;
import java.sql.SQLException;

import com.sce.esp.object.dao.StResourceDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDIT26_ST_RESOURCE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM StResource o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM StResource o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM StResource o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByScheduleTask", query="SELECT o FROM StResource o WHERE o.ST_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIT26_ST_RESOURCE o WHERE o.ID=?"),//AUTO-GENERATED
})
public class StResource extends StResourceDAO{
	public StResource(){
	}
	public StResource(int id){
		super(id);
	}
	public StResource(StResource orig){
		super(orig);
	}
	@Override
	public void setTask(AbstractTask at) throws SQLException {
		setScheduleTask((ScheduleTask) at);
	}
}
