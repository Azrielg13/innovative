package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.SapWbsDAO;
@Entity
@Table(schema="MDI",name="MDIS30_SAP_WBS")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM SapWbs o WHERE o.SAP_WBS_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM SapWbs o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM SapWbs o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS30_SAP_WBS o WHERE o.SAP_WBS_ID=?"),//AUTO-GENERATED
})
public class SapWbs extends SapWbsDAO implements Serializable{

	private static final long serialVersionUID = 8216088082436173322L;

	public SapWbs(){
	}
	public SapWbs(int sapWbsId) {
		super(sapWbsId);
	}
	
	public SapWbs(SapWbs sapWbs) {
		super(sapWbs);
	}
	
	public String toString(){
		return this.getName();
	}

}
