package com.sce.esp.object.model;
import java.sql.SQLException;
import java.util.Collection;
import java.util.TreeSet;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.CprPrimeDAO;
@Entity
@Table(schema="MDI",name="MDISC4_CPR_PRIME")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM CprPrime o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM CprPrime o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM CprPrime o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByCprPlant", query="SELECT o FROM CprPrime o WHERE o.CPR_PLANT_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByCprPrimeAcct", query="SELECT o FROM CprPrime o WHERE o.CPR_PRIME_ACCT_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDISC4_CPR_PRIME o WHERE o.ID=?"),//AUTO-GENERATED
})
public class CprPrime extends CprPrimeDAO{
	public CprPrime(){
	}
	public CprPrime(int id){
		super(id);
	}
	public CprPrime(CprPrime orig){
		super(orig);
	}
	public String toString(){
		String out = getPrime();
		try {
			out+=" -- "+getCprPrimeAcct();
		} catch (SQLException e) {
			e.printStackTrace();
		}
		return out;
	}
	public Collection<CprSub> getCprSubs() throws SQLException {
		TreeSet<CprSub> cprSubs = new TreeSet<CprSub>();
		for(CprSub cprSub:getCprPlant().getCprSubs())
			if(cprSub.getCprSubAcct().getCprPrimeAcct()==getCprPrimeAcct())
				cprSubs.add(cprSub);
		return cprSubs;
	}
}
