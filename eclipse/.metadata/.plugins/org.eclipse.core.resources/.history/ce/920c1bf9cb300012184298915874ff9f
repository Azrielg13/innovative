package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PifLineJustDAO;
@Entity
@Table(schema="MDI",name="MDI693_PIF_LINE_JUST")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PifLineJust o WHERE o.PLANYEAR=?1 AND o.LINE_SECTION_ID=?2 AND o.PIF_ID=?3"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PifLineJust o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PifLineJust o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByPif", query="SELECT o FROM PifLineJust o WHERE o.PLANYEAR=?1 AND o.PIF_ID=?2 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI693_PIF_LINE_JUST o WHERE o.PLANYEAR=? AND o.LINE_SECTION_ID=? AND o.PIF_ID=?"),//AUTO-GENERATED
})
public class PifLineJust extends PifLineJustDAO implements Serializable{
	
	private static final long serialVersionUID = 3097770941129477182L;

	/**
	 * Instantiates a new pif line just.
	 * 
	 * @param planyear the planyear
	 * @param lineSectionId the line section id
	 * @param pifId the pif id
	 */
	public PifLineJust(){
	}
	public PifLineJust(int planyear, int lineSectionId, int pifId){
		super(planyear,lineSectionId,pifId);
	}
	
	/**
	 * Instantiates a new pif line just.
	 * 
	 * @param planyear the planyear
	 * @param pifId the pif id
	 */
	public PifLineJust(int planyear,Pif pif) {
		super(planyear,0,pif.getPifId());
	}

	public PifLineJust(PifLineJust pifLineJust) {
		super(pifLineJust);
	}
}


