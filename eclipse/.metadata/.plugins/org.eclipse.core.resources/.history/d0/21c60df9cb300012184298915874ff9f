package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.SubWeatherDAO;
@Entity
@Table(schema="MDI",name="MDI590_SUB_WEATHER")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM SubWeather o WHERE o.PLANYEAR=?1 AND o.BBANK_SUB_ID=?2 AND o.STATION_ID=?3"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM SubWeather o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM SubWeather o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByBbank", query="SELECT o FROM SubWeather o WHERE o.PLANYEAR=?1 AND o.BBANK_SUB_ID=?2 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByWs", query="SELECT o FROM SubWeather o WHERE o.PLANYEAR=?1 AND o.STATION_ID=?2 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI590_SUB_WEATHER o WHERE o.PLANYEAR=? AND o.BBANK_SUB_ID=? AND o.STATION_ID=?"),//AUTO-GENERATED
})
public class SubWeather extends SubWeatherDAO implements Serializable{
	
	private static final long serialVersionUID = 4670680428045924014L;

	/**
	 * The Constructor.
	 * 
	 * @param subId the sub id
	 * @param stationId the station id
	 * @param simId 	 * @param planyear the planyear
	 * @param planyear the planyear
	 */
	public SubWeather(){
	}
	public SubWeather(int planyear, int subId, int stationId){
		super(planyear,subId,stationId);
	}
	
	public SubWeather(SubWeather subWeather) {
		super(subWeather);
	}

	/**
	 * To string.
	 * 
	 * @return the string
	 */
	public String toString(){
		try{
			return getWeatherstation().toString();
		}catch(Exception e){
			e.printStackTrace();
		}
		return "";
	}
}
