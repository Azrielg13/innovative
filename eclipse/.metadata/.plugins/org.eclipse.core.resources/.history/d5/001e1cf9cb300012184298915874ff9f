package com.sce.esp.object.model;
import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.ProgDAO;
@Entity
@Table(schema="MDI",name="MDI110_PROG")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM Prog o WHERE o.PLANYEAR=?1 AND o.PROG_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM Prog o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM Prog o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByDepartment", query="SELECT o FROM Prog o WHERE o.PLANYEAR=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI110_PROG o WHERE o.PLANYEAR=? AND o.PROG_ID=?"),//AUTO-GENERATED
})
public class Prog extends ProgDAO implements Serializable{
	private static final long serialVersionUID = 1L;
	public Prog(){
	}
	public Prog(int planyear, int progId){
		super(planyear, progId);
	}
	public Prog(Prog orig){
		super(orig);
	}
}
