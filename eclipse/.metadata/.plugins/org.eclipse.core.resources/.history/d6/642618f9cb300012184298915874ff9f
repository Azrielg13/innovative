package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.DuctBankFileDAO;
@Entity
@Table(schema="MDI",name="MDI705_DUCT_BANK_FILE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM DuctBankFile o WHERE o.PLANYEAR=?1 AND o.FILE_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM DuctBankFile o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM DuctBankFile o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByDuctBank", query="SELECT o FROM DuctBankFile o WHERE o.PLANYEAR=?1 AND o.DB_ID=?2 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI705_DUCT_BANK_FILE o WHERE o.PLANYEAR=? AND o.FILE_ID=?"),//AUTO-GENERATED
})
public class DuctBankFile extends DuctBankFileDAO implements Serializable{

	private static final long serialVersionUID = 994912099199854336L;

	/**
	 * The Constructor.
	 *
	 * @param fileId the file id
	 * @param planyear the planyear
	 */
	public DuctBankFile(){
	}
	public DuctBankFile(int planyear, int fileId){
		super(planyear,fileId);
	}

	/**
	 * The Constructor.
	 *
	 * @param planyear the planyear
	 */
	public DuctBankFile(int planyear){
		super(planyear,0);
	}
	
	public DuctBankFile(DuctBankFile ductBankFile) {
		super(ductBankFile);
	}

	public boolean isEditable(){
		return getPlanyear() == Department.getActivePlanYear();
	}
}
