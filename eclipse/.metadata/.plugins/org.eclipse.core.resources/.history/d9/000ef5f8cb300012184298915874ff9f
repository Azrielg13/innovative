package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.PifGwCatDAO;
@Entity
@Table(schema="MDI",name="MDI667_PIF_GW_CAT")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM PifGwCat o WHERE o.PLANYEAR=?1 AND o.PIF_GW_CAT_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM PifGwCat o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM PifGwCat o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI667_PIF_GW_CAT o WHERE o.PLANYEAR=? AND o.PIF_GW_CAT_ID=?"),//AUTO-GENERATED
})
public class PifGwCat extends PifGwCatDAO implements Serializable{

	private static final long serialVersionUID = 8249683404829385851L;

	@Override
	public String toString() {
		return getGwName();
	}

	public PifGwCat(){
	}
	public PifGwCat(int planyear, int pifGwCatId) {
		super(planyear, pifGwCatId);
	}

	public PifGwCat(PifGwCat pifGwCat) {
	super(pifGwCat);
	}

}
