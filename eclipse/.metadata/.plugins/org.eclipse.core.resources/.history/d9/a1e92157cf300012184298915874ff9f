package com.sce.esp.object.model;

import java.sql.SQLException;

import com.sce.esp.log.EspLogger;
import com.sce.esp.object.ESPTestCase;

public class N1Test extends ESPTestCase {
	public void testN1Reserve() throws SQLException{
		ScopeType st = new ScopeType();
		st.setTitle("Add Eddie");
		st.setDescription("Add a great programmer");
		st.setInstallationOption(ScopeType.HIGH_OR_LOW);
		st.setUnit("Feet");
		//st.insert();
		assertTrue(Department.getActivePlanYear()>=2011);
		assertEquals(2002,Department.getMinPlanYear());
		Bbank s1 = (Bbank)Bbank.getInstanceBySubName(2011, "Alder", 12);
		assertNotNull(s1);
		assertEquals(.016,s1.getYear(0, 2013).getTempSens());
		EspLogger.log(EspLogger.LEVEL.MESSAGE, this, s1.getYear(0, 2013).getN1Demand());
		EspLogger.log(EspLogger.LEVEL.MESSAGE, this, s1.getYear(0, 2013).getN1Reserve());
	}
}
