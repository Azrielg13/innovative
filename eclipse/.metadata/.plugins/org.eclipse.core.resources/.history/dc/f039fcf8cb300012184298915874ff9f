package com.sce.esp.object.model;

import java.io.Serializable;
import java.sql.SQLException;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.AbankN1TieDAO;
import com.sce.esp.util.Calculate;
@Entity
@Table(schema="MDI",name="MDI375_ABANK_N1_TIE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM AbankN1Tie o WHERE o.PLANYEAR=?1 AND o.TIE_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM AbankN1Tie o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM AbankN1Tie o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findBySys", query="SELECT o FROM AbankN1Tie o WHERE o.PLANYEAR=?1 AND o.SYS_ID=?2 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI375_ABANK_N1_TIE o WHERE o.PLANYEAR=? AND o.TIE_ID=?"),//AUTO-GENERATED
})
public class AbankN1Tie extends AbankN1TieDAO implements Serializable{
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 3735884368820421458L;

	/**
	 * The Constructor.
	 * 
	 * @param planyear the planyear
	 */
	public AbankN1Tie(){
	}
	public AbankN1Tie(int planyear){
		super(planyear,0);
	}
	
	/**
	 * The Constructor.
	 * 
	 * @param planyear the planyear
	 * @param tieId the tie id
	 */
	public AbankN1Tie(int planyear, int tieId){
		super(planyear,tieId);
	}
	
	public AbankN1Tie(AbankN1Tie abankN1Tie) {
		super(abankN1Tie); 
	}

	/**
	 * Gets the distance.
	 * 
	 * @return the distance
	 * 
	 * @throws SQLException the SQL exception
	 */
	public double getDistance()throws SQLException{
		return Calculate.distance(getSys().getLatitude(), getSys().getLongitude(), getAbank().getLatitude(), getAbank().getLongitude());
	}
	
	/**
	 * To string.
	 * 
	 * @return the string
	 */
	public String toString(){
		try {
			return ""+getAbank();
		} catch (SQLException e) {
			e.printStackTrace();
		}
		return getHashKey();
	}
}

