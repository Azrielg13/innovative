import javax.persistence.Cache;
import javax.persistence.Column;
import javax.persistence.EntityManager;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.TypedQuery;
public abstract class SceProjLicExempDAO extends DataAccessObject{
	public static enum KEY_PROPERTY{SCE_PROJ_LIC_EXEMP_ID};
	public static enum PROPERTY{SCE_PROJ_LIC_EXEMP_ID,SCE_PROJ_REV_ID,LIC_EXEMPTION_ID,USER_OVERRIDE,NOTE,ALT_DOUBLE1,ALT_VARCHAR1,ALT_DATE1,INSERT_TS,INSERT_USER_ID,MODIFIED_TS,MODIFIED_USER_ID,DELETED_TS,DELETED_USER_ID};
	public final static int SCEPROJLICEXEMPID_LIMIT = 9;
	public final static int SCEPROJREVID_LIMIT = 9;
	public final static int LICEXEMPTIONID_LIMIT = 9;
	public final static int USEROVERRIDE_LIMIT = 9;
	public final static int NOTE_LIMIT = 1024;
	public final static int ALTDOUBLE1_LIMIT = 24;
	public final static int ALTVARCHAR1_LIMIT = 512;
	public final static int ALTDATE1_LIMIT = 7;
	private int sceProjLicExempId;
	private int sceProjRevId;
	private int licExemptionId;
	private int userOverride;
	private String note;
	private double altDouble1;
	private String altVarchar1;
	private Calendar altDate1;
	private LicExemption licExemption;
	private SceProjRev sceProjRev;
	public static SceProjLicExemp getInstance(int sceProjLicExempId)throws java.sql.SQLException{
		return getInstance(sceProjLicExempId, true);
	}
	public static SceProjLicExemp getInstance(int sceProjLicExempId, boolean fetch)throws java.sql.SQLException{
		if(isNull(sceProjLicExempId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(sceProjLicExempId);
		Cache cache = em.getEntityManagerFactory().getCache();
		SceProjLicExemp o = null;
		if(cache != null && cache.contains(SceProjLicExemp.class, pk))
			o = em.find(SceProjLicExemp.class, pk);
		if(o==null && getAICache().contains(SceProjLicExemp.class, pk))
			o = getAICache().find(SceProjLicExemp.class, pk);
		if(o==null && fetch)
			o = em.find(SceProjLicExemp.class, pk);
		if(o==null && fetch && SceProjLicExemp.class.getAnnotation(AutoInsertable.class) != null){
			o = new SceProjLicExemp(sceProjLicExempId);
			getAICache().cache(SceProjLicExemp.class, o);
		}
		return o;
	}
	public static Collection<SceProjLicExemp> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<SceProjLicExemp> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<SceProjLicExemp> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM SceProjLicExemp o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<SceProjLicExemp> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<SceProjLicExemp> tq = em.createQuery(jpql,SceProjLicExemp.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<SceProjLicExemp> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<SceProjLicExemp> tq = em.createNamedQuery(name,SceProjLicExemp.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public SceProjLicExempDAO(){}
	public SceProjLicExempDAO(int sceProjLicExempId){
		this.sceProjLicExempId=sceProjLicExempId;
	}
	public SceProjLicExempDAO(SceProjLicExempDAO orig){
		super(orig);
		copyFrom(orig);
	}
	public void copyFrom(SceProjLicExempDAO orig){
		this.sceProjRevId=orig.getSceProjRevId();
		this.licExemptionId=orig.getLicExemptionId();
		this.userOverride=orig.getUserOverride();
		this.note=orig.getNote();
		this.altDouble1=orig.getAltDouble1();
		this.altVarchar1=orig.getAltVarchar1();
		this.altDate1=orig.getAltDate1();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{sceProjLicExempId};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	@Id
	@SequenceGenerator(name="MDIT07_SEQ",sequenceName="MDIT07_SEQ")
	@Column(name="SCE_PROJ_LIC_EXEMP_ID",nullable=false,length=9)
	public int getSceProjLicExempId(){
		return sceProjLicExempId;
	}
	public void setSceProjLicExempId(int sceProjLicExempId) throws java.sql.SQLException{
		if(isSame(sceProjLicExempId, getSceProjLicExempId()))return;
		int oldValue = getSceProjLicExempId();
		this.sceProjLicExempId=sceProjLicExempId;
		setProperty("SCE_PROJ_LIC_EXEMP_ID", sceProjLicExempId, oldValue);
	}
	@Column(name="SCE_PROJ_REV_ID",nullable=false,length=9)
	public int getSceProjRevId(){
		return sceProjRevId;
	}
	public void setSceProjRevId(int sceProjRevId) throws java.sql.SQLException{
		if(isSame(sceProjRevId, getSceProjRevId()))return;
		int oldValue = getSceProjRevId();
		this.sceProjRevId=sceProjRevId;
		setProperty("SCE_PROJ_REV_ID", sceProjRevId, oldValue);
		sceProjRev=null;
	}
	@Column(name="LIC_EXEMPTION_ID",nullable=false,length=9)
	public int getLicExemptionId(){
		return licExemptionId;
	}
	public void setLicExemptionId(int licExemptionId) throws java.sql.SQLException{
		if(isSame(licExemptionId, getLicExemptionId()))return;
		int oldValue = getLicExemptionId();
		this.licExemptionId=licExemptionId;
		setProperty("LIC_EXEMPTION_ID", licExemptionId, oldValue);
		licExemption=null;
	}
	@Column(name="USER_OVERRIDE",nullable=true,length=9)
	public int getUserOverride(){
		return userOverride;
	}
	public void setUserOverride(int userOverride) throws java.sql.SQLException{
		if(isSame(userOverride, getUserOverride()))return;
		int oldValue = getUserOverride();
		this.userOverride=userOverride;
		setProperty("USER_OVERRIDE", userOverride, oldValue);
	}
	@Column(name="NOTE",nullable=true,length=1024)
	public String getNote(){
		return note;
	}
	public void setNote(String note) throws java.sql.SQLException{
		if(isSame(note, getNote()))return;
		String oldValue = getNote();
		this.note=note;
		setProperty("NOTE", note, oldValue);
	}
	@Column(name="ALT_DOUBLE1",nullable=true,length=24)
	public double getAltDouble1(){
		return altDouble1;
	}
	public void setAltDouble1(double altDouble1) throws java.sql.SQLException{
		if(isSame(altDouble1, getAltDouble1()))return;
		double oldValue = getAltDouble1();
		this.altDouble1=altDouble1;
		setProperty("ALT_DOUBLE1", altDouble1, oldValue);
	}
	@Column(name="ALT_VARCHAR1",nullable=true,length=512)
	public String getAltVarchar1(){
		return altVarchar1;
	}
	public void setAltVarchar1(String altVarchar1) throws java.sql.SQLException{
		if(isSame(altVarchar1, getAltVarchar1()))return;
		String oldValue = getAltVarchar1();
		this.altVarchar1=altVarchar1;
		setProperty("ALT_VARCHAR1", altVarchar1, oldValue);
	}
	@Column(name="ALT_DATE1",nullable=true,length=7)
	public Calendar getAltDate1(){
		return altDate1;
	}
	public void setAltDate1(Calendar altDate1) throws java.sql.SQLException{
		if(isSame(altDate1, getAltDate1()))return;
		Calendar oldValue = getAltDate1();
		this.altDate1=altDate1;
		setProperty("ALT_DATE1", altDate1, oldValue);
	}
	public LicExemption getLicExemption()throws java.sql.SQLException{
		if(licExemption==null)
			licExemption=LicExemption.getInstance(getLicExemptionId());
		return licExemption;
	}
	public void setLicExemption(LicExemption licExemption)throws java.sql.SQLException{
		setLicExemptionId(licExemption==null?0:licExemption.getLicExemptionId());
		this.licExemption=licExemption;
	}
	public SceProjRev getSceProjRev()throws java.sql.SQLException{
		if(sceProjRev==null)
			sceProjRev=SceProjRev.getInstance(getSceProjRevId());
		return sceProjRev;
	}
	public void setSceProjRev(SceProjRev sceProjRev)throws java.sql.SQLException{
		setSceProjRevId(sceProjRev==null?0:sceProjRev.getSceProjRevId());
		this.sceProjRev=sceProjRev;
	}
	public SceProjLicExemp copy()throws java.sql.SQLException{
		SceProjLicExemp cp = new SceProjLicExemp((SceProjLicExemp)this);
		copyChildrenTo(cp);
		return cp;
	}
	public void copyChildrenTo(SceProjLicExempDAO cp)throws java.sql.SQLException{
		super.copyChildrenTo(cp);
	}
	public Vector<String> getDifference(SceProjLicExempDAO o){
		Vector<String> diffs = super.getDifference(o);
		if(!isSame(getSceProjLicExempId(),o.getSceProjLicExempId())) diffs.add("SCE_PROJ_LIC_EXEMP_ID");
		if(!isSame(getSceProjRevId(),o.getSceProjRevId())) diffs.add("SCE_PROJ_REV_ID");
		if(!isSame(getLicExemptionId(),o.getLicExemptionId())) diffs.add("LIC_EXEMPTION_ID");
		if(!isSame(getUserOverride(),o.getUserOverride())) diffs.add("USER_OVERRIDE");
		if(!isSame(getNote(),o.getNote())) diffs.add("NOTE");
		if(!isSame(getAltDouble1(),o.getAltDouble1())) diffs.add("ALT_DOUBLE1");
		if(!isSame(getAltVarchar1(),o.getAltVarchar1())) diffs.add("ALT_VARCHAR1");
		if(!isSame(getAltDate1(),o.getAltDate1())) diffs.add("ALT_DATE1");
		return diffs;
	}
	public void insertChildren()throws java.sql.SQLException{
	}
