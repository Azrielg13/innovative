package com.sce.esp.object.dao;
/**Copy Right Frank todo */
/**Description of class, (we need to get this from somewhere, database? xml?)*/
import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.object.jpa.EntityManagerHelper;
import com.sce.esp.object.jpa.PrimaryKey;
import com.sce.esp.object.model.Assumption;
import com.sce.esp.object.model.AssumptionLoc;
import java.util.Calendar;
import java.util.Collection;
import java.util.TreeSet;
import java.util.Vector;
import javax.persistence.Cache;
import javax.persistence.Column;
import javax.persistence.EntityManager;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.TypedQuery;
public abstract class AssumptionDAO extends DataAccessObject{
	public static enum KEY_PROPERTY{ASSUMPTION_ID};
	public static enum PROPERTY{ASSUMPTION_ID,NAME,DESCRIPTION,HELP_TEXT,DEFAULT_ASSUMPTION,ALT_DOUBLE1,ALT_VARCHAR1,ALT_DATE1,INSERT_TS,INSERT_USER_ID,MODIFIED_TS,MODIFIED_USER_ID,DELETED_TS,DELETED_USER_ID};
	public final static int ASSUMPTIONID_LIMIT = 9;
	public final static int NAME_LIMIT = 64;
	public final static int DESCRIPTION_LIMIT = 1024;
	public final static int HELPTEXT_LIMIT = 1024;
	public final static int DEFAULTASSUMPTION_LIMIT = 1024;
	public final static int ALTDOUBLE1_LIMIT = 24;
	public final static int ALTVARCHAR1_LIMIT = 1024;
	public final static int ALTDATE1_LIMIT = 7;
	private int assumptionId;
	private String name;
	private String description;
	private String helpText;
	private String defaultAssumption;
	private double altDouble1;
	private String altVarchar1;
	private Calendar altDate1;
	private Collection<AssumptionLoc> assumptionLocs;
	public static Assumption getInstance(int assumptionId)throws java.sql.SQLException{
		return getInstance(assumptionId, true);
	}
	public static Assumption getInstance(int assumptionId, boolean fetch)throws java.sql.SQLException{
		if(isNull(assumptionId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(assumptionId);
		Cache cache = em.getEntityManagerFactory().getCache();
		Assumption o = null;
		if(cache != null && cache.contains(Assumption.class, pk))
			o = em.find(Assumption.class, pk);
		if(o==null && getAICache().contains(Assumption.class, pk))
			o = getAICache().find(Assumption.class, pk);
		if(o==null && fetch)
			o = em.find(Assumption.class, pk);
		if(o==null && fetch && Assumption.class.getAnnotation(AutoInsertable.class) != null){
			o = new Assumption(assumptionId);
			getAICache().cache(Assumption.class, o);
		}
		return o;
	}
	public static Collection<Assumption> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<Assumption> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<Assumption> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM Assumption o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<Assumption> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<Assumption> tq = em.createQuery(jpql,Assumption.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<Assumption> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<Assumption> tq = em.createNamedQuery(name,Assumption.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public AssumptionDAO(){}
	public AssumptionDAO(int assumptionId){
		this.assumptionId=assumptionId;
	}
	public AssumptionDAO(AssumptionDAO orig){
		super(orig);
		copyFrom(orig);
	}
	public void copyFrom(AssumptionDAO orig){
		this.name=orig.getName();
		this.description=orig.getDescription();
		this.helpText=orig.getHelpText();
		this.defaultAssumption=orig.getDefaultAssumption();
		this.altDouble1=orig.getAltDouble1();
		this.altVarchar1=orig.getAltVarchar1();
		this.altDate1=orig.getAltDate1();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{assumptionId};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	@Id
	@SequenceGenerator(name="MDIT65_SEQ",sequenceName="MDIT65_SEQ")
	@Column(name="ASSUMPTION_ID",nullable=false,length=9)
	public int getAssumptionId(){
		return assumptionId;
	}
	public void setAssumptionId(int assumptionId) throws java.sql.SQLException{
		if(isSame(assumptionId, getAssumptionId()))return;
		int oldValue = getAssumptionId();
		this.assumptionId=assumptionId;
		setProperty("ASSUMPTION_ID", assumptionId, oldValue);
	}
	@Column(name="NAME",nullable=false,length=64)
	public String getName(){
		return name;
	}
	public void setName(String name) throws java.sql.SQLException{
		if(isSame(name, getName()))return;
		String oldValue = getName();
		this.name=name;
		setProperty("NAME", name, oldValue);
	}
	@Column(name="DESCRIPTION",nullable=true,length=1024)
	public String getDescription(){
		return description;
	}
	public void setDescription(String description) throws java.sql.SQLException{
		if(isSame(description, getDescription()))return;
		String oldValue = getDescription();
		this.description=description;
		setProperty("DESCRIPTION", description, oldValue);
	}
	@Column(name="HELP_TEXT",nullable=true,length=1024)
	public String getHelpText(){
		return helpText;
	}
	public void setHelpText(String helpText) throws java.sql.SQLException{
		if(isSame(helpText, getHelpText()))return;
		String oldValue = getHelpText();
		this.helpText=helpText;
		setProperty("HELP_TEXT", helpText, oldValue);
	}
	@Column(name="DEFAULT_ASSUMPTION",nullable=true,length=1024)
	public String getDefaultAssumption(){
		return defaultAssumption;
	}
	public void setDefaultAssumption(String defaultAssumption) throws java.sql.SQLException{
		if(isSame(defaultAssumption, getDefaultAssumption()))return;
		String oldValue = getDefaultAssumption();
		this.defaultAssumption=defaultAssumption;
		setProperty("DEFAULT_ASSUMPTION", defaultAssumption, oldValue);
	}
	@Column(name="ALT_DOUBLE1",nullable=true,length=24)
	public double getAltDouble1(){
		return altDouble1;
	}
	public void setAltDouble1(double altDouble1) throws java.sql.SQLException{
		if(isSame(altDouble1, getAltDouble1()))return;
		double oldValue = getAltDouble1();
		this.altDouble1=altDouble1;
		setProperty("ALT_DOUBLE1", altDouble1, oldValue);
	}
	@Column(name="ALT_VARCHAR1",nullable=true,length=1024)
	public String getAltVarchar1(){
		return altVarchar1;
	}
	public void setAltVarchar1(String altVarchar1) throws java.sql.SQLException{
		if(isSame(altVarchar1, getAltVarchar1()))return;
		String oldValue = getAltVarchar1();
		this.altVarchar1=altVarchar1;
		setProperty("ALT_VARCHAR1", altVarchar1, oldValue);
	}
	@Column(name="ALT_DATE1",nullable=true,length=7)
	public Calendar getAltDate1(){
		return altDate1;
	}
	public void setAltDate1(Calendar altDate1) throws java.sql.SQLException{
		if(isSame(altDate1, getAltDate1()))return;
		Calendar oldValue = getAltDate1();
		this.altDate1=altDate1;
		setProperty("ALT_DATE1", altDate1, oldValue);
	}
	public Collection<AssumptionLoc> getAssumptionLocs()throws java.sql.SQLException{
		if(isNewInstance() || assumptionLocs != null){
			if(assumptionLocs == null)
				assumptionLocs = new TreeSet<AssumptionLoc>();
			return assumptionLocs;
		}
		return AssumptionLoc.getNamedCollection("findByAssumption",getAssumptionId());
	}
	public void addAssumptionLoc(AssumptionLoc assumptionLoc)throws java.sql.SQLException{
		assumptionLoc.setAssumption((Assumption)this);
		if(isNewInstance() || assumptionLocs != null)
			getAssumptionLocs().add(assumptionLoc);
		else
			assumptionLoc.insert();
	}
	public void removeAssumptionLoc(AssumptionLoc assumptionLoc)throws java.sql.SQLException{
		if(isNewInstance() || assumptionLocs != null)
			getAssumptionLocs().remove(assumptionLoc);
		else
			assumptionLoc.delete();
	}
	public Assumption copy()throws java.sql.SQLException{
		Assumption cp = new Assumption((Assumption)this);
		copyChildrenTo(cp);
		return cp;
	}
	public void copyChildrenTo(AssumptionDAO cp)throws java.sql.SQLException{
		super.copyChildrenTo(cp);
		for(AssumptionLoc child:getAssumptionLocs())
			cp.addAssumptionLoc(child.copy());
	}
	public Vector<String> getDifference(AssumptionDAO o){
		Vector<String> diffs = super.getDifference(o);
		if(!isSame(getAssumptionId(),o.getAssumptionId())) diffs.add("ASSUMPTION_ID");
		if(!isSame(getName(),o.getName())) diffs.add("NAME");
		if(!isSame(getDescription(),o.getDescription())) diffs.add("DESCRIPTION");
		if(!isSame(getHelpText(),o.getHelpText())) diffs.add("HELP_TEXT");
		if(!isSame(getDefaultAssumption(),o.getDefaultAssumption())) diffs.add("DEFAULT_ASSUMPTION");
		if(!isSame(getAltDouble1(),o.getAltDouble1())) diffs.add("ALT_DOUBLE1");
		if(!isSame(getAltVarchar1(),o.getAltVarchar1())) diffs.add("ALT_VARCHAR1");
		if(!isSame(getAltDate1(),o.getAltDate1())) diffs.add("ALT_DATE1");
		return diffs;
	}
	public void insertChildren()throws java.sql.SQLException{
		if(assumptionLocs != null){
			for(AssumptionLoc assumptionLoc:getAssumptionLocs()){
				if(assumptionLoc.isNewInstance())
					assumptionLoc.setAssumption((Assumption)this);
				assumptionLoc.insert();
			}
			assumptionLocs = null;
		}
	}
}
