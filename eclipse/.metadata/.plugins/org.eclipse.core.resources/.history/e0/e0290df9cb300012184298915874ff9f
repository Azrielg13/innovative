package com.sce.esp.object.model;
import com.sce.esp.object.dao.DgDeratDAO;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;
@Entity
@Table(schema="MDI",name="MDI830_DG_DERAT")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM DgDerat o WHERE o.PLANYEAR=?1 AND o.SCE_PROJ_REV_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM DgDerat o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM DgDerat o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI830_DG_DERAT o WHERE o.PLANYEAR=? AND o.SCE_PROJ_REV_ID=?"),//AUTO-GENERATED
})
public class DgDerat extends DgDeratDAO{
	public DgDerat(){
	}
	public DgDerat(int planyear, int sceProjRevId){
		super(planyear, sceProjRevId);
	}
	public DgDerat(DgDerat orig){
		super(orig);
	}
}
