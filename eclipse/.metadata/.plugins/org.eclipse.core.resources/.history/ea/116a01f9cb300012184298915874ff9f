package com.sce.esp.object.model;
import java.io.Serializable;
import java.util.Calendar;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.SysDailyDAO;
import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.util.FormatText;
@Entity
@AutoInsertable
@Table(schema="MDI",name="MDI080_SYS_DAILY")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM SysDaily o WHERE o.PLANYEAR=?1 AND o.SYS_ID=?2 AND o.RCD_DATE=?3"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM SysDaily o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM SysDaily o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findBySys", query="SELECT o FROM SysDaily o WHERE o.PLANYEAR=?1 AND o.SYS_ID=?2 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI080_SYS_DAILY o WHERE o.PLANYEAR=? AND o.SYS_ID=? AND o.RCD_DATE=?"),//AUTO-GENERATED
	@NamedNativeQuery(name = "findByID_FETCH", query="SELECT o.* FROM MDI080_SYS_DAILY o WHERE o.PLANYEAR=? AND o.SYS_ID=? AND TO_CHAR(RCD_DATE,'YYYY-MM-DD')=?"),//AUTO-GENERATED
})
public class SysDaily extends SysDailyDAO implements Serializable{
	
	private static final long serialVersionUID = -1330884714149333701L;
	
	/**
	 * The Constructor.
	 * 
	 * @param sysId the sys id
	 * @param planyear the planyear
	 * @param rcdDate the rcd date
	 */
	public SysDaily(){
	}
	public SysDaily(int planyear, int sysId, Calendar rcdDate){
		super(planyear,sysId,(Calendar)rcdDate.clone());
	}
	
	public SysDaily(SysDaily sysDaily) {
		super(sysDaily);
	}

	/**
	 * To string.
	 * 
	 * @return the string
	 */
	public String toString() {
		return FormatText.USER_DATE.format(getRcdDate().getTime());
	}
}
