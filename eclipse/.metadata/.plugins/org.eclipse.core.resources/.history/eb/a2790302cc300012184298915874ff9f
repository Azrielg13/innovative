package com.sce.esp.object.dao;
/**Copy Right Frank todo */
/**Description of class, (we need to get this from somewhere, database? xml?)*/
import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.object.jpa.EntityManagerHelper;
import com.sce.esp.object.jpa.PrimaryKey;
import com.sce.esp.object.model.WorkGroup;
import com.sce.esp.object.model.WorkGroupDis;
import java.util.Calendar;
import java.util.Collection;
import java.util.TreeSet;
import java.util.Vector;
import javax.persistence.Cache;
import javax.persistence.Column;
import javax.persistence.EntityManager;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.TypedQuery;
public abstract class WorkGroupDAO extends DataAccessObject{
	public static enum KEY_PROPERTY{WORK_GROUP_ID};
	public static enum PROPERTY{WORK_GROUP_ID,NAME,DESCRIPTION,ALT_DOUBLE1,ALT_VARCHAR1,ALT_DATE1,INSERT_TS,INSERT_USER_ID,MODIFIED_TS,MODIFIED_USER_ID,DELETED_TS,DELETED_USER_ID};
	public final static int WORKGROUPID_LIMIT = 9;
	public final static int NAME_LIMIT = 64;
	public final static int DESCRIPTION_LIMIT = 256;
	public final static int ALTDOUBLE1_LIMIT = 24;
	public final static int ALTVARCHAR1_LIMIT = 256;
	public final static int ALTDATE1_LIMIT = 7;
	private int workGroupId;
	private String name;
	private String description;
	private double altDouble1;
	private String altVarchar1;
	private Calendar altDate1;
	private Collection<WorkGroupDis> workGroupDiss;
	public static WorkGroup getInstance(int workGroupId)throws java.sql.SQLException{
		return getInstance(workGroupId, true);
	}
	public static WorkGroup getInstance(int workGroupId, boolean fetch)throws java.sql.SQLException{
		if(isNull(workGroupId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(workGroupId);
		Cache cache = em.getEntityManagerFactory().getCache();
		WorkGroup o = null;
		if(cache != null && cache.contains(WorkGroup.class, pk))
			o = em.find(WorkGroup.class, pk);
		if(o==null && getAICache().contains(WorkGroup.class, pk))
			o = getAICache().find(WorkGroup.class, pk);
		if(o==null && fetch)
			o = em.find(WorkGroup.class, pk);
		if(o==null && fetch && WorkGroup.class.getAnnotation(AutoInsertable.class) != null){
			o = new WorkGroup(workGroupId);
			getAICache().cache(WorkGroup.class, o);
		}
		return o;
	}
	public static Collection<WorkGroup> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<WorkGroup> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<WorkGroup> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM WorkGroup o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<WorkGroup> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<WorkGroup> tq = em.createQuery(jpql,WorkGroup.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<WorkGroup> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<WorkGroup> tq = em.createNamedQuery(name,WorkGroup.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public WorkGroupDAO(){}
	public WorkGroupDAO(int workGroupId){
		this.workGroupId=workGroupId;
	}
	public WorkGroupDAO(WorkGroupDAO orig){
		super(orig);
		copyFrom(orig);
	}
	public void copyFrom(WorkGroupDAO orig){
		this.name=orig.getName();
		this.description=orig.getDescription();
		this.altDouble1=orig.getAltDouble1();
		this.altVarchar1=orig.getAltVarchar1();
		this.altDate1=orig.getAltDate1();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{workGroupId};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	@Id
	@SequenceGenerator(name="MDIS13_SEQ",sequenceName="MDIS13_SEQ")
	@Column(name="WORK_GROUP_ID",nullable=false,length=9)
	public int getWorkGroupId(){
		return workGroupId;
	}
	public void setWorkGroupId(int workGroupId) throws java.sql.SQLException{
		if(isSame(workGroupId, getWorkGroupId()))return;
		int oldValue = getWorkGroupId();
		this.workGroupId=workGroupId;
		setProperty("WORK_GROUP_ID", workGroupId, oldValue);
	}
	@Column(name="NAME",nullable=false,length=64)
	public String getName(){
		return name;
	}
	public void setName(String name) throws java.sql.SQLException{
		if(isSame(name, getName()))return;
		String oldValue = getName();
		this.name=name;
		setProperty("NAME", name, oldValue);
	}
	@Column(name="DESCRIPTION",nullable=true,length=256)
	public String getDescription(){
		return description;
	}
	public void setDescription(String description) throws java.sql.SQLException{
		if(isSame(description, getDescription()))return;
		String oldValue = getDescription();
		this.description=description;
		setProperty("DESCRIPTION", description, oldValue);
	}
	@Column(name="ALT_DOUBLE1",nullable=true,length=24)
	public double getAltDouble1(){
		return altDouble1;
	}
	public void setAltDouble1(double altDouble1) throws java.sql.SQLException{
		if(isSame(altDouble1, getAltDouble1()))return;
		double oldValue = getAltDouble1();
		this.altDouble1=altDouble1;
		setProperty("ALT_DOUBLE1", altDouble1, oldValue);
	}
	@Column(name="ALT_VARCHAR1",nullable=true,length=256)
	public String getAltVarchar1(){
		return altVarchar1;
	}
	public void setAltVarchar1(String altVarchar1) throws java.sql.SQLException{
		if(isSame(altVarchar1, getAltVarchar1()))return;
		String oldValue = getAltVarchar1();
		this.altVarchar1=altVarchar1;
		setProperty("ALT_VARCHAR1", altVarchar1, oldValue);
	}
	@Column(name="ALT_DATE1",nullable=true,length=7)
	public Calendar getAltDate1(){
		return altDate1;
	}
	public void setAltDate1(Calendar altDate1) throws java.sql.SQLException{
		if(isSame(altDate1, getAltDate1()))return;
		Calendar oldValue = getAltDate1();
		this.altDate1=altDate1;
		setProperty("ALT_DATE1", altDate1, oldValue);
	}
	public Collection<WorkGroupDis> getWorkGroupDiss()throws java.sql.SQLException{
		if(isNewInstance() || workGroupDiss != null){
			if(workGroupDiss == null)
				workGroupDiss = new TreeSet<WorkGroupDis>();
			return workGroupDiss;
		}
		return WorkGroupDis.getNamedCollection("findByWorkGroup",getWorkGroupId());
	}
	public void addWorkGroupDis(WorkGroupDis workGroupDis)throws java.sql.SQLException{
		workGroupDis.setWorkGroup((WorkGroup)this);
		if(isNewInstance() || workGroupDiss != null)
			getWorkGroupDiss().add(workGroupDis);
		else
			workGroupDis.insert();
	}
	public void removeWorkGroupDis(WorkGroupDis workGroupDis)throws java.sql.SQLException{
		if(isNewInstance() || workGroupDiss != null)
			getWorkGroupDiss().remove(workGroupDis);
		else
			workGroupDis.delete();
	}
	public WorkGroup copy()throws java.sql.SQLException{
		WorkGroup cp = new WorkGroup((WorkGroup)this);
		copyChildrenTo(cp);
		return cp;
	}
	public void copyChildrenTo(WorkGroupDAO cp)throws java.sql.SQLException{
		super.copyChildrenTo(cp);
		for(WorkGroupDis child:getWorkGroupDiss())
			cp.addWorkGroupDis(child.copy());
	}
	public Vector<String> getDifference(WorkGroupDAO o){
		Vector<String> diffs = super.getDifference(o);
		if(!isSame(getWorkGroupId(),o.getWorkGroupId())) diffs.add("WORK_GROUP_ID");
		if(!isSame(getName(),o.getName())) diffs.add("NAME");
		if(!isSame(getDescription(),o.getDescription())) diffs.add("DESCRIPTION");
		if(!isSame(getAltDouble1(),o.getAltDouble1())) diffs.add("ALT_DOUBLE1");
		if(!isSame(getAltVarchar1(),o.getAltVarchar1())) diffs.add("ALT_VARCHAR1");
		if(!isSame(getAltDate1(),o.getAltDate1())) diffs.add("ALT_DATE1");
		return diffs;
	}
	public void insertChildren()throws java.sql.SQLException{
		if(workGroupDiss != null){
			for(WorkGroupDis workGroupDis:getWorkGroupDiss()){
				if(workGroupDis.isNewInstance())
					workGroupDis.setWorkGroup((WorkGroup)this);
				workGroupDis.insert();
			}
			workGroupDiss = null;
		}
	}
}
