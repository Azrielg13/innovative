import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.object.jpa.EntityManagerHelper;
import com.sce.esp.object.jpa.PrimaryKey;
import com.sce.esp.object.model.DriverCriteria;
import java.util.Calendar;
import java.util.Collection;
import java.util.Vector;
import javax.persistence.Cache;
import javax.persistence.Column;
import javax.persistence.EntityManager;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.TypedQuery;
public abstract class DriverCriteriaDAO extends DataAccessObject{
	public static enum KEY_PROPERTY{DRIVER_CRITERIA_ID};
	public static enum PROPERTY{DRIVER_CRITERIA_ID,NAME,DESCRIPTION,HELP_TEXT,DEFAULT_CRITERIA,DEFAULT_VIOLATION,MAGNITUDE_UNITS,ALT_DOUBLE1,ALT_VARCHAR1,ALT_DATE1,INSERT_TS,INSERT_USER_ID,MODIFIED_TS,MODIFIED_USER_ID,DELETED_TS,DELETED_USER_ID};
	public final static int DRIVERCRITERIAID_LIMIT = 9;
	public final static int NAME_LIMIT = 64;
	public final static int DESCRIPTION_LIMIT = 1024;
	public final static int HELPTEXT_LIMIT = 1024;
	public final static int DEFAULTCRITERIA_LIMIT = 1024;
	public final static int DEFAULTVIOLATION_LIMIT = 1024;
	public final static int MAGNITUDEUNITS_LIMIT = 256;
	public final static int ALTDOUBLE1_LIMIT = 24;
	public final static int ALTVARCHAR1_LIMIT = 1024;
	public final static int ALTDATE1_LIMIT = 7;
	private int driverCriteriaId;
	private String name;
	private String description;
	private String helpText;
	private String defaultCriteria;
	private String defaultViolation;
	private String magnitudeUnits;
	private double altDouble1;
	private String altVarchar1;
	private Calendar altDate1;
	public static DriverCriteria getInstance(int driverCriteriaId)throws java.sql.SQLException{
		return getInstance(driverCriteriaId, true);
	}
	public static DriverCriteria getInstance(int driverCriteriaId, boolean fetch)throws java.sql.SQLException{
		if(isNull(driverCriteriaId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(driverCriteriaId);
		Cache cache = em.getEntityManagerFactory().getCache();
		DriverCriteria o = null;
		if(cache != null && cache.contains(DriverCriteria.class, pk))
			o = em.find(DriverCriteria.class, pk);
		if(o==null && getAICache().contains(DriverCriteria.class, pk))
			o = getAICache().find(DriverCriteria.class, pk);
		if(o==null && fetch)
			o = em.find(DriverCriteria.class, pk);
		if(o==null && fetch && DriverCriteria.class.getAnnotation(AutoInsertable.class) != null){
			o = new DriverCriteria(driverCriteriaId);
			getAICache().cache(DriverCriteria.class, o);
		}
		return o;
	}
	public static Collection<DriverCriteria> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<DriverCriteria> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<DriverCriteria> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM DriverCriteria o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<DriverCriteria> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<DriverCriteria> tq = em.createQuery(jpql,DriverCriteria.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<DriverCriteria> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<DriverCriteria> tq = em.createNamedQuery(name,DriverCriteria.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public DriverCriteriaDAO(){}
	public DriverCriteriaDAO(int driverCriteriaId){
		this.driverCriteriaId=driverCriteriaId;
	}
	public DriverCriteriaDAO(DriverCriteriaDAO orig){
		super(orig);
		copyFrom(orig);
	}
	public void copyFrom(DriverCriteriaDAO orig){
		this.name=orig.getName();
		this.description=orig.getDescription();
		this.helpText=orig.getHelpText();
		this.defaultCriteria=orig.getDefaultCriteria();
		this.defaultViolation=orig.getDefaultViolation();
		this.magnitudeUnits=orig.getMagnitudeUnits();
		this.altDouble1=orig.getAltDouble1();
		this.altVarchar1=orig.getAltVarchar1();
		this.altDate1=orig.getAltDate1();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{driverCriteriaId};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	@Id
	@SequenceGenerator(name="MDIT35_SEQ",sequenceName="MDIT35_SEQ")
	@Column(name="DRIVER_CRITERIA_ID",nullable=false,length=9)
	public int getDriverCriteriaId(){
		return driverCriteriaId;
	}
	public void setDriverCriteriaId(int driverCriteriaId) throws java.sql.SQLException{
		if(isSame(driverCriteriaId, getDriverCriteriaId()))return;
		int oldValue = getDriverCriteriaId();
		this.driverCriteriaId=driverCriteriaId;
		setProperty("DRIVER_CRITERIA_ID", driverCriteriaId, oldValue);
	}
	@Column(name="NAME",nullable=false,length=64)
	public String getName(){
		return name;
	}
	public void setName(String name) throws java.sql.SQLException{
		if(isSame(name, getName()))return;
		String oldValue = getName();
		this.name=name;
		setProperty("NAME", name, oldValue);
	}
	@Column(name="DESCRIPTION",nullable=true,length=1024)
	public String getDescription(){
		return description;
	}
	public void setDescription(String description) throws java.sql.SQLException{
		if(isSame(description, getDescription()))return;
		String oldValue = getDescription();
		this.description=description;
		setProperty("DESCRIPTION", description, oldValue);
	}
	@Column(name="HELP_TEXT",nullable=true,length=1024)
	public String getHelpText(){
		return helpText;
	}
	public void setHelpText(String helpText) throws java.sql.SQLException{
		if(isSame(helpText, getHelpText()))return;
		String oldValue = getHelpText();
		this.helpText=helpText;
		setProperty("HELP_TEXT", helpText, oldValue);
	}
	@Column(name="DEFAULT_CRITERIA",nullable=true,length=1024)
	public String getDefaultCriteria(){
		return defaultCriteria;
	}
	public void setDefaultCriteria(String defaultCriteria) throws java.sql.SQLException{
		if(isSame(defaultCriteria, getDefaultCriteria()))return;
		String oldValue = getDefaultCriteria();
		this.defaultCriteria=defaultCriteria;
		setProperty("DEFAULT_CRITERIA", defaultCriteria, oldValue);
	}
	@Column(name="DEFAULT_VIOLATION",nullable=true,length=1024)
	public String getDefaultViolation(){
		return defaultViolation;
	}
	public void setDefaultViolation(String defaultViolation) throws java.sql.SQLException{
		if(isSame(defaultViolation, getDefaultViolation()))return;
		String oldValue = getDefaultViolation();
		this.defaultViolation=defaultViolation;
		setProperty("DEFAULT_VIOLATION", defaultViolation, oldValue);
	}
	@Column(name="MAGNITUDE_UNITS",nullable=true,length=256)
	public String getMagnitudeUnits(){
		return magnitudeUnits;
	}
	public void setMagnitudeUnits(String magnitudeUnits) throws java.sql.SQLException{
		if(isSame(magnitudeUnits, getMagnitudeUnits()))return;
		String oldValue = getMagnitudeUnits();
		this.magnitudeUnits=magnitudeUnits;
		setProperty("MAGNITUDE_UNITS", magnitudeUnits, oldValue);
	}
	@Column(name="ALT_DOUBLE1",nullable=true,length=24)
	public double getAltDouble1(){
		return altDouble1;
	}
	public void setAltDouble1(double altDouble1) throws java.sql.SQLException{
		if(isSame(altDouble1, getAltDouble1()))return;
		double oldValue = getAltDouble1();
		this.altDouble1=altDouble1;
		setProperty("ALT_DOUBLE1", altDouble1, oldValue);
	}
	@Column(name="ALT_VARCHAR1",nullable=true,length=1024)
	public String getAltVarchar1(){
		return altVarchar1;
	}
	public void setAltVarchar1(String altVarchar1) throws java.sql.SQLException{
		if(isSame(altVarchar1, getAltVarchar1()))return;
		String oldValue = getAltVarchar1();
		this.altVarchar1=altVarchar1;
		setProperty("ALT_VARCHAR1", altVarchar1, oldValue);
	}
	@Column(name="ALT_DATE1",nullable=true,length=7)
	public Calendar getAltDate1(){
		return altDate1;
	}
	public void setAltDate1(Calendar altDate1) throws java.sql.SQLException{
		if(isSame(altDate1, getAltDate1()))return;
		Calendar oldValue = getAltDate1();
		this.altDate1=altDate1;
		setProperty("ALT_DATE1", altDate1, oldValue);
	}
	public DriverCriteria copy()throws java.sql.SQLException{
		DriverCriteria cp = new DriverCriteria((DriverCriteria)this);
		copyChildrenTo(cp);
		return cp;
	}
	public void copyChildrenTo(DriverCriteriaDAO cp)throws java.sql.SQLException{
		super.copyChildrenTo(cp);
	}
	public Vector<String> getDifference(DriverCriteriaDAO o){
		Vector<String> diffs = super.getDifference(o);
		if(!isSame(getDriverCriteriaId(),o.getDriverCriteriaId())) diffs.add("DRIVER_CRITERIA_ID");
		if(!isSame(getName(),o.getName())) diffs.add("NAME");
		if(!isSame(getDescription(),o.getDescription())) diffs.add("DESCRIPTION");
		if(!isSame(getHelpText(),o.getHelpText())) diffs.add("HELP_TEXT");
		if(!isSame(getDefaultCriteria(),o.getDefaultCriteria())) diffs.add("DEFAULT_CRITERIA");
		if(!isSame(getDefaultViolation(),o.getDefaultViolation())) diffs.add("DEFAULT_VIOLATION");
		if(!isSame(getMagnitudeUnits(),o.getMagnitudeUnits())) diffs.add("MAGNITUDE_UNITS");
		if(!isSame(getAltDouble1(),o.getAltDouble1())) diffs.add("ALT_DOUBLE1");
		if(!isSame(getAltVarchar1(),o.getAltVarchar1())) diffs.add("ALT_VARCHAR1");
		if(!isSame(getAltDate1(),o.getAltDate1())) diffs.add("ALT_DATE1");
		return diffs;
	}
	public void insertChildren()throws java.sql.SQLException{
	}
}
