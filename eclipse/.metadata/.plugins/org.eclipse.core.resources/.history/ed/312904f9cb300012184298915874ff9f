package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.ContDAO;
@Entity
@Table(schema="MDI",name="MDI275_CONT")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM Cont o WHERE o.PLANYEAR=?1 AND o.CONT_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM Cont o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM Cont o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByDepartment", query="SELECT o FROM Cont o WHERE o.PLANYEAR=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI275_CONT o WHERE o.PLANYEAR=? AND o.CONT_ID=?"),//AUTO-GENERATED
})
public class Cont extends ContDAO implements Serializable{
	
	/**
	 * 
	 */
	private static final long serialVersionUID = -6555603144116245653L;

	/**
	 * The Constructor.
	 * 
	 * @param planyear the planyear
	 */
	public Cont(){
	}
	public Cont(int planyear) {
		super(planyear,0);
	}
	
	/**
	 * The Constructor.
	 * 
	 * @param planyear the planyear
	 * @param contId the cont id
	 */
	public Cont(int planyear, int contId){
		super(planyear,contId);
	}
	
	public Cont(Cont cont) {
		super(cont);
	}

	/**
	 * To string.
	 * 
	 * @return the string
	 */
	public String toString() {
		String str = isContSub()?"Sub":"Ckt";
		return str+" - "+getTitle();
	}
}

