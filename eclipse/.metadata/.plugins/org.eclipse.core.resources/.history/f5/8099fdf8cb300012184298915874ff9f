package com.sce.esp.object.model;
import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.SceProjViolationDAO;
@Entity
@Table(schema="MDI",name="MDIT55_SCE_PROJ_VIOLATION")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM SceProjViolation o WHERE o.SCE_PROJ_VIOLATION_ID=?1"),
	@NamedQuery(name = "findAll", query="SELECT o FROM SceProjViolation o"),
	@NamedQuery(name = "findAllActive", query="SELECT o FROM SceProjViolation o WHERE o.DELETED_TS IS NULL"),
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIT55_SCE_PROJ_VIOLATION o WHERE o.SCE_PROJ_VIOLATION_ID=?"),
})
public class SceProjViolation extends SceProjViolationDAO implements Serializable{

	private static final long serialVersionUID = 7352995288183185549L;
	public SceProjViolation(){
	}
	public SceProjViolation(int sceProjViolationId){
		super(sceProjViolationId);
	}
	public SceProjViolation(SceProjViolation orig){
		super(orig);
	}
}
