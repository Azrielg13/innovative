package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.ProgramSapWbsDAO;
@Entity
@Table(schema="MDI",name="MDIS32_PROGRAM_SAP_WBS")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM ProgramSapWbs o WHERE o.PROGRAM_ID=?1 AND o.SAP_WBS_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM ProgramSapWbs o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM ProgramSapWbs o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByProgram", query="SELECT o FROM ProgramSapWbs o WHERE o.PROGRAM_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findBySapWbs", query="SELECT o FROM ProgramSapWbs o WHERE o.SAP_WBS_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS32_PROGRAM_SAP_WBS o WHERE o.PROGRAM_ID=? AND o.SAP_WBS_ID=?"),//AUTO-GENERATED
})
public class ProgramSapWbs extends ProgramSapWbsDAO implements Serializable{

	private static final long serialVersionUID = -964487796785733778L;

	public ProgramSapWbs(){
	}
	public ProgramSapWbs(int programId, int sapWbsId) {
		super(programId, sapWbsId);
	}

	public ProgramSapWbs(ProgramSapWbs programSapWbs) {
		super(programSapWbs);
	}	
}
