package com.sce.esp.object.model;
import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.ElemEscRateDAO;
@Entity
@Table(schema="MDI",name="MDIT19_ELEM_ESC_RATE")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM ElemEscRate o WHERE o.ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM ElemEscRate o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM ElemEscRate o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findBySceElem", query="SELECT o FROM ElemEscRate o WHERE o.SCE_ELEM_ID=?1 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIT19_ELEM_ESC_RATE o WHERE o.ID=?"),//AUTO-GENERATED
})
public class ElemEscRate extends ElemEscRateDAO{
	public ElemEscRate(){
	}
	public ElemEscRate(int id){
		super(id);
	}
	public ElemEscRate(ElemEscRate orig){
		super(orig);
	}
}
