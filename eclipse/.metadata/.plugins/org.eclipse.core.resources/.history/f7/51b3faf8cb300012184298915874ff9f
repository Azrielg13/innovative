package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.SubQbrDAO;
@Entity
@Table(schema="MDI",name="MDIS98_SUB_QBR")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM SubQbr o WHERE o.SUB_QBR_ID=?1"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM SubQbr o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM SubQbr o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDIS98_SUB_QBR o WHERE o.SUB_QBR_ID=?"),//AUTO-GENERATED
})
public class SubQbr extends SubQbrDAO implements Serializable{

	private static final long serialVersionUID = -1682249139602129542L;

	public SubQbr(){
	}
	public SubQbr(int subQbrId) {
		super(subQbrId);
	}
	
	public SubQbr(SubQbr subQbr) {
		super(subQbr);
	}
	
	public String toString() {
		return getName();
	}
}
