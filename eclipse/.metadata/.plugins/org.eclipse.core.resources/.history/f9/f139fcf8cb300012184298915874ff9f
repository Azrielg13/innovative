package com.sce.esp.object.model;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.NamedNativeQueries;
import javax.persistence.NamedNativeQuery;
import javax.persistence.NamedQueries;
import javax.persistence.NamedQuery;
import javax.persistence.Table;

import com.sce.esp.object.dao.LgProjGisDAO;
@Entity
@Table(schema="MDI",name="MDI905_LG_PROJ_GIS")
@NamedQueries({
	@NamedQuery(name = "findByID", query="SELECT o FROM LgProjGis o WHERE o.PLANYEAR=?1 AND o.GIS_ID=?2"),//AUTO-GENERATED
	@NamedQuery(name = "findAll", query="SELECT o FROM LgProjGis o"),//AUTO-GENERATED
	@NamedQuery(name = "findAllActive", query="SELECT o FROM LgProjGis o WHERE o.DELETED_TS IS NULL"),//AUTO-GENERATED
	@NamedQuery(name = "findByLgProj", query="SELECT o FROM LgProjGis o WHERE o.PLANYEAR=?1 AND o.LG_PROJ_ID=?2 AND o.DELETED_TS IS NULL"),//AUTO-GENERATED
})
@NamedNativeQueries({
	@NamedNativeQuery(name = "refresh", query="SELECT o.* FROM MDI905_LG_PROJ_GIS o WHERE o.PLANYEAR=? AND o.GIS_ID=?"),//AUTO-GENERATED
})
public class LgProjGis extends LgProjGisDAO implements Serializable{
	private static final long serialVersionUID = -4701371059527019164L;
	public LgProjGis(){
	}
	public LgProjGis(int planyear, int gisId){
		super(planyear,gisId);
	}
	public LgProjGis(int planyear) {
		super(planyear,0);
	}
	
	public LgProjGis(LgProjGis lgProjGis) {
		super(lgProjGis);
	}
	/**
	 * To string.
	 * 
	 * @return the string
	 */
	public String toString() {
		return ""+getGisId();
	}
}

