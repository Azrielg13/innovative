package com.sce.esp.object.dao;
/**Copy Right Frank todo */
/**Description of class, (we need to get this from somewhere, database? xml?)*/
import com.sce.esp.object.jpa.AutoInsertable;
import com.sce.esp.object.jpa.EntityManagerHelper;
import com.sce.esp.object.jpa.PrimaryKey;
import com.sce.esp.object.model.Mitigation;
import java.util.Collection;
import java.util.Vector;
import javax.persistence.Cache;
import javax.persistence.Column;
import javax.persistence.EntityManager;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.TypedQuery;
public abstract class MitigationDAO extends DataAccessObject{
	public static enum KEY_PROPERTY{MITIGATION_ID};
	public static enum PROPERTY{MITIGATION_ID,TYPE,DESCRIPTION,HELP_TEXT,ALT_DOUBLE1,ALT_VARCHAR1,INSERT_TS,INSERT_USER_ID,MODIFIED_TS,MODIFIED_USER_ID,DELETED_TS,DELETED_USER_ID};
	public final static int MITIGATIONID_LIMIT = 9;
	public final static int TYPE_LIMIT = 64;
	public final static int DESCRIPTION_LIMIT = 1024;
	public final static int HELPTEXT_LIMIT = 1024;
	public final static int ALTDOUBLE1_LIMIT = 24;
	public final static int ALTVARCHAR1_LIMIT = 1024;
	private int mitigationId;
	private String type;
	private String description;
	private String helpText;
	private double altDouble1;
	private String altVarchar1;
	public static Mitigation getInstance(int mitigationId)throws java.sql.SQLException{
		return getInstance(mitigationId, true);
	}
	public static Mitigation getInstance(int mitigationId, boolean fetch)throws java.sql.SQLException{
		if(isNull(mitigationId))return null;
		EntityManager em = EntityManagerHelper.getEntityManager();
		PrimaryKey pk = new PrimaryKey(mitigationId);
		Cache cache = em.getEntityManagerFactory().getCache();
		Mitigation o = null;
		if(cache != null && cache.contains(Mitigation.class, pk))
			o = em.find(Mitigation.class, pk);
		if(o==null && getAICache().contains(Mitigation.class, pk))
			o = getAICache().find(Mitigation.class, pk);
		if(o==null && fetch)
			o = em.find(Mitigation.class, pk);
		if(o==null && fetch && Mitigation.class.getAnnotation(AutoInsertable.class) != null){
			o = new Mitigation(mitigationId);
			getAICache().cache(Mitigation.class, o);
		}
		return o;
	}
	public static Collection<Mitigation> getAll()throws java.sql.SQLException{
		return getNamedCollection("findAll");
	}
	public static Collection<Mitigation> getAllActive()throws java.sql.SQLException{
		return getNamedCollection("findAllActive");
	}
	public static Collection<Mitigation> getCollection(String[] props, Object... values)throws java.sql.SQLException{
		String qlString = "SELECT o FROM Mitigation o";
		if(props != null && props.length > 0){
			qlString += " WHERE";
			int p=0;
			for(String prop:props){
				if(p > 0)
					qlString +=" AND";
				if(values[p]==null)
					qlString += " o."+prop+" IS NULL";
				else
					qlString += " o."+prop+" = ?"+(p+1);
				p++;
			}
		}
		return getCollection(qlString,values);
	}
	public synchronized static Collection<Mitigation> getCollection(String jpql, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<Mitigation> tq = em.createQuery(jpql,Mitigation.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public synchronized static Collection<Mitigation> getNamedCollection(String name, Object... values)throws java.sql.SQLException{
		EntityManager em = EntityManagerHelper.getEntityManager();
		TypedQuery<Mitigation> tq = em.createNamedQuery(name,Mitigation.class);
		if(values != null && values.length > 0){
			int p=1;
			for(Object value:values)
				if(value != null)
					tq = tq.setParameter(p++, value);
		}
		return tq.getResultList();
	}
	public MitigationDAO(){}
	public MitigationDAO(int mitigationId){
		this.mitigationId=mitigationId;
	}
	public MitigationDAO(MitigationDAO orig){
		super(orig);
		copyFrom(orig);
	}
	public void copyFrom(MitigationDAO orig){
		this.type=orig.getType();
		this.description=orig.getDescription();
		this.helpText=orig.getHelpText();
		this.altDouble1=orig.getAltDouble1();
		this.altVarchar1=orig.getAltVarchar1();
	}
	public String getHashKey(){
		return getHashKey(getKeyValues());
	}
	public Object[] getKeyValues(){
		return new Object[]{mitigationId};
	}
	@Override
	public int hashCode(){
		return PrimaryKey.hashCode(getKeyValues());
	}
	@Id
	@SequenceGenerator(name="MDIV10_SEQ",sequenceName="MDIV10_SEQ")
	@Column(name="MITIGATION_ID",nullable=false,length=9)
	public int getMitigationId(){
		return mitigationId;
	}
	public void setMitigationId(int mitigationId) throws java.sql.SQLException{
		if(isSame(mitigationId, getMitigationId()))return;
		int oldValue = getMitigationId();
		this.mitigationId=mitigationId;
		setProperty("MITIGATION_ID", mitigationId, oldValue);
	}
	@Column(name="TYPE",nullable=false,length=64)
	public String getType(){
		return type;
	}
	public void setType(String type) throws java.sql.SQLException{
		if(isSame(type, getType()))return;
		String oldValue = getType();
		this.type=type;
		setProperty("TYPE", type, oldValue);
	}
	@Column(name="DESCRIPTION",nullable=true,length=1024)
	public String getDescription(){
		return description;
	}
	public void setDescription(String description) throws java.sql.SQLException{
		if(isSame(description, getDescription()))return;
		String oldValue = getDescription();
		this.description=description;
		setProperty("DESCRIPTION", description, oldValue);
	}
	@Column(name="HELP_TEXT",nullable=true,length=1024)
	public String getHelpText(){
		return helpText;
	}
	public void setHelpText(String helpText) throws java.sql.SQLException{
		if(isSame(helpText, getHelpText()))return;
		String oldValue = getHelpText();
		this.helpText=helpText;
		setProperty("HELP_TEXT", helpText, oldValue);
	}
	@Column(name="ALT_DOUBLE1",nullable=true,length=24)
	public double getAltDouble1(){
		return altDouble1;
	}
	public void setAltDouble1(double altDouble1) throws java.sql.SQLException{
		if(isSame(altDouble1, getAltDouble1()))return;
		double oldValue = getAltDouble1();
		this.altDouble1=altDouble1;
		setProperty("ALT_DOUBLE1", altDouble1, oldValue);
	}
	@Column(name="ALT_VARCHAR1",nullable=true,length=1024)
	public String getAltVarchar1(){
		return altVarchar1;
	}
	public void setAltVarchar1(String altVarchar1) throws java.sql.SQLException{
		if(isSame(altVarchar1, getAltVarchar1()))return;
		String oldValue = getAltVarchar1();
		this.altVarchar1=altVarchar1;
		setProperty("ALT_VARCHAR1", altVarchar1, oldValue);
	}
	public Mitigation copy()throws java.sql.SQLException{
		Mitigation cp = new Mitigation((Mitigation)this);
		copyChildrenTo(cp);
		return cp;
	}
	public void copyChildrenTo(MitigationDAO cp)throws java.sql.SQLException{
		super.copyChildrenTo(cp);
	}
	public Vector<String> getDifference(MitigationDAO o){
		Vector<String> diffs = super.getDifference(o);
		if(!isSame(getMitigationId(),o.getMitigationId())) diffs.add("MITIGATION_ID");
		if(!isSame(getType(),o.getType())) diffs.add("TYPE");
		if(!isSame(getDescription(),o.getDescription())) diffs.add("DESCRIPTION");
		if(!isSame(getHelpText(),o.getHelpText())) diffs.add("HELP_TEXT");
		if(!isSame(getAltDouble1(),o.getAltDouble1())) diffs.add("ALT_DOUBLE1");
		if(!isSame(getAltVarchar1(),o.getAltVarchar1())) diffs.add("ALT_VARCHAR1");
		return diffs;
	}
	public void insertChildren()throws java.sql.SQLException{
	}
}
